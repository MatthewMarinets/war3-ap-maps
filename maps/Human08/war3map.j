    return ( IsUnitAliveBJ(GetFilterUnit()) == true )
endfunction

function Trig_All_Units_Dead_Conditions takes nothing returns boolean
    if ( not ( CountUnitsInGroup(GetUnitsOfPlayerMatching(Player(1), Condition(function Trig_All_Units_Dead_Func008001001002))) == 0 ) ) then
        return false
    endif
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_All_Units_Dead_Actions takes nothing returns nothing
    set udg_GameOver=true
    call DisableTrigger(gg_trg_AllShipsDown)
    call DisableTrigger(gg_trg_MainTimerExpires)
    call QueuedTriggerClearBJ()
    call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_FAILED, "TRIGSTR_238")
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call TriggerExecute(gg_trg_Level_Defeat)
endfunction

//===========================================================================
function InitTrig_All_Units_Dead takes nothing returns nothing
    set gg_trg_All_Units_Dead=CreateTrigger()
    call TriggerRegisterPlayerUnitEventSimple(gg_trg_All_Units_Dead, Player(1), EVENT_PLAYER_UNIT_DEATH)
    call TriggerAddCondition(gg_trg_All_Units_Dead, Condition(function Trig_All_Units_Dead_Conditions))
    call TriggerAddAction(gg_trg_All_Units_Dead, function Trig_All_Units_Dead_Actions)
endfunction

