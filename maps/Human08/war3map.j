//===========================================================================
// Trigger: GauntletCinematic
//===========================================================================
function Trig_GauntletCinematic_Conditions takes nothing returns boolean
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_GauntletCinematic_Func018001 takes nothing returns boolean
    return ( udg_GauntletCinematicEscape == true )
endfunction

function Trig_GauntletCinematic_Func021001 takes nothing returns boolean
    return ( udg_GauntletCinematicEscape == true )
endfunction

function Trig_GauntletCinematic_Actions takes nothing returns nothing
    set udg_GauntletCinematicRunning=true
    call LeaderboardDisplayBJ(false, udg_ShipsRemaining)
    call TimerDialogDisplayBJ(false, udg_CountdownTimerWindow)
    call PauseTimerBJ(true, udg_PeasantsChoppingTimer)
    call StopSoundBJ(GetLastPlayedSound(), true)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call PlayThematicMusicBJ("Sound\\Music\\mp3Music\\Tension.mp3")
    call TriggerSleepAction(1.00)
    call EnableTrigger(gg_trg_GauntletCinematicEscape)
    call CinematicModeBJ(true, GetPlayersAll())
    call SetSkyModel("Environment\\Sky\\LordaeronWinterSky\\LordaeronWinterSky.mdl")
    call UseTimeOfDayBJ(false)
    call CameraSetupApplyForPlayer(true, gg_cam_GauntletInitial, Player(1), 0)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call PauseAllUnitsBJ(true)
    if ( Trig_GauntletCinematic_Func018001() ) then
        return
    else
        call DoNothing()
    endif
    call CameraSetupApplyForPlayer(true, gg_cam_GauntletFinal, Player(1), 6.00)
    call TriggerSleepAction(6.00)
    if ( Trig_GauntletCinematic_Func021001() ) then
        return
    else
        call DoNothing()
    endif
    call DisableTrigger(gg_trg_GauntletCinematicEscape)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call CameraSetupApplyForPlayer(true, gg_cam_GauntletInitial, Player(1), 0)
    call PauseAllUnitsBJ(false)
    call TriggerExecute(gg_trg_PauseBoats)
    // PUT PEASANTS BACK TO MINING
    call TriggerExecute(gg_trg_PeasantsMine)
    call TriggerSleepAction(1.00)
    call TriggerExecute(gg_trg_GauntletCinematicVisibilities)
    call ResetToGameCameraForPlayer(Player(1), 0)
    call CinematicModeBJ(false, GetPlayersAll())
    call LeaderboardDisplayBJ(true, udg_ShipsRemaining)
    call TimerDialogDisplayBJ(true, udg_CountdownTimerWindow)
    call PauseTimerBJ(false, udg_PeasantsChoppingTimer)
    call SetSkyModel(null)
    call UseTimeOfDayBJ(true)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call QueuedTriggerRemoveBJ(GetTriggeringTrigger())
endfunction

