function InitCustomTeams takes nothing returns nothing
    // Force: TRIGSTR_235
    call SetPlayerTeam(Player(1), 0)
    call SetPlayerTeam(Player(8), 0)
    call SetPlayerTeam(Player(9), 0)

    //   Allied
    call SetPlayerAlliance(Player(1), Player(8), ALLIANCE_PASSIVE, true)
    call SetPlayerAlliance(Player(1), Player(9), ALLIANCE_PASSIVE, true)
    call SetPlayerAlliance(Player(8), Player(1), ALLIANCE_PASSIVE, true)
    call SetPlayerAlliance(Player(8), Player(9), ALLIANCE_PASSIVE, true)
    call SetPlayerAlliance(Player(9), Player(1), ALLIANCE_PASSIVE, true)
    call SetPlayerAlliance(Player(9), Player(8), ALLIANCE_PASSIVE, true)

    //   Shared Vision
    call SetPlayerAlliance(Player(1), Player(8), ALLIANCE_SHARED_VISION, true)
    call SetPlayerAlliance(Player(1), Player(9), ALLIANCE_SHARED_VISION, true)
    call SetPlayerAlliance(Player(8), Player(1), ALLIANCE_SHARED_VISION, true)
    call SetPlayerAlliance(Player(8), Player(9), ALLIANCE_SHARED_VISION, true)
    call SetPlayerAlliance(Player(9), Player(1), ALLIANCE_SHARED_VISION, true)
    call SetPlayerAlliance(Player(9), Player(8), ALLIANCE_SHARED_VISION, true)

    // Force: TRIGSTR_236
    call SetPlayerTeam(Player(6), 1)

    // Force: TRIGSTR_059
    call SetPlayerTeam(Player(4), 2)

endfunction

