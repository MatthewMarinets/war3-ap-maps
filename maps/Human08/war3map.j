    else
        call DoNothing()
    endif
    call CreateNUnitsAtLoc(1, 'hfoo', Player(9), GetRectCenter(gg_rct_IntroZeppelinTarget), 180.00)
    call PlaySoundBJ(gg_snd_Loading)
    set udg_EmissaryGuard00=GetLastCreatedUnit()
