function Trig_Serpents_Cleanup_Actions takes nothing returns nothing
    call ConditionalTriggerExecute( gg_trg_Leaderboard_Show )
    // Remove Units
    call RemoveUnit( GroupPickRandomUnit(GetUnitsInRectAll(gg_rct_Serpents_ElvestoNaga1)) )
    call RemoveUnit( GroupPickRandomUnit(GetUnitsInRectAll(gg_rct_Serpents_ElvestoNaga2)) )
