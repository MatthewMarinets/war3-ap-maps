//===========================================================================
// Trigger: Initialization
//===========================================================================
function Trig_Initialization_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_XN )
    call SetAllyColorFilterState( 0 )
    call ConditionalTriggerExecute( gg_trg_Init_01_Players )
    call ConditionalTriggerExecute( gg_trg_Init_02a_Units )
    call ConditionalTriggerExecute( gg_trg_Init_02b_Enemy_Units )
    call ConditionalTriggerExecute( gg_trg_Init_03_Music )
    call ConditionalTriggerExecute( gg_trg_Init_04_Sound )
    call ConditionalTriggerExecute( gg_trg_Init_05a_Environment )
    call ConditionalTriggerExecute( gg_trg_Init_05b_Points )
    call ConditionalTriggerExecute( gg_trg_Init_06_Quests )
    call ConditionalTriggerExecute( gg_trg_Init_07a_Hard )
    call ConditionalTriggerExecute( gg_trg_Init_07b_Normal )
    call ConditionalTriggerExecute( gg_trg_Init_07c_Easy )
    // Start Intro Cinematic
    call QueuedTriggerAddBJ( gg_trg_Intro_Cinematic_Q, true )
endfunction

//===========================================================================
function InitTrig_Initialization takes nothing returns nothing
    set gg_trg_Initialization = CreateTrigger(  )
    call TriggerAddAction( gg_trg_Initialization, function Trig_Initialization_Actions )
endfunction

//===========================================================================
// Trigger: Init 01 Players
//===========================================================================
function Trig_Init_01_Players_Actions takes nothing returns nothing
    // Alliance Settings
    call SetPlayerAllianceStateBJ( udg_AP1_Player, Player(PLAYER_NEUTRAL_PASSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_AP1_Player, udg_P2_Reinforcements, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P2_Reinforcements, udg_AP1_Player, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P2_Reinforcements, udg_P4_Illidan, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P2_Reinforcements, udg_P7_Guardians, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P2_Reinforcements, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P4_Illidan, udg_P2_Reinforcements, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P4_Illidan, udg_P7_Guardians, bj_ALLIANCE_UNALLIED )
    call SetPlayerAllianceStateBJ( udg_P4_Illidan, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P7_Guardians, udg_P2_Reinforcements, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P7_Guardians, udg_P4_Illidan, bj_ALLIANCE_UNALLIED )
    call SetPlayerAllianceStateBJ( udg_P7_Guardians, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P2_Reinforcements, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P4_Illidan, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P7_Guardians, bj_ALLIANCE_NEUTRAL )
    // Initial Resources
    call SetPlayerStateBJ( udg_AP1_Player, PLAYER_STATE_RESOURCE_GOLD, 0 )
    call SetPlayerStateBJ( udg_AP1_Player, PLAYER_STATE_RESOURCE_LUMBER, 0 )
    // Player Colors
    call SetPlayerColorBJ( udg_AP1_Player, PLAYER_COLOR_BLUE, true )
    call SetPlayerColorBJ( udg_P2_Reinforcements, PLAYER_COLOR_LIGHT_BLUE, true )
    call SetPlayerColorBJ( udg_P5_Cinaga, PLAYER_COLOR_RED, true )
    call SetPlayerColorBJ( udg_P7_Guardians, GetPlayerColor(Player(PLAYER_NEUTRAL_AGGRESSIVE)), true )
    // Hide Excess Players
    call SetPlayerOnScoreScreenBJ( false, udg_P2_Reinforcements )
    call SetPlayerOnScoreScreenBJ( false, udg_P5_Cinaga )
    call SetPlayerOnScoreScreenBJ( false, udg_P7_Guardians )
endfunction

