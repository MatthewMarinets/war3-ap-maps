    call RemoveUnit( udg_Guldan )
    call RemoveUnit( udg_NaishaCin )
    // Unhide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Glyph3_Cleanup_Func017002 )
