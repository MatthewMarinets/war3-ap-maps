function Trig_Victory_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_Escape_Completed )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    call TimerDialogDisplayForPlayerBJ( false, udg_EscapeTimerWindow, udg_AP1_Player )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Prep )
    call ConditionalTriggerExecute( gg_trg_Victory_Setup )
    call TriggerSleepAction( 0.50 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory1A, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory1B, udg_AP1_Player, 6.00 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call VolumeGroupResetBJ(  )
    call SetUnitLifePercentBJ( udg_Maiev, 100 )
    call SetUnitInvulnerable( udg_Maiev, false )
    call IssuePointOrderLocBJ( udg_Maiev, "move", GetRectCenter(gg_rct_Victory_Maiev_Move_2) )
    call PauseUnitBJ( false, udg_VictoryDemon )
    call IssuePointOrderLocBJ( udg_VictoryDemon, "move", GetRectCenter(gg_rct_Victory_Demon_MoveTarg) )
    call PauseUnitBJ( false, udg_VictoryHoundA )
    call PauseUnitBJ( false, udg_VictoryHoundB )
    call PauseUnitBJ( false, udg_VictoryHoundC )
    call PauseUnitBJ( false, udg_VictoryHoundD )
    call IssueTargetOrderBJ( udg_VictoryHoundA, "attack", udg_Maiev )
    call IssueTargetOrderBJ( udg_VictoryHoundB, "attack", udg_Maiev )
    call IssueTargetOrderBJ( udg_VictoryHoundC, "attack", udg_Maiev )
    call IssueTargetOrderBJ( udg_VictoryHoundD, "attack", udg_Maiev )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Victory_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TriggerSleepAction( 0.15 )
    if ( Trig_Victory_Cinematic_Q_Func039001() ) then
        return
    else
        call DoNothing(  )
    endif
    call PauseUnitBJ( true, udg_VictoryDemon )
    call SetUnitAnimation( udg_VictoryDemon, "attack slam" )
    call UnitResetCooldown( udg_Maiev )
    call TriggerSleepAction( 0.20 )
    if ( Trig_Victory_Cinematic_Q_Func044001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetUnitManaPercentBJ( udg_Maiev, 100 )
    call IssuePointOrderLocBJ( udg_Maiev, "blink", GetRectCenter(gg_rct_Victory_Maiev_Move_2) )
    call TriggerSleepAction( 0.50 )
    if ( Trig_Victory_Cinematic_Q_Func048001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_Maiev, "move", GetRectCenter(gg_rct_Victory_Maiev_Move_3) )
    call TriggerSleepAction( 0.20 )
    if ( Trig_Victory_Cinematic_Q_Func051001() ) then
        return
    else
        call DoNothing(  )
    endif
    call PauseUnitBJ( false, udg_VictoryDemon )
    call IssueTargetOrderBJ( udg_VictoryDemon, "attack", udg_Maiev )
    call TriggerSleepAction( 2 )
    if ( Trig_Victory_Cinematic_Q_Func055001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Victory_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call SetSpeechVolumeGroupsBJ(  )
    call StopSoundBJ( gg_snd_EarthquakeLoop1, true )
    call StopSoundBJ( gg_snd_Earthquake, true )
    call TriggerSleepAction( 2.00 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Run )
endfunction

