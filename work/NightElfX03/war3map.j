function InitTrig_Init_01_Players takes nothing returns nothing
    set gg_trg_Init_01_Players = CreateTrigger(  )
    call TriggerAddAction( gg_trg_Init_01_Players, function Trig_Init_01_Players_Actions )
endfunction

//===========================================================================
// Trigger: Init 02a Units
//===========================================================================
function Trig_Init_02a_Units_Func019002 takes nothing returns nothing
    call ShowUnitHide( GetEnumUnit() )
endfunction

function Trig_Init_02a_Units_Actions takes nothing returns nothing
    // Heroes
    call ConditionalTriggerExecute( gg_trg_Load_Heroes )
    call SetPlayerHandicapXPBJ( udg_AP1_Player, 25.00 )
    call SetPlayerHandicapXPBJ( udg_P4_Illidan, 0.00 )
    call SetPlayerHandicapXPBJ( udg_P7_Guardians, 0.00 )
    // StartingUnits
    set udg_IntroArcherA = gg_unit_earc_0005
    set udg_IntroArcherB = gg_unit_earc_0009
    set udg_IntroArcherC = gg_unit_earc_0006
    set udg_Naisha = gg_unit_ensh_0000
    set udg_IntroHuntressB = gg_unit_esen_0008
    set udg_IntroStartingUnits = GetUnitsInRectOfPlayer(gg_rct_Intro_Starting_Units, udg_AP1_Player)
    call GroupAddUnitSimple( udg_Maiev, udg_IntroStartingUnits )
    // Rescue Groups
    set udg_R1 = GetUnitsInRectOfPlayer(gg_rct_R1_Units, udg_P2_Reinforcements)
    set udg_R2 = GetUnitsInRectOfPlayer(gg_rct_R2_Units, udg_P2_Reinforcements)
    set udg_R3 = GetUnitsInRectOfPlayer(gg_rct_R3_Units, udg_P2_Reinforcements)
    // Cinematic Units
    call ForGroupBJ( GetUnitsOfPlayerAndTypeId(Player(PLAYER_NEUTRAL_PASSIVE), 'o000'), function Trig_Init_02a_Units_Func019002 )
endfunction

