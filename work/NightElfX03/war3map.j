    return ( udg_InCombat == false )
endfunction

function Trig_Hint_Blink_Water_Event_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    // Give the user a chance to figure it out
    loop
        exitwhen ( Trig_Hint_Blink_Water_Event_Func003001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 0.50))
    endloop
    call PolledWait( 5.00 )
    // Give the user a hint
    call QueuedTriggerAddBJ( gg_trg_Hint_Blink_Water_Q, true )
endfunction

