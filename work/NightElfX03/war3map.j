    call RemoveUnit( GroupPickRandomUnit(GetUnitsInRectAll(gg_rct_Serpents_ElvestoNaga6)) )
    // Restore Abilities
    call SetPlayerAbilityAvailableBJ( true, 'Ashm', udg_AP1_Player )
    // Unhide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Serpents_Cleanup_Func012002 )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Serpents_Cleanup_Func014002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    call ConditionalTriggerExecute( gg_trg_Post_Cinematic_Pause )
    // Pause Time Of Day
    // Re-enable XP Gain
    if ( Trig_Serpents_Cleanup_Func020001() ) then
        call SuspendHeroXPBJ( true, udg_Maiev )
    else
        call DoNothing(  )
    endif
    // Return Heroes
    call SetUnitPositionLoc( udg_Maiev, udg_MaievReturnSpot )
    call SetUnitPositionLocFacingLocBJ( udg_Vashj, GetRectCenter(gg_rct_Lady_Vashj), GetRectCenter(gg_rct_Eye_Maiev_Start) )
    // Restore Selection
    call SelectGroupBJ( udg_SelectedUnits )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Camera
    call CameraClearNoiseForPlayer( udg_AP1_Player )
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP1_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP1_Player, udg_CameraReturnPoint, 0 )
endfunction

