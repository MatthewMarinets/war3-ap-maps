function Trig_Init_05a_Environment_Actions takes nothing returns nothing
    // Initial Time Of Day
    call SetTimeOfDay( 12.00 )
    call SetTimeOfDay( 23.00 )
    call UseTimeOfDayBJ( false )
    // Weather Effects
    call AddWeatherEffectSaveLast( gg_rct_Weather01, 'FDwl' )
    set udg_Weather01Fog = GetLastCreatedWeatherEffect()
    call EnableWeatherEffect( udg_Weather01Fog, true )
    call AddWeatherEffectSaveLast( gg_rct_Weather02, 'FDwl' )
    set udg_Weather02Fog = GetLastCreatedWeatherEffect()
    call EnableWeatherEffect( udg_Weather02Fog, true )
    call AddWeatherEffectSaveLast( gg_rct_Weather03, 'FDwl' )
    set udg_Weather03Fog = GetLastCreatedWeatherEffect()
    call EnableWeatherEffect( udg_Weather03Fog, true )
    // Doodads
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZOob', gg_rct_Intro_Doodads01 )
    call SetDoodadAnimationRectBJ( "death", 'JZud', GetPlayableMapRect() )
    call SetDoodadAnimationRectBJ( "death", 'ZZcd', gg_rct_Eye_Vision_Area )
    call SetDoodadAnimationRectBJ( "death", 'ZZcd', gg_rct_Intro_Doodads01 )
    call SetDoodadAnimationRectBJ( "death", 'YOfs', gg_rct_Glyph2_DemonFire )
    call SetDoodadAnimationRectBJ( "death", 'YOfb', gg_rct_Glyph2_DemonFire )
    call SetDoodadAnimationRectBJ( "Death", 'DOas', GetEntireMapRect() )
    // Fog
    // Initial Visibility
    call CreateFogModifierRectBJ( true, Player(0), FOG_OF_WAR_MASKED, gg_rct_R1_AntiVision_Area )
    set udg_R1RocksAntiVM = GetLastCreatedFogModifier()
    call CreateFogModifierRectBJ( true, Player(0), FOG_OF_WAR_MASKED, gg_rct_R2_AntiVision_Area )
    set udg_R2RocksAntiVM = GetLastCreatedFogModifier()
    call CreateFogModifierRectBJ( true, Player(0), FOG_OF_WAR_MASKED, gg_rct_R3_AntiVision_Area )
    set udg_R3RocksAntiVM = GetLastCreatedFogModifier()
    // Bridges
    set udg_Bridge = gg_dest_YT14_0196
    call KillDestructable( udg_Bridge )
    // Gates
    call SetDoodadAnimationRectBJ( "death", 'DOas', gg_rct_Statue )
    set udg_SlimeDoor = gg_dest_ZTd7_0334
    call SetDestructableInvulnerableBJ( udg_SlimeDoor, true )
    // Floor Plates
    set udg_FloorPlate01A = gg_dest_DTfx_0707
    set udg_FloorPlate01B = gg_dest_DTfx_0708
    set udg_FloorPlate01C = gg_dest_DTfx_0706
    set udg_FloorPlate01D = gg_dest_DTfx_0327
    set udg_FloorPlate02A = gg_dest_DTfx_0240
    set udg_FloorPlate02B = gg_dest_DTfx_2573
    set udg_FloorPlate02C = gg_dest_DTfx_2572
    set udg_FloorPlate02D = gg_dest_DTfx_2574
    call SetDestructableAnimationBJ( udg_FloorPlate01A, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate01B, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate01C, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate01D, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate02A, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate02B, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate02C, "stand alternate" )
    call SetDestructableAnimationBJ( udg_FloorPlate02D, "stand alternate" )
