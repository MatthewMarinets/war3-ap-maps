function Trig_Intro_Cleanup_Actions takes nothing returns nothing
    // Restore Abilities
    call SetPlayerAbilityAvailableBJ( true, 'Ashm', udg_AP1_Player )
    // Remove Guldans Units
    call ForGroupBJ( udg_GuldansForces, function Trig_Intro_Cleanup_Func004002 )
    // Move Units
    call SetUnitPositionLocFacingBJ( udg_Maiev, GetRectCenter(gg_rct_Intro_Maiev_02), 0.00 )
    call SetUnitPositionLocFacingBJ( udg_IntroArcherA, GetRectCenter(gg_rct_Intro_ArcherA_02), 0.00 )
    call SetUnitPositionLocFacingBJ( udg_IntroArcherB, GetRectCenter(gg_rct_Intro_ArcherB_02), 0.00 )
    call SetUnitPositionLocFacingBJ( udg_IntroArcherC, GetRectCenter(gg_rct_Intro_ArcherC_02), 0.00 )
    call SetUnitPositionLocFacingBJ( udg_Naisha, GetRectCenter(gg_rct_Intro_Naisha_02), 0.00 )
    call SetUnitPositionLocFacingBJ( udg_IntroHuntressB, GetRectCenter(gg_rct_Intro_HuntressB_02), 0.00 )
    // Unhide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Intro_Cleanup_Func013002 )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Intro_Cleanup_Func015002 )
    call ForGroupBJ( udg_IntroStartingUnits, function Trig_Intro_Cleanup_Func016002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    call ConditionalTriggerExecute( gg_trg_Post_Cinematic_Pause )
    // Pause Time Of Day
    // Re-enable XP Gain
    if ( Trig_Intro_Cleanup_Func022001() ) then
        call SuspendHeroXPBJ( true, udg_Maiev )
    else
        call DoNothing(  )
    endif
    // Reset Doodads
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZOob', gg_rct_Intro_Doodads01 )
    call SetDoodadAnimationRectBJ( "Stand", 'ZCv1', gg_rct_Intro_Doodads01 )
    call SetDoodadAnimationRectBJ( "Stand", 'ZCv2', gg_rct_Intro_Doodads01 )
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZCv1', gg_rct_Intro_Doodads02 )
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZCv2', gg_rct_Intro_Doodads02 )
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZCv1', gg_rct_Intro_Doodads05 )
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZCv2', gg_rct_Intro_Doodads05 )
    call RemoveDestructable( gg_dest_B000_2512 )
    call RemoveDestructable( gg_dest_B000_2502 )
    call RemoveDestructable( gg_dest_B000_2508 )
    call RemoveDestructable( gg_dest_B000_2503 )
    call RemoveDestructable( gg_dest_B002_2504 )
    call RemoveDestructable( gg_dest_B001_2505 )
    call RemoveDestructable( gg_dest_B000_2506 )
    call SetDoodadAnimationRectBJ( "Death", 'XOmr', gg_rct_Intro_Doodads03 )
    call SetDoodadAnimationRectBJ( "Death", 'XOmr', gg_rct_Intro_Doodads04 )
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZZgr', gg_rct_Intro_Doodads03 )
    call SetDoodadAnimationRectBJ( "Stand Alternate", 'ZZgr', gg_rct_Intro_Doodads04 )
    call SetDestructableAnimationBJ( gg_dest_ZTsg_2501, "death" )
    // Restore Selection
    call SelectUnitSingle( udg_Maiev )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP1_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP1_Player, CameraSetupGetDestPositionLoc(gg_cam_Intro_Skipped), 0 )
    // Turn off Weather
    call EnableWeatherEffect( udg_Weather01Rays, false )
    call EnableWeatherEffect( udg_Weather01Fog, true )
    // Wipe Visibility
    call CreateFogModifierRectBJ( true, udg_AP1_Player, FOG_OF_WAR_MASKED, GetPlayableMapRect() )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
    call CreateFogModifierRectBJ( true, udg_AP1_Player, FOG_OF_WAR_VISIBLE, gg_rct_Intro_Vision_Area )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
endfunction

