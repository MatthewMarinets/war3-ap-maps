    call GroupRemoveGroup( udg_IntroStartingUnits, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Intro_Setup_Func015002 )
    // Hide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Intro_Setup_Func017002 )
