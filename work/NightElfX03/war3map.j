//===========================================================================
function Trig_Bridge_Resurrection_Actions takes nothing returns nothing
    call DestructableRestoreLife( udg_Bridge, GetDestructableMaxLife(udg_Bridge), true )
    // Skip 3 seconds of animation
    call SetDestAnimationSpeedPercent( udg_Bridge, 600.00 )
    call PolledWait( 0.50 )
