    call TriggerSleepAction( 0.10 )
    // Find a valid speaker, if one exists
    set udg_Speaker = GroupPickRandomUnit(GetUnitsOfPlayerMatching(udg_AP1_Player, Condition(function Trig_R3_Rescued_Func018002001002)))
