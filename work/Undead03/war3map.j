//
// MASTER SWITCH
// Runs all starting Triggers
//===========================================================================
function Trig_Map_Initilization_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_U )
    call FogMaskEnableOn(  )
    call ConditionalTriggerExecute( gg_trg_Load_Hero )
    call ConditionalTriggerExecute( gg_trg_Setup_Sylvanas )
    call ConditionalTriggerExecute( gg_trg_Setup_AI_Alliance )
    call ConditionalTriggerExecute( gg_trg_Setup_Undead )
    call ConditionalTriggerExecute( gg_trg_Setup_Quest )
    call ConditionalTriggerExecute( gg_trg_Setup_AI_Ignore_Unit )
    call ConditionalTriggerExecute( gg_trg_Undead_Units_Dead )
    call ConditionalTriggerExecute( gg_trg_Setup_All_Players )
    call ConditionalTriggerExecute( gg_trg_Setup_Elf_Group )
    call ConditionalTriggerExecute( gg_trg_Setup_Difficulty )
    call ConditionalTriggerExecute( gg_trg_SetupBridge )
    call ConditionalTriggerExecute( gg_trg_Setup_Easy_Diff )
    call ConditionalTriggerExecute( gg_trg_Setup_Sounds )
    call CommentString( "----------" )
    call ConditionalTriggerExecute( gg_trg_Cinematic_Dialogue_A )
