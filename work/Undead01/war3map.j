    call GroupRemoveGroup( udg_TempNearbyUnits, udg_RelaxedVillagers )
    call CommentString( "If there are any runners, set the focus to the dead unit's loc" )
    if ( Trig_T1Villagers_Scared_Func008001() ) then
        return
