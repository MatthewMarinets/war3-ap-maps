    set udg_TeleportEffectsTail = ( udg_TeleportEffectsTail + 1 )
    call CommentString( "Fade acolyte back in" )
    call SetUnitInvulnerable( GetOrderedUnit(), true )
    call ShowUnitShow( GetOrderedUnit() )
    call TriggerSleepAction( udg_AcolyteFadeStep )
