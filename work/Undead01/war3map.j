function Trig_Bridge_Scene_Conditions takes nothing returns boolean
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    if ( not ( udg_BridgeAborted == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Bridge_Scene_Func004001 takes nothing returns boolean
    return ( udg_BridgeAcolyteBubbleExists == true )
endfunction

function Trig_Bridge_Scene_Func018001 takes nothing returns boolean
    return ( udg_BridgeAborted == true )
endfunction

function Trig_Bridge_Scene_Func021001 takes nothing returns boolean
    return ( udg_BridgeAborted == true )
endfunction

function Trig_Bridge_Scene_Func024001 takes nothing returns boolean
    return ( udg_BridgeAborted == true )
endfunction

function Trig_Bridge_Scene_Actions takes nothing returns nothing
    call CommentString( "Drop the TalkToMe bubble" )
    if ( Trig_Bridge_Scene_Func004001() ) then
        call DestroyEffectBJ( udg_BridgeAcolyteBubble )
    else
        call DoNothing(  )
    endif
    set udg_BridgeAcolyteBubbleExists = false
    call CommentString( "Acolyte does his stuff" )
    call IssuePointOrderLocBJ( udg_BridgeAcolyte, "move", GetRectCenter(gg_rct_Bridge_Villager_Dest) )
    call TriggerSleepAction( 0.50 )
    call IssueImmediateOrderBJ( udg_BridgeAcolyte, "stop" )
    call CommentString( "Replace the AcolyteInHiding with a real Acolyte" )
    call ReplaceUnitBJ( udg_BridgeAcolyte, 'uaco', bj_UNIT_STATE_METHOD_RELATIVE )
    call SetUnitFacingToFaceUnitTimed( GetLastReplacedUnitBJ(), udg_Arthas, 0.10 )
    set udg_BridgeAcolyte = GetLastReplacedUnitBJ()
    call CommentString( "Acolyte does his stuff" )
    call UnitShareVisionBJ( true, udg_BridgeAcolyte, udg_AP4_Undead )
    call SetSpeechVolumeGroupsBJ(  )
    call TransmissionFromUnitWithNameBJ( udg_APG4_Undead, udg_BridgeAcolyte, "TRIGSTR_039", gg_snd_U01Acolyte10, "TRIGSTR_040", bj_TIMETYPE_ADD, 0, true )
    if ( Trig_Bridge_Scene_Func018001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetUnitFacingToFaceUnitTimed( udg_BridgeAcolyte, udg_Arthas, 0.40 )
    call TransmissionFromUnitWithNameBJ( udg_APG4_Undead, udg_Arthas, "TRIGSTR_041", gg_snd_U01Arthas11, "TRIGSTR_045", bj_TIMETYPE_SUB, 0.20, true )
    if ( Trig_Bridge_Scene_Func021001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetUnitFacingToFaceUnitTimed( udg_BridgeAcolyte, udg_Arthas, 0.40 )
    call TransmissionFromUnitWithNameBJ( udg_APG4_Undead, udg_BridgeAcolyte, "TRIGSTR_048", gg_snd_U01Acolyte12, "TRIGSTR_049", bj_TIMETYPE_ADD, 0, true )
    if ( Trig_Bridge_Scene_Func024001() ) then
        return
    else
        call DoNothing(  )
    endif
    call DisableTrigger( gg_trg_Bridge_Acolyte_Dies )
    call VolumeGroupResetBJ(  )
    call CommentString( "Signal the summoning" )
    call IssueImmediateOrderBJ( udg_BridgeAcolyte, "holdposition" )
    call TriggerSleepAction( 1.60 )
    call CommentString( "Display hint" )
    call ConditionalTriggerExecute( gg_trg_Hint_Red_Acolytes )
    call TriggerSleepAction( 10.00 )
    call CommentString( "Dequeue the scene" )
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

