    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 80.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 70.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 60.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 50.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 40.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 30.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 20.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 10.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call SetUnitVertexColorBJ( GetOrderedUnit(), 100, 100, 100, 0.00 )
    call TriggerSleepAction( udg_AcolyteFadeStep )
    call CommentString( "Destroy the effect at the destination" )
    call DestroyEffectBJ( udg_TeleportEffects[udg_TeleportEffectsTail] )
    set udg_TeleportEffectsTail = ( udg_TeleportEffectsTail + 1 )
    call CommentString( "Keep track of the new recruit" )
    call SetUnitInvulnerable( GetOrderedUnit(), false )
    call GroupAddUnitSimple( GetOrderedUnit(), udg_RescuedAcolytes )
    set udg_RescuedAcolytesCount = ( udg_RescuedAcolytesCount + 1 )
    call ConditionalTriggerExecute( gg_trg_Acolyte_Leaderboard_Update )
    call CommentString( "Throw the player a bone" )
    call AddHeroXPSwapped( udg_AcolyteXP, udg_Arthas, true )
    call CommentString( "Acolyte does his stuff" )
    call SetUnitFacingToFaceUnitTimed( GetOrderedUnit(), udg_Tichondrius, 0.20 )
    call TriggerSleepAction( 0.20 )
    call SetUnitAnimation( GetOrderedUnit(), "stand work gold" )
    call QueueUnitAnimationBJ( GetOrderedUnit(), "stand" )
endfunction

