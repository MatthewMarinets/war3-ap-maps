function Trig_Bandit_Scene_Actions takes nothing returns nothing
    call CommentString( "Bandits attack" )
    call CreateFogModifierRadiusLocBJ( true, udg_AP4_Undead, FOG_OF_WAR_VISIBLE, GetUnitLoc(udg_TownHall), 768.00 )
    set udg_TownHallVM = GetLastCreatedFogModifier()
    call SmartCameraPanBJ( udg_AP4_Undead, CameraSetupGetDestPositionLoc(gg_cam_Bandit_Scene), 0.5 )
    call TriggerSleepAction( 0.50 )
    call ForGroup( udg_BanditRaiders, function Trig_Bandit_Scene_Func007002 )
