//===========================================================================
// Trigger: PatrolA Heartbeat
//
// Every heartbeat, the patrol is sent off to its current destination.  If a unit gets stuck, it will just have to wait for the next heartbeat to retry.
//===========================================================================
function Trig_PatrolA_Heartbeat_Func004001 takes nothing returns boolean
    return ( udg_PatrolACaptain != null )
endfunction

function Trig_PatrolA_Heartbeat_Func007001 takes nothing returns boolean
    return ( udg_PatrolOrderReset == true )
endfunction

function Trig_PatrolA_Heartbeat_Func008001 takes nothing returns boolean
    return ( udg_PatrolAGuard01 != null )
endfunction

function Trig_PatrolA_Heartbeat_Func009001 takes nothing returns boolean
    return ( udg_PatrolAGuard02 != null )
endfunction

function Trig_PatrolA_Heartbeat_Func010001 takes nothing returns boolean
    return ( udg_PatrolAGuard03 != null )
endfunction

function Trig_PatrolA_Heartbeat_Func011001 takes nothing returns boolean
    return ( udg_PatrolAGuard04 != null )
endfunction

function Trig_PatrolA_Heartbeat_Actions takes nothing returns nothing
    set udg_PatrolOrderReset = false
    call CommentString( "Let the captain take the first few steps" )
    if ( Trig_PatrolA_Heartbeat_Func004001() ) then
        call IssuePointOrderLocBJ( udg_PatrolACaptain, "attack", udg_PatrolATargetCap )
    else
        call DoNothing(  )
    endif
    call TriggerSleepAction( 0.50 )
    call CommentString( "If the timer was just restarted, wait for the next heartbeat" )
    if ( Trig_PatrolA_Heartbeat_Func007001() ) then
        return
    else
        call DoNothing(  )
    endif
    if ( Trig_PatrolA_Heartbeat_Func008001() ) then
        call IssuePointOrderLocBJ( udg_PatrolAGuard01, "attack", udg_PatrolATargetG01 )
    else
        call DoNothing(  )
    endif
    if ( Trig_PatrolA_Heartbeat_Func009001() ) then
        call IssuePointOrderLocBJ( udg_PatrolAGuard02, "attack", udg_PatrolATargetG02 )
    else
        call DoNothing(  )
    endif
    if ( Trig_PatrolA_Heartbeat_Func010001() ) then
        call IssuePointOrderLocBJ( udg_PatrolAGuard03, "attack", udg_PatrolATargetG03 )
    else
        call DoNothing(  )
    endif
    if ( Trig_PatrolA_Heartbeat_Func011001() ) then
        call IssuePointOrderLocBJ( udg_PatrolAGuard04, "attack", udg_PatrolATargetG04 )
    else
        call DoNothing(  )
    endif
    call StartTimerBJ( udg_PatrolATimer, false, 8.00 )
endfunction

