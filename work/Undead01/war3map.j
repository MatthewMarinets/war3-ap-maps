    call IssuePointOrderLocBJ( udg_BanditsRunGuy02, "move", GetRectCenter(gg_rct_BanditsRunGuy02_Dest) )
    call TriggerSleepAction( 0.50 )
    call IssueImmediateOrderBJ( udg_TownHall, "townbellon" )
    call TriggerSleepAction( 3.00 )
    call CommentString( "Dequeue the scene" )
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
    call CommentString( "VM persists for a short while after the scene starts" )
    call TriggerSleepAction( 10.00 )
    call DestroyFogModifier( udg_TownHallVM )
