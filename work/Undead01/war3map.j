    call CommentString( "If there are any villagers nearby, send them running" )
    set udg_TempNearbyUnits = GetUnitsInRangeOfLocMatching(384.00, GetUnitLoc(udg_ScaredUnit), Condition(function Trig_T1Villagers_Scared_Func003002003))
