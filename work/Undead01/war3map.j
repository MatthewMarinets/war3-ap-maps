        call PlaySoundBJ( gg_snd_BanditSayYourPrayers )
    else
        call DoNothing(  )
    endif
    call TriggerSleepAction( 1.50 )
    call CommentString( "Villagers panic, peasants become militia" )
    if ( Trig_Bandit_Scene_Func013001() ) then
        call PlaySoundBJ( gg_snd_BanditsRun )
