function Trig_Sheep_Scene_Actions takes nothing returns nothing
    call CommentString( "Replace the AcolyteInHiding with a real Acolyte" )
    call ShowUnitHide( udg_SheepShield01 )
    call ShowUnitHide( udg_SheepShield02 )
    call ReplaceUnitBJ( udg_SheepAcolyte, 'uaco', bj_UNIT_STATE_METHOD_RELATIVE )
    call SetUnitFacingToFaceUnitTimed( GetLastReplacedUnitBJ(), udg_Arthas, 0.10 )
    set udg_SheepAcolyte = GetLastReplacedUnitBJ()
    call ShowUnitShow( udg_SheepShield01 )
    call ShowUnitShow( udg_SheepShield02 )
    call CommentString( "Signal the summoning" )
    call IssueImmediateOrderBJ( udg_SheepAcolyte, "holdposition" )
    call CommentString( "Acolyte does his stuff" )
    call UnitShareVisionBJ( true, udg_SheepAcolyte, udg_AP4_Undead )
    call ConditionalTriggerExecute( gg_trg_Sheep_Drop )
    call PauseUnitBJ( false, udg_SheepShield02 )
    call ForGroup( GetUnitsInRangeOfLocMatching(512.00, GetUnitLoc(udg_SheepAcolyte), Condition(function Trig_Sheep_Scene_Func015001003)), function Trig_Sheep_Scene_Func015002 )
    call ConditionalTriggerExecute( gg_trg_Random_Acolyte_Ack )
    call CommentString( "Dequeue the scene" )
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

