function Trig_Init_06_Acolytes_Actions takes nothing returns nothing
    call CommentString( "Init Bridge Scene Acolyte" )
    set udg_BridgeAcolyte = gg_unit_nC00_0015
    call AddSpecialEffectTargetUnitBJ( "overhead", udg_BridgeAcolyte, "Abilities\\Spells\\Other\\TalkToMe\\TalkToMe.mdl" )
    set udg_BridgeAcolyteBubble = GetLastCreatedEffectBJ()
    set udg_BridgeAcolyteBubbleExists = true
    call CommentString( "Init GraveHint Acolyte" )
    set udg_GraveTipAcolyte = gg_unit_nC00_0024
    call AddSpecialEffectTargetUnitBJ( "overhead", udg_GraveTipAcolyte, "Abilities\\Spells\\Other\\TalkToMe\\TalkToMe.mdl" )
    set udg_GraveTipAcolyteBubble = GetLastCreatedEffectBJ()
    set udg_GraveTipAcolyteBubbleExists = true
    call CommentString( "Init Harvesters" )
    set udg_HarvesterFake = gg_unit_nC00_0021
    call CommentString( "Init Bandit Captive" )
    set udg_BanditCaptive = gg_unit_nC00_0099
    call AddSpecialEffectTargetUnitBJ( "origin", udg_BanditCaptive, "Abilities\\Spells\\Orc\\Ensnare\\EnsnareTarget.mdl" )
    set udg_BanditCaptiveNet = GetLastCreatedEffectBJ()
    set udg_BanditCaptiveNetExists = true
    call CommentString( "Init Gate Scene Acolyte" )
    set udg_GateAcolyte = gg_unit_nC00_0104
    call ShowUnitHide( udg_GateAcolyte )
    call CommentString( "Init Sheep Scene Acolyte" )
    set udg_SheepAcolyte = gg_unit_nC00_0106
    call SetUnitAnimation( udg_SheepAcolyte, "stand ready" )
    call CommentString( "Init Cornered Acolyte" )
    set udg_CorneredAcolyte = gg_unit_nC00_0066
    call SetUnitAnimation( udg_CorneredAcolyte, "stand ready" )
    call CommentString( "Init Acolytes" )
    set udg_RescuableAcolytes = GetUnitsOfPlayerAndTypeId(Player(0), 'nC00')
    call GroupRemoveUnitSimple( udg_BridgeAcolyte, udg_RescuableAcolytes )
    call GroupRemoveUnitSimple( udg_GraveTipAcolyte, udg_RescuableAcolytes )
    call GroupRemoveUnitSimple( udg_BanditCaptive, udg_RescuableAcolytes )
    call GroupRemoveUnitSimple( udg_SheepAcolyte, udg_RescuableAcolytes )
    call CommentString( "Setup Acolyte Dancing" )
    set udg_AcolyteDanceAngleMin = AngleBetweenPoints(GetRectCenter(gg_rct_Acolyte_Dance_Focus), GetRectCenter(gg_rct_Acolyte_Dance_Angle_Min))
    set udg_AcolyteDanceAngleMax = AngleBetweenPoints(GetRectCenter(gg_rct_Acolyte_Dance_Focus), GetRectCenter(gg_rct_Acolyte_Dance_Angle_Max))
    set udg_AcolyteDanceDistMin = DistanceBetweenPoints(GetRectCenter(gg_rct_Acolyte_Dance_Focus), GetRectCenter(gg_rct_Acolyte_Dance_Distance_Min))
    set udg_AcolyteDanceDistMax = DistanceBetweenPoints(GetRectCenter(gg_rct_Acolyte_Dance_Focus), GetRectCenter(gg_rct_Acolyte_Dance_Distance_Max))
    call StartTimerBJ( udg_AcolyteDanceTimer, true, 2.00 )
    call TriggerSleepAction( 0.01 )
    call CommentString( "Re-count the acolytes on the map." )
    set udg_AcolytesPossible = CountUnitsInGroup(GetUnitsOfPlayerAndTypeId(udg_P1_RedVillagers, 'nC00'))
    call CommentString( "Failsafe - If the mission goal is impossible, spawn 3 more acolytes in the graveyard." )
    if ( Trig_Init_06_Acolytes_Func043001() ) then
        set bj_forLoopAIndex = 1
        set bj_forLoopAIndexEnd = 3
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call CreateNUnitsAtLocFacingLocBJ( 1, 'nC00', Player(0), GetRandomLocInRect(gg_rct_Graveyard_Area), GetRectCenter(gg_rct_Graveyard_Area) )
            set bj_forLoopAIndex = bj_forLoopAIndex + 1
        endloop
    else
        call DoNothing(  )
    endif
endfunction

