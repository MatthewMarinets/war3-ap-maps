    call PlaySoundBJ( gg_snd_Skeleton01RescueAck )
    call PlaySoundBJ( gg_snd_Ghoul03ARescueAck )
    call TriggerSleepAction( 1.10 )
    call ConditionalTriggerExecute( gg_trg_QuestGraveyard_Complete )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    call CommentString( "Display the Cannibalize hint" )
