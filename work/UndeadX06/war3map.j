    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P05_YellowUndead, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P06_OrangeUndead, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P07_GreenUndead, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P10_LightBlueDwarves, bj_ALLIANCE_NEUTRAL )
    // Initial Resources
    call SetPlayerStateBJ( udg_AP02_SylvanasPlayer, PLAYER_STATE_RESOURCE_GOLD, 500 )
    call SetPlayerStateBJ( udg_AP02_SylvanasPlayer, PLAYER_STATE_RESOURCE_LUMBER, 150 )
    // -
    call SetPlayerStateBJ( udg_P09_GarithosHuman, PLAYER_STATE_RESOURCE_GOLD, 500 )
    call SetPlayerStateBJ( udg_P09_GarithosHuman, PLAYER_STATE_RESOURCE_LUMBER, 150 )
    // Purple
    call SetPlayerStateBJ( udg_P04_PurpleUndead, PLAYER_STATE_RESOURCE_GOLD, 5000 )
    call SetPlayerStateBJ( udg_P04_PurpleUndead, PLAYER_STATE_RESOURCE_LUMBER, 5000 )
    // Yellow
    call SetPlayerStateBJ( udg_P05_YellowUndead, PLAYER_STATE_RESOURCE_GOLD, 5000 )
    call SetPlayerStateBJ( udg_P05_YellowUndead, PLAYER_STATE_RESOURCE_LUMBER, 5000 )
    // Orange
    call SetPlayerStateBJ( udg_P06_OrangeUndead, PLAYER_STATE_RESOURCE_GOLD, 5000 )
    call SetPlayerStateBJ( udg_P06_OrangeUndead, PLAYER_STATE_RESOURCE_LUMBER, 5000 )
    // Green
    call SetPlayerStateBJ( udg_P07_GreenUndead, PLAYER_STATE_RESOURCE_GOLD, 5000 )
    call SetPlayerStateBJ( udg_P07_GreenUndead, PLAYER_STATE_RESOURCE_LUMBER, 5000 )
    // Player Colors
    call SetPlayerColorBJ( udg_P05_YellowUndead, PLAYER_COLOR_ORANGE, true )
    call SetPlayerColorBJ( udg_P01_RedUndead, PLAYER_COLOR_PURPLE, true )
    call SetPlayerColorBJ( udg_P09_GarithosHuman, PLAYER_COLOR_LIGHT_BLUE, true )
