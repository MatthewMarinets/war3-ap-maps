    call SetPlayerAllianceStateBJ( udg_AP02_SylvanasPlayer, udg_P09_GarithosHuman, bj_ALLIANCE_ALLIED_ADVUNITS )
    call SetPlayerAllianceStateBJ( udg_P09_GarithosHuman, udg_AP02_SylvanasPlayer, bj_ALLIANCE_ALLIED_ADVUNITS )
