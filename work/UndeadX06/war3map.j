function Trig_Victory_Setup_Actions takes nothing returns nothing
    call SetPlayerAllianceStateBJ( Player(1), Player(3), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(8), Player(3), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(3), Player(8), bj_ALLIANCE_NEUTRAL )
