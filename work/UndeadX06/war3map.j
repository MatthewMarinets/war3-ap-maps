function Trig_Dwarves_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG2_Player )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Dwarves_Cinematic_Q_Func009C() ) then
        return
    else
    endif
    call ConditionalTriggerExecute( gg_trg_Dwarves_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Mid_Dwarves_01, udg_AP02_SylvanasPlayer, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Mid_Dwarves_03, udg_AP02_SylvanasPlayer, 5.00 )
    // Units Movement
    call CreateNUnitsAtLoc( 1, 'hfoo', udg_P09_GarithosHuman, GetRectCenter(gg_rct_Dwarves_Footman_01), 10.00 )
    set udg_DwarvesFootman01 = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'hfoo', udg_P09_GarithosHuman, GetRectCenter(gg_rct_Dwarves_Footman_02_A), 260.00 )
    set udg_DwarvesFootman02 = GetLastCreatedUnit()
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Dwarves_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call SetUnitFacingToFaceUnitTimed( udg_DwarvesFootman01, udg_Garithos, 0.60 )
    call IssuePointOrderLocBJ( udg_DwarvesFootman02, "move", GetRectCenter(gg_rct_Dwarves_Footman_02_Aa) )
    call TriggerSleepAction( 1.30 )
    call IssuePointOrderLocBJ( udg_DwarvesFootman02, "move", GetRectCenter(gg_rct_Dwarves_Footman_02_B) )
    call TriggerSleepAction( 1.30 )
    if ( Trig_Dwarves_Cinematic_Q_Func031001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_DwarvesFootman02, "TRIGSTR_197", gg_snd_L06Footman17, "TRIGSTR_198", bj_TIMETYPE_ADD, 0, false )
    call SetUnitFacingToFaceUnitTimed( udg_Garithos, udg_DwarvesFootman02, 0.60 )
    call WaitForSoundBJ( gg_snd_L06Footman17, 0 )
    if ( Trig_Dwarves_Cinematic_Q_Func035001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Garithos, "TRIGSTR_199", gg_snd_L06Garithos18, "TRIGSTR_200", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_L06Garithos18, 0 )
    if ( Trig_Dwarves_Cinematic_Q_Func038001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Dwarves_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Dwarves_Cleanup )
    call CinematicModeBJ( false, udg_APG2_Player )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_Dwarves_Discover )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    set udg_InCinematic = false
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

