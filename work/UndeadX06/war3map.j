    call SetPlayerOnScoreScreenBJ( false, udg_P01_RedUndead )
    call SetPlayerOnScoreScreenBJ( false, udg_P05_YellowUndead )
    call SetPlayerOnScoreScreenBJ( false, udg_P10_LightBlueDwarves )
