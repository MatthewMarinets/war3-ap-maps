    // Set Sky
    call SetSkyModel( "Environment\\Sky\\LordaeronWinterSkyRed\\LordaeronWinterSkyRed.mdl" )
    // Clear Selection
    call ClearSelection(  )
    // Revive Hero
    set udg_RevivalHero = udg_Sylvanas
    set udg_RevivalSpot = GetRectCenter(gg_rct_Sylvanas_End)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_SylvanasReturnSpot = udg_RevivalReturnSpot
    // Revive Hero
    set udg_RevivalHero = udg_Varimathras
    set udg_RevivalSpot = GetRectCenter(gg_rct_Varimathras_End)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_VarimathrasReturnSpot = udg_RevivalReturnSpot
    // Revive Hero
    set udg_RevivalHero = udg_Balnazzar
    set udg_RevivalSpot = GetRectCenter(gg_rct_Balnazzar_End)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_BalnazzarReturnSpot = udg_RevivalReturnSpot
    // Revive Hero
    set udg_RevivalHero = udg_Garithos
    set udg_RevivalSpot = GetRectCenter(gg_rct_Garithos_End)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_GarithosReturnSpot = udg_RevivalReturnSpot
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Sylvanas )
    call SuspendHeroXPBJ( false, udg_Garithos )
    call SuspendHeroXPBJ( false, udg_Balnazzar )
    call SuspendHeroXPBJ( false, udg_Varimathras )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Sylvanas )
    call PauseUnitBJ( false, udg_Varimathras )
    call PauseUnitBJ( false, udg_Garithos )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerAll(udg_AP02_SylvanasPlayer)
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P01_RedUndead), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P04_PurpleUndead), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P05_YellowUndead), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P06_OrangeUndead), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P07_GreenUndead), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P09_GarithosHuman), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P10_LightBlueDwarves), udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Sylvanas, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Varimathras, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Balnazzar, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Garithos, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func054002 )
    // Disable Aura
    call SetPlayerAbilityAvailableBJ( false, 'AUav', udg_AP02_SylvanasPlayer )
    call SetPlayerAbilityAvailableBJ( false, 'AUav', udg_P04_PurpleUndead )
    call SetPlayerAbilityAvailableBJ( false, 'AHad', udg_P09_GarithosHuman )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP02_SylvanasPlayer), function Trig_Victory_Setup_Func060002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P04_PurpleUndead), function Trig_Victory_Setup_Func061002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P07_GreenUndead), function Trig_Victory_Setup_Func062002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P09_GarithosHuman), function Trig_Victory_Setup_Func063002 )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\TragicConfrontation.mp3" )
endfunction

