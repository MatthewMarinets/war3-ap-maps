    call CinematicModeBJ( true, udg_APG2_Player )
    call ConditionalTriggerExecute( gg_trg_Intro_Setup )
    call CameraSetupApplyForPlayer( true, gg_cam_Intro_01, udg_AP02_SylvanasPlayer, 0 )
