//===========================================================================
// Trigger: SetupFormations
//
// The locations of the preplaced "Formation" regions are read and stored as formation data (in polar coordinates).
// For simplicity, the initial formation is assumed to be facing 0 degrees (east).
//===========================================================================
function Trig_SetupFormations_Actions takes nothing returns nothing
    call CommentString( "Configure Region Array" )
    set udg_FormationRegionArray[1] = gg_rct_Formation1
    set udg_FormationRegionArray[2] = gg_rct_Formation2
    set udg_FormationRegionArray[3] = gg_rct_Formation3
    set udg_FormationRegionArray[4] = gg_rct_Formation4
    set udg_FormationRegionArray[5] = gg_rct_Formation5
    set udg_FormationRegionArray[6] = gg_rct_Formation6
    set udg_FormationRegionArray[7] = gg_rct_Formation7
    set udg_FormationRegionArray[8] = gg_rct_Formation8
    set udg_FormationRegionArray[9] = gg_rct_Formation9
    call CommentString( "Configure Unit Ranges" )
    set udg_FormationTotal = 9
    set udg_CaravanWagonFirst = 1
    set udg_CaravanWagonLast = 4
    set udg_CaravanGuardFirst = 5
    set udg_CaravanGuardLast = 9
    call CommentString( "Read formation data from initial region placement" )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = udg_FormationTotal
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_FormationDistanceArray[GetForLoopIndexA()] = DistanceBetweenPoints(GetRectCenter(gg_rct_Formation_Center), GetRectCenter(udg_FormationRegionArray[GetForLoopIndexA()]))
