        return false
    endif
    if ( not ( CountUnitsInGroup(udg_BatriderGroup) < 6 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Check_for_Batrider_Replacing_Actions takes nothing returns nothing
    set udg_TempPointA = GetRandomLocInRect(gg_rct_ReplacementBatriderChill)
    call MoveRectToLoc( udg_ReplacementBatriderTarget, udg_TempPointA )
    call RemoveLocation( udg_TempPointA )
    set udg_TempPointA = GetRectCenter(gg_rct_ReplacementBatriderSpawn)
    call CreateNUnitsAtLoc( 1, 'otbr', udg_P04_Trolls, udg_TempPointA, bj_UNIT_FACING )
    set udg_NewBatrider = GetLastCreatedUnit()
    call RemoveLocation( udg_TempPointA )
    call GroupAddUnitSimple( udg_NewBatrider, udg_BatriderGroup )
    set udg_TempPointA = GetRectCenter(gg_rct_ReplacementBatriderTarget)
    call IssuePointOrderLocBJ( udg_NewBatrider, "move", udg_TempPointA )
    call RemoveLocation( udg_TempPointA )
endfunction

