    call SetPlayerAbilityAvailableBJ( true, 'AIae', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( true, 'AIcf', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( true, 'AIgx', udg_AP1_Player )
