    call SetDoodadAnimationRectBJ( "death", 'D001', gg_rct_RunesToPyreIsland )
    // Turn on Runes
    call SetDoodadAnimationRectBJ( "stand", 'D002', gg_rct_RunesToOrgrimmar )
    call SetDoodadAnimationRectBJ( "stand", 'D001', gg_rct_RunesToOrgrimmar )
    // Put Special Effect Over Voljin
    call AddSpecialEffectTargetUnitBJ( "overhead", udg_Voljin, "Abilities\\Spells\\Other\\TalkToMe\\TalkToMe.mdl" )
    set udg_SpeciaEffectTalkToMe = GetLastCreatedEffectBJ()
    // Reveal Darkspear Village
    call CreateFogModifierRadiusLocBJ( true, udg_AP1_Player, FOG_OF_WAR_VISIBLE, GetRectCenter(gg_rct_VisionVillage), 1280.00 )
    set udg_VMVillage = GetLastCreatedFogModifier()
    // Ping and Space Bar
    call SetCameraQuickPositionLocForPlayer( udg_AP1_Player, GetRectCenter(gg_rct_VisionVillage) )
    call PingMinimapLocForForce( udg_APG1_Player, GetRectCenter(gg_rct_VisionVillage), 7.00 )
