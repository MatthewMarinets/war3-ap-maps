function Trig_Intro_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    // Set Up Scene
    call CinematicModeBJ( true, GetPlayersAll() )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Intro_Cinematic_Q_Func008001() ) then
        return
    else
        call DoNothing(  )
    endif
    call ConditionalTriggerExecute( gg_trg_Intro_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Intro01A, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Intro01B, udg_AP1_Player, 10.00 )
    call IssuePointOrderLocBJ( udg_IntroZeppelin, "move", GetRectCenter(gg_rct_IntroZeppelin02) )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Intro_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TriggerSleepAction( 5.00 )
    if ( Trig_Intro_Cinematic_Q_Func022001() ) then
        return
    else
        call DoNothing(  )
    endif
    // Make Zeppelin Land
    call SetUnitFlyHeightBJ( udg_IntroZeppelin, 90.00, 75.00 )
    call TriggerSleepAction( 2 )
    if ( Trig_Intro_Cinematic_Q_Func026001() ) then
        return
    else
        call DoNothing(  )
    endif
    // Create Cinematic Rexxar
    call CreateNUnitsAtLoc( 1, 'Orex', udg_AP1_Player, GetRectCenter(gg_rct_IntroRexxar01), 45.00 )
    call PlaySoundOnUnitBJ( gg_snd_Loading, 100, udg_IntroZeppelin )
    set udg_RexxarCinematic = GetLastCreatedUnit()
    call SetUnitInvulnerable( udg_RexxarCinematic, true )
    call SuspendHeroXPBJ( false, udg_RexxarCinematic )
    // Create Cinematic Rokhan
    call CreateNUnitsAtLoc( 1, 'Orkn', udg_AP1_Player, GetRectCenter(gg_rct_IntroRokhan01), 45.00 )
    set udg_RokhanCinematic = GetLastCreatedUnit()
    call SetUnitInvulnerable( udg_RokhanCinematic, true )
    call SuspendHeroXPBJ( false, udg_RokhanCinematic )
    // Create Cinematic Chen
    if ( Trig_Intro_Cinematic_Q_Func039C() ) then
        call CreateNUnitsAtLoc( 1, 'Nsjs', udg_AP1_Player, GetRectCenter(gg_rct_IntroChen01), 45.00 )
        set udg_ChenCinematic = GetLastCreatedUnit()
        call SetUnitInvulnerable( udg_ChenCinematic, true )
        call SuspendHeroXPBJ( false, udg_ChenCinematic )
    else
    endif
    // Heroes move forward
    call IssuePointOrderLocBJ( udg_RexxarCinematic, "move", GetRectCenter(gg_rct_SpawnPoint1A) )
    call TriggerSleepAction( 0.50 )
    if ( Trig_Intro_Cinematic_Q_Func043001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_Intro02, udg_AP1_Player, 0 )
    call TriggerSleepAction( 1.50 )
    if ( Trig_Intro_Cinematic_Q_Func046001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_RokhanCinematic, "move", GetRectCenter(gg_rct_SpawnPoint1B) )
    call IssuePointOrderLocBJ( udg_ChenCinematic, "move", GetRectCenter(gg_rct_SpawnPoint1C) )
    // Zeppelin Lifts Off
    call SetUnitFlyHeightBJ( udg_IntroZeppelin, 280.00, 75.00 )
    call TriggerSleepAction( 3.00 )
    if ( Trig_Intro_Cinematic_Q_Func052001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_Intro03A, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Intro03B, udg_AP1_Player, ( GetSoundDurationBJ(gg_snd_D09ARexxar02) + 1 ) )
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_RexxarCinematic, "TRIGSTR_739", gg_snd_D09ARexxar02, "TRIGSTR_740", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_D09ARexxar02, 0 )
    if ( Trig_Intro_Cinematic_Q_Func057001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Intro_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Intro_Cleanup )
    call CinematicModeBJ( false, GetPlayersAll() )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_WarnTrolls_Update )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    set udg_InCinematic = false
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

