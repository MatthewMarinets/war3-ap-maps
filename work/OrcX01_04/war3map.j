    // Remove Surviving Batriders
    call ForGroupBJ( GetUnitsOfTypeIdAll('otbr'), function Trig_Battleships_Destroyed_Cleanup_Func021002 )
    // Remove Surviving Humans
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P11_Humans), function Trig_Battleships_Destroyed_Cleanup_Func023002 )
    // Unhide Units
    call GroupAddUnitSimple( udg_Rexxar, udg_HiddenUnits )
    call GroupAddUnitSimple( udg_Rokhan, udg_HiddenUnits )
    call GroupAddUnitSimple( udg_Chen, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Battleships_Destroyed_Cleanup_Func028002 )
    // Change Owner of Heroes
    call SetUnitOwner( udg_Rexxar, udg_AP1_Player, true )
    call SetUnitOwner( udg_Rokhan, udg_AP1_Player, true )
    call SetUnitOwner( udg_Chen, udg_AP1_Player, true )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Battleships_Destroyed_Cleanup_Func038001() ) then
        call SuspendHeroXPBJ( true, udg_Rexxar )
