    // Position Heroes
    call SetUnitPositionLoc( udg_Rexxar, GetRectCenter(gg_rct_SpawnPoint2A) )
    call SetUnitPositionLoc( udg_Rokhan, GetRectCenter(gg_rct_SpawnPoint2B) )
    call SetUnitPositionLoc( udg_Chen, GetRectCenter(gg_rct_SpawnPoint2C) )
    call SetUnitFacingTimed( udg_Rexxar, bj_UNIT_FACING, 315.00 )
    call SetUnitFacingTimed( udg_Rokhan, bj_UNIT_FACING, 315.00 )
    call SetUnitFacingTimed( udg_Chen, bj_UNIT_FACING, 315.00 )
    // Rebind to the nearby res stone
    call ConditionalTriggerExecute( gg_trg_Pick_Resurrection_Stone_03 )
    // Select Rexxar
    call SelectUnitSingle( udg_Rexxar )
    // Reset Sky
    call SetSkyModel( null )
    // Enable Transition
