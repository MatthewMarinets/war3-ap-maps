//===========================================================================
// Trigger: Wyvern to Rexxar
//===========================================================================
function Trig_Wyvern_to_Rexxar_Conditions takes nothing returns boolean
    if ( not ( udg_RexxarInWyvernForm == true ) ) then
        return false
    endif
    if ( not ( GetEnteringUnit() == udg_RexxarWyvern ) ) then
        return false
    endif
    return true
endfunction

function Trig_Wyvern_to_Rexxar_Func006001 takes nothing returns boolean
    return ( IsUnitSelected(udg_RexxarWyvern, udg_AP1_Player) == true )
endfunction

function Trig_Wyvern_to_Rexxar_Func025001 takes nothing returns boolean
    return ( udg_ExpDisabledForRexxar == false )
endfunction

function Trig_Wyvern_to_Rexxar_Func027001 takes nothing returns boolean
    return ( udg_RexxarSelected == true )
endfunction

function Trig_Wyvern_to_Rexxar_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    // Pause Hero
    call PauseUnitBJ( true, udg_RexxarWyvern )
    // Check Selection
    if ( Trig_Wyvern_to_Rexxar_Func006001() ) then
        set udg_RexxarSelected = true
    else
        set udg_RexxarSelected = false
    endif
    // Slowly Land
    call SetUnitFlyHeightBJ( udg_RexxarWyvern, 90.00, 75.00 )
    call TriggerSleepAction( 2.00 )
    call AddSpecialEffectLocBJ( GetUnitLoc(udg_RexxarWyvern), "Abilities\\Spells\\Items\\TomeOfRetraining\\TomeOfRetrainingCaster.mdl" )
    // Unhide Rexxar
    call PauseUnitBJ( false, udg_Rexxar )
    call SetUnitOwner( udg_Rexxar, udg_AP1_Player, false )
    call SetUnitPositionLocFacingBJ( udg_Rexxar, GetUnitLoc(udg_RexxarWyvern), GetUnitFacing(udg_RexxarWyvern) )
    call ShowUnitShow( udg_Rexxar )
    // Remove Rexxar Wyvern
    call RemoveUnit( udg_RexxarWyvern )
    set udg_RexxarInWyvernForm = false
    // Play Sound
    call PlaySoundOnUnitBJ( udg_SoundToHero, 100, udg_Rexxar )
    call PlaySoundOnUnitBJ( gg_snd_SpiritWalkerMorph, 100, udg_Rexxar )
    // Re-enable XP Gain
    if ( Trig_Wyvern_to_Rexxar_Func025001() ) then
        call SuspendHeroXPBJ( true, udg_Rexxar )
    else
        call DoNothing(  )
    endif
    // Check For Reselection
    if ( Trig_Wyvern_to_Rexxar_Func027001() ) then
        call SelectUnitAddForPlayer( udg_Rexxar, udg_AP1_Player )
    else
        call DoNothing(  )
    endif
    // Ping Hero
    call PingMinimapLocForForce( udg_APG1_Player, GetUnitLoc(udg_Rexxar), 1 )
    call TriggerSleepAction( 1.00 )
    // Enable Other Conversion
    call EnableTrigger( gg_trg_Rexxar_to_Wyvern )
endfunction

