    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Battleships_Destroyed_Cleanup_Func011002 )
    // Remove Hero Wyverns
    call RemoveUnit( udg_RexxarWyvern )
    call RemoveUnit( udg_RokhanWyvern )
