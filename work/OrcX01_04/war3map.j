    // Turn On Runes
    call SetDoodadAnimationRectBJ( "stand", 'D002', gg_rct_RunesToPyreIsland )
    call SetDoodadAnimationRectBJ( "stand", 'D001', gg_rct_RunesToPyreIsland )
    // Create Zeppelin
    call CreateNUnitsAtLoc( 1, 'n00A', Player(8), GetRectCenter(gg_rct_Zeppelin_To_Pyre_Island_Start), 0.00 )
    call SetUnitInvulnerable( GetLastCreatedUnit(), true )
    // Make Player Ally With Trolls
    call SetPlayerAllianceStateBJ( udg_AP1_Player, udg_P04_Trolls, bj_ALLIANCE_ALLIED )
    // Turn On Voljin Voiceover
    call EnableTrigger( gg_trg_Voljin_Voiceover )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP1_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP1_Player, GetUnitLoc(udg_Rexxar), 0 )
    // Set Bindstone
    call ConditionalTriggerExecute( gg_trg_Pick_Resurrection_Stone_02 )
