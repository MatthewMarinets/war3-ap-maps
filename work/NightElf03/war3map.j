function Trig_Load_Tyrande_Actions takes nothing returns nothing
    call RestoreUnitLocFacingAngleBJ( "Tyrande", "NightElf03", GetLastCreatedGameCacheBJ(), Player(1), GetRectCenter(gg_rct_Tyrande_Start_01), 270.00 )
    set udg_Tyrande = GetLastRestoredUnitBJ()
    call SetUnitManaPercentBJ( udg_Tyrande, 100 )
