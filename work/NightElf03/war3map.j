    call CommentString( "-----------------------Create Evil Arthas-------------------------" )
    call CreateNUnitsAtLoc( 1, 'Etyr', Player(1), GetRectCenter(gg_rct_Tyrande_Start_01), 270.00 )
    set udg_Tyrande = GetLastCreatedUnit()
    call SetHeroLevel( udg_Tyrande, 3, false )
    call SelectHeroSkill( udg_Tyrande, 'AEst' )
    call SelectHeroSkill( udg_Tyrande, 'AHfa' )
    call SelectHeroSkill( udg_Tyrande, 'AEar' )
endfunction

