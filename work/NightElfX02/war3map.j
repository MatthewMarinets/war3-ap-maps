    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P1_Naga), function Trig_Victory_Setup_Func035002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P3_Naga2), function Trig_Victory_Setup_Func036002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P9_Drakthul), function Trig_Victory_Setup_Func037002 )
