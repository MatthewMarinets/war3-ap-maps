    call UnitShareVisionBJ( true, gg_unit_nnsw_0190, udg_AP2_Player )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Naga_Cleanup_Func024001() ) then
