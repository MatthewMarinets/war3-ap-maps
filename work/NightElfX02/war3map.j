function Trig_Hermit_1_Cleanup_Actions takes nothing returns nothing
    // Restore Abilities
    call SetPlayerAbilityAvailableBJ( true, 'Ashm', udg_AP2_Player )
    // Remove Fake Maiev
    call RemoveUnit( udg_MaievCinematic )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Hermit_1_Cleanup_Func006002 )
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Hermit_1_Cleanup_Func007002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Hermit_1_Cleanup_Func013001() ) then
        call SuspendHeroXPBJ( true, udg_Maiev )
    else
        call DoNothing(  )
