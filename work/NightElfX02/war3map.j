    // Hide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Hermit_1_Setup_Func035002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP2_Player), function Trig_Hermit_1_Setup_Func037002 )
