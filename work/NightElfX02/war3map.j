//===========================================================================
// Trigger: Victory Cinematic Q
//===========================================================================
function Trig_Victory_Cinematic_Q_Func026001 takes nothing returns boolean
    return ( udg_VictorySkipped == true )
endfunction

function Trig_Victory_Cinematic_Q_Func029001 takes nothing returns boolean
    return ( udg_VictorySkipped == true )
endfunction

function Trig_Victory_Cinematic_Q_Func032001 takes nothing returns boolean
    return ( udg_VictorySkipped == true )
endfunction

function Trig_Victory_Cinematic_Q_Func035001 takes nothing returns boolean
    return ( udg_VictorySkipped == true )
endfunction

function Trig_Victory_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_Kill_Naga_Completed )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG2_Player )
    call TriggerSleepAction( 0.25 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Prep )
    call ConditionalTriggerExecute( gg_trg_Victory_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_End_01A, udg_AP2_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_End_01B, udg_AP2_Player, 8.00 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call SetSoundVolumeBJ( gg_snd_RollingThunder1, 70.00 )
    call PlaySoundBJ( gg_snd_RollingThunder1 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Victory_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TriggerSleepAction( 5.00 )
    if ( Trig_Victory_Cinematic_Q_Func026001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetSoundVolumeBJ( gg_snd_LightningBolt1, 70.00 )
    call PlaySoundBJ( gg_snd_LightningBolt1 )
    if ( Trig_Victory_Cinematic_Q_Func029001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_MaievCinematic, "TRIGSTR_173", gg_snd_S02Maiev48, "TRIGSTR_174", bj_TIMETYPE_ADD, 0.00, false )
    call WaitForSoundBJ( gg_snd_S02Maiev48, 2.00 )
    if ( Trig_Victory_Cinematic_Q_Func032001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_MaievCinematic, "move", GetRectCenter(gg_rct_Victory_Warder_02) )
    call TriggerSleepAction( 1.00 )
    if ( Trig_Victory_Cinematic_Q_Func035001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Victory_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call IssuePointOrderLocBJ( udg_VictoryArcher01, "move", GetRectCenter(gg_rct_Victory_Warder_02) )
    call TriggerSleepAction( 0.50 )
    call IssuePointOrderLocBJ( udg_VictoryArcher02, "move", GetRectCenter(gg_rct_Victory_Warder_02) )
    call TriggerSleepAction( 0.50 )
    call IssuePointOrderLocBJ( udg_VictoryArcher03, "move", GetRectCenter(gg_rct_Victory_Warder_02) )
    call TriggerSleepAction( 0.50 )
    call IssuePointOrderLocBJ( udg_VictoryArcher04, "move", GetRectCenter(gg_rct_Victory_Warder_02) )
    call TriggerSleepAction( 0.50 )
    // Remove Fake Maiev
    call RemoveUnit( udg_MaievCinematic )
    // Run Next Level
    call ConditionalTriggerExecute( gg_trg_Next_Level_Run )
endfunction

