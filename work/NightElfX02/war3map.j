function Trig_Reveal_Naga_Conditions takes nothing returns boolean
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Reveal_Naga_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    call PingMinimapLocForForce( udg_APG2_Player, GetRectCenter(gg_rct_Visibility_Naga_2), 7.00 )
    call SetCameraQuickPositionLocForPlayer( udg_AP2_Player, GetRectCenter(gg_rct_Visibility_Naga_2) )
    call CreateFogModifierRadiusLocBJ( true, udg_AP2_Player, FOG_OF_WAR_VISIBLE, GetRectCenter(gg_rct_Visibility_Naga_1), 1024.00 )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
    call CreateFogModifierRadiusLocBJ( true, udg_AP2_Player, FOG_OF_WAR_VISIBLE, GetRectCenter(gg_rct_Visibility_Naga_2), 1024.00 )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
endfunction

