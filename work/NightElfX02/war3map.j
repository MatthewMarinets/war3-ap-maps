function Trig_Hermit_1_Setup_Actions takes nothing returns nothing
    // Save Camera Position
    set udg_CameraReturnPoint = GetCameraTargetPositionLoc()
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\LordaeronFallSky\\LordaeronFallSky.mdl" )
    // Set Fog
    call SetTerrainFogExBJ( 0, 4000.00, 8000.00, 0, 0.00, 50.00, 0.00 )
    // Save Selection
    set udg_SelectedUnits = GetUnitsSelectedAll(udg_AP2_Player)
    call ClearSelection(  )
    // Revive Real Hero
    set udg_RevivalHero = udg_Maiev
    set udg_RevivalSpot = GetRectCenter(gg_rct_Hermit1_Maiev02)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_MaievReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceUnitTimed( udg_Maiev, udg_Drakthul, 0 )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Maiev )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_AP2_Player, Condition(function Trig_Hermit_1_Setup_Func023002002))
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P1_Naga, Condition(function Trig_Hermit_1_Setup_Func024001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P3_Naga2, Condition(function Trig_Hermit_1_Setup_Func025001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P4_Illidan, Condition(function Trig_Hermit_1_Setup_Func026001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(Player(PLAYER_NEUTRAL_AGGRESSIVE)), udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Hermit_1_Setup_Func028002 )
