function Trig_Draenei_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call LeaderboardDisplayBJ( false, udg_LeaderboardCounter )
    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Draenei_Cinematic_Q_Func010C() ) then
        return
    else
    endif
    call ConditionalTriggerExecute( gg_trg_Draenei_Setup )
    call TriggerSleepAction( 0.25 )
    // Units Take Position
    call CreateNUnitsAtLoc( 1, 'nchg', Player(4), GetRectCenter(gg_rct_Mid_Orc_01), 180.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinOrcGroup )
    call CreateNUnitsAtLoc( 1, 'nchg', Player(4), GetRectCenter(gg_rct_Mid_Orc_02), 180.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinOrcGroup )
    call CreateNUnitsAtLoc( 1, 'nchr', Player(4), GetRectCenter(gg_rct_Mid_Orc_03), 180.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinOrcGroup )
    call CreateNUnitsAtLoc( 1, 'nchw', Player(4), GetRectCenter(gg_rct_Mid_Orc_04), 180.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinOrcGroup )
    call ForGroupBJ( udg_MidCinOrcGroup, function Trig_Draenei_Cinematic_Q_Func026A )
    // -
    call CreateNUnitsAtLoc( 1, 'ndrs', udg_P07_DraeneiGreen, GetRectCenter(gg_rct_Mid_Draenei_02), 350.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinDraeneiGroup )
    call CreateNUnitsAtLoc( 1, 'ndrn', udg_P07_DraeneiGreen, GetRectCenter(gg_rct_Mid_Draenei_01), 350.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinDraeneiGroup )
    call CreateNUnitsAtLoc( 1, 'ndrn', udg_P07_DraeneiGreen, GetRectCenter(gg_rct_Mid_Draenei_04), 350.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinDraeneiGroup )
    call CreateNUnitsAtLoc( 1, 'ndrh', udg_P07_DraeneiGreen, GetRectCenter(gg_rct_Mid_Draenei_03), 350.00 )
    call RemoveGuardPosition( GetLastCreatedUnit() )
    call GroupAddUnitSimple( GetLastCreatedUnit(), udg_MidCinDraeneiGroup )
    call ForGroupBJ( udg_MidCinDraeneiGroup, function Trig_Draenei_Cinematic_Q_Func040A )
    // Attack
    call ForGroupBJ( udg_MidCinOrcGroup, function Trig_Draenei_Cinematic_Q_Func042A )
    // -
    call CameraSetupApplyForPlayer( true, gg_cam_Mid_01_Draenei_01, udg_AP01_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Mid_01_Draenei_02, udg_AP01_Player, ( GetSoundDurationBJ(gg_snd_A06Kael23) + 10.00 ) )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 0.50 )
    call EnableTrigger( gg_trg_Draenei_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TriggerSleepAction( 4.00 )
    if ( Trig_Draenei_Cinematic_Q_Func054001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Kael, "TRIGSTR_179", gg_snd_A06Kael23, "TRIGSTR_180", bj_TIMETYPE_ADD, 0, false )
    call ForGroupBJ( udg_MidCinDraeneiGroup, function Trig_Draenei_Cinematic_Q_Func056A )
    call WaitForSoundBJ( gg_snd_A06Kael23, 0 )
    if ( Trig_Draenei_Cinematic_Q_Func058001() ) then
        return
    else
        call DoNothing(  )
    endif
    call VolumeGroupSetVolumeBJ( SOUND_VOLUMEGROUP_COMBAT, 80.00 )
    call VolumeGroupSetVolumeBJ( SOUND_VOLUMEGROUP_SPELLS, 80.00 )
    call TriggerSleepAction( 2.00 )
    if ( Trig_Draenei_Cinematic_Q_Func062001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Draenei_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Draenei_Cleanup )
    call CinematicModeBJ( false, udg_APG1_Player )
    call LeaderboardDisplayBJ( true, udg_LeaderboardCounter )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    set udg_InCinematic = false
    // Quest Update
    call ConditionalTriggerExecute( gg_trg_Quest_Draenei_Discover )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

