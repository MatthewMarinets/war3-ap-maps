function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\Outland_Sky\\Outland_Sky.mdl" )
    // Clear Selection
    call ClearSelection(  )
    // Revive Hero
    set udg_RevivalHero = udg_Illidan
    set udg_RevivalSpot = GetRectCenter(gg_rct_Victory_Illidian_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_IllidanReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceLocTimed( udg_Illidan, GetRectCenter(gg_rct_Victory_Gate), 0 )
    // Revive Hero
    set udg_RevivalHero = udg_Kael
    set udg_RevivalSpot = GetRectCenter(gg_rct_Victory_Kael_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_KaelReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceLocTimed( udg_Kael, GetRectCenter(gg_rct_Victory_Gate), 0 )
    // Revive Hero
    set udg_RevivalHero = udg_Vashj
    set udg_RevivalSpot = GetRectCenter(gg_rct_Victory_Vashj_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_VashjReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceLocTimed( udg_Vashj, GetRectCenter(gg_rct_Victory_Gate), 0 )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Kael )
    call SuspendHeroXPBJ( false, udg_Vashj )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Illidan )
    call PauseUnitBJ( false, udg_Kael )
    call PauseUnitBJ( false, udg_Vashj )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_AP01_Player, Condition(function Trig_Victory_Setup_Func034002002))
    call GroupRemoveUnitSimple( udg_Illidan, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Kael, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Vashj, udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P02_BlueGate), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P03_TealGate), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P04_PurpleChaosOrc), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P05_YellowChaosOrc), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P06_OrangeChaosOrc), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P07_DraeneiGreen), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P08_PinkGate), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P09_GrayGate), udg_HiddenUnits )
    call GroupRemoveUnitSimple( gg_unit_n000_0005, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func047002 )
    // Hide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func049002 )
    // Disable Abilities
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', udg_AP01_Player )
    call SetPlayerAbilityAvailableBJ( false, 'AIba', udg_AP01_Player )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP01_Player), function Trig_Victory_Setup_Func054002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P02_BlueGate), function Trig_Victory_Setup_Func055002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P04_PurpleChaosOrc), function Trig_Victory_Setup_Func056002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P05_YellowChaosOrc), function Trig_Victory_Setup_Func057002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P07_DraeneiGreen), function Trig_Victory_Setup_Func058002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P08_PinkGate), function Trig_Victory_Setup_Func059002 )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\HeroicVictory.mp3" )
endfunction

