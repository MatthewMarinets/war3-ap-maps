function Trig_Victory_Door_Open_Conditions takes nothing returns boolean
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetEnteringUnit()) == udg_AP1_Player ) ) then
        return false
    endif
    if ( not ( UnitHasItemOfTypeBJ(GetEnteringUnit(), 'kybl') == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Victory_Door_Open_Func008Func001C takes nothing returns boolean
    if ( not ( GetDestructableTypeId(GetEnumDestructable()) == 'DTrx' ) ) then
        return false
    endif
    return true
endfunction

function Trig_Victory_Door_Open_Func008A takes nothing returns nothing
    if ( Trig_Victory_Door_Open_Func008Func001C() ) then
        call ChangeElevatorHeight( GetEnumDestructable(), 2 )
        call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_NORTH, GetEnumDestructable() )
        call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_SOUTH, GetEnumDestructable() )
    else
    endif
endfunction

function Trig_Victory_Door_Open_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    call DisableTrigger( gg_trg_Old_KingdomDoorMessage )
    call RemoveItem( GetItemOfTypeFromUnitBJ(GetEnteringUnit(), 'kybl') )
    set udg_GameOver = true
    call SetUnitInvulnerable( udg_Arthas, true )
    call SetUnitInvulnerable( udg_Anubarak, true )
    call EnumDestructablesInRectAll( gg_rct_ElevatorVictory, function Trig_Victory_Door_Open_Func008A )
    call PlaySoundAtPointBJ( gg_snd_Elevator, 100, GetRectCenter(gg_rct_ElevatorVictory), 0 )
    call TriggerSleepAction( 2.00 )
    call SetDestAnimationSpeedPercent( gg_dest_ATg1_0118, 20.00 )
    call ModifyGateBJ( bj_GATEOPERATION_OPEN, gg_dest_ATg1_0118 )
    call PlaySoundAtPointBJ( gg_snd_LargeCityGateOpen1, 100, GetRectCenter(gg_rct_VictoryGateOpenSound), 0 )
    call QueuedTriggerAddBJ( gg_trg_Victory_Cinematic_Q, true )
endfunction

