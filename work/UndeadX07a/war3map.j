function Trig_Room04_Spike_Trap_Plate_Actions takes nothing returns nothing
    set udg_SpikeNotReady = true
    // Activate Plate
    call KillDestructable( gg_dest_DTfx_0097 )
    call PlaySoundAtPointBJ( gg_snd_Switch, 100, GetDestructableLoc(gg_dest_DTfx_0097), 0 )
    call TriggerSleepAction( 0.80 )
    // Plate Effect
    call EnumDestructablesInRectAll( gg_rct_SpikeArea, function Trig_Room04_Spike_Trap_Plate_Func008A )
    set udg_UnitsSpiked = 0
    call ForGroupBJ( GetUnitsInRectAll(gg_rct_SpikeArea), function Trig_Room04_Spike_Trap_Plate_Func010A )
    call TriggerSleepAction( 1.00 )
    set bj_forLoopBIndex = 1
    set bj_forLoopBIndexEnd = udg_UnitsSpiked
    loop
