function Trig_Victory_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_The_Old_Kingdom_Completed )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Prep )
    call ConditionalTriggerExecute( gg_trg_Victory_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory01, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory02, udg_AP1_Player, 12.00 )
    call IssuePointOrderLocBJ( udg_Arthas, "move", GetRectCenter(gg_rct_VictoryArthas02) )
    call PlaySoundOnUnitBJ( gg_snd_HorseLoop3, 70.00, udg_Arthas )
    call IssuePointOrderLocBJ( udg_FakeAnubarak, "move", GetRectCenter(gg_rct_VictoryAnub02) )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Victory_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Arthas, "TRIGSTR_199", gg_snd_L07AArthas50, "TRIGSTR_200", bj_TIMETYPE_ADD, 0, true )
    call WaitForSoundBJ( gg_snd_L07AArthas50, 0 )
    if ( Trig_Victory_Cinematic_Q_Func028001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_FakeAnubarak, "TRIGSTR_201", gg_snd_L07AAnubarak51, "TRIGSTR_202", bj_TIMETYPE_ADD, 0, true )
    call WaitForSoundBJ( gg_snd_L07AAnubarak51, 0 )
    if ( Trig_Victory_Cinematic_Q_Func031001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_FakeAnubarak, "TRIGSTR_203", gg_snd_L07AAnubarak52, "TRIGSTR_204", bj_TIMETYPE_ADD, 0, true )
    call WaitForSoundBJ( gg_snd_L07AAnubarak52, 0 )
    if ( Trig_Victory_Cinematic_Q_Func034001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetUnitLookAt( udg_Arthas, "bone_head", udg_Anubarak, 0, 0, 90 )
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Arthas, "TRIGSTR_205", gg_snd_L07AArthas53, "TRIGSTR_206", bj_TIMETYPE_ADD, 0, true )
    call WaitForSoundBJ( gg_snd_L07AArthas53, 0 )
    call IssuePointOrderLocBJ( udg_FakeAnubarak, "move", GetRectCenter(gg_rct_VictoryAnub03) )
    if ( Trig_Victory_Cinematic_Q_Func039001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Victory_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 2.00 )
    call RemoveUnit( udg_FakeAnubarak )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Run )
endfunction

