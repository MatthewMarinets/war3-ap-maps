    // NPCs
    call SetUnitInvulnerable( udg_KarginsForge, true )
    set udg_Baelgun = gg_unit_H000_0023
    call SetHeroLevelBJ( udg_Baelgun, 10, false )
    call SelectHeroSkill( udg_Baelgun, 'AHtb' )
    call SelectHeroSkill( udg_Baelgun, 'AHtb' )
    call SelectHeroSkill( udg_Baelgun, 'AHtb' )
    call SelectHeroSkill( udg_Baelgun, 'AHtc' )
    call SelectHeroSkill( udg_Baelgun, 'AHtc' )
    call SelectHeroSkill( udg_Baelgun, 'AHtc' )
    call SelectHeroSkill( udg_Baelgun, 'AHbh' )
    call SelectHeroSkill( udg_Baelgun, 'AHbh' )
    call SelectHeroSkill( udg_Baelgun, 'AHbh' )
    call SelectHeroSkill( udg_Baelgun, 'AHav' )
    call PauseUnitBJ( true, gg_unit_hmtm_0003 )
    call PauseUnitBJ( true, gg_unit_hmtt_0090 )
    // ExposedGoldCoinsInvulnerable
    call SetItemInvulnerableBJ( RandomItemInRectSimpleBJ(gg_rct_GoldCoin04_Exposed), true )
    call SetItemInvulnerableBJ( RandomItemInRectSimpleBJ(gg_rct_GoldCoin05_Exposed), true )
    // Unit Groups
    set udg_BaelgunGroup = GetUnitsInRectOfPlayer(gg_rct_Baelgun_Group, udg_AP2_Dwarves)
    // Creeps
    call SetUnitColor( gg_unit_ubdd_0089, PLAYER_COLOR_PURPLE )
    // Destructible
    call SetDestructableInvulnerableBJ( gg_dest_ATg1_0118, true )
    call SetDestructableInvulnerableBJ( gg_dest_DTg1_0100, true )
    call ShowDestructableBJ( false, gg_dest_DTlv_0082 )
    call SetDestructableInvulnerableBJ( gg_dest_DTlv_0082, true )
    // Make vital units indestructible
    call SetUnitInvulnerable( gg_unit_hmtm_0003, true )
    call SetUnitInvulnerable( gg_unit_hrif_0015, true )
    call SetUnitInvulnerable( gg_unit_hrif_0012, true )
    call SetUnitInvulnerable( gg_unit_hrif_0034, true )
    call PauseUnitBJ( true, gg_unit_hmtm_0003 )
    call PauseUnitBJ( true, gg_unit_hrif_0015 )
    call PauseUnitBJ( true, gg_unit_hrif_0012 )
    call PauseUnitBJ( true, gg_unit_hrif_0034 )
