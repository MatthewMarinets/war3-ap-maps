    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Death_Revenant_Dies_Func010002002001 takes nothing returns boolean
    return ( GetOwningPlayer(GetFilterUnit()) == Player(PLAYER_NEUTRAL_AGGRESSIVE) )
endfunction

function Trig_Death_Revenant_Dies_Func010002002002 takes nothing returns boolean
    return ( IsUnitAliveBJ(GetFilterUnit()) == true )
endfunction

function Trig_Death_Revenant_Dies_Func010002002 takes nothing returns boolean
    return GetBooleanAnd( Trig_Death_Revenant_Dies_Func010002002001(), Trig_Death_Revenant_Dies_Func010002002002() )
endfunction

function Trig_Death_Revenant_Dies_Func011002 takes nothing returns nothing
    call KillUnit( GetEnumUnit() )
endfunction

function Trig_Death_Revenant_Dies_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    set udg_RevenantPosition = GetUnitLoc(GetDyingUnit())
    set udg_RevenantFacing = GetUnitFacing(GetDyingUnit())
    set udg_ForestCleansed = true
    call CommentString( "Death Effect" )
    call AddSpecialEffectLocBJ( udg_RevenantPosition, "Abilities\\Spells\\NightElf\\Starfall\\StarfallCaster.mdl" )
    set udg_Effect01 = GetLastCreatedEffectBJ()
    call TriggerSleepAction( 0.25 )
    set udg_CreepsToCleanse = GetUnitsInRectMatching(gg_rct_Fog, Condition(function Trig_Death_Revenant_Dies_Func010002002))
    call ForGroup( udg_CreepsToCleanse, function Trig_Death_Revenant_Dies_Func011002 )
    call CreateNUnitsAtLoc( 1, 'Udre', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_RevenantPosition, udg_RevenantFacing )
    call KillUnit( GetLastCreatedUnit() )
    call SetSoundPositionLocBJ( gg_snd_HeroDreadLordDeath, udg_RevenantPosition, 0 )
    call PlaySoundBJ( gg_snd_HeroDreadLordDeath )
    call TriggerSleepAction( 0.50 )
    call AddSpecialEffectLocBJ( udg_RevenantPosition, "Abilities\\Spells\\Human\\Resurrection\\ResurrectionTarget.mdl" )
    set udg_Effect02 = GetLastCreatedEffectBJ()
    call TriggerSleepAction( 1.50 )
    call AddSpecialEffectLocBJ( udg_RevenantPosition, "Abilities\\Spells\\NightElf\\Tranquility\\Tranquility.mdl" )
    set udg_Effect03 = GetLastCreatedEffectBJ()
    call CommentString( "Remove blight and turn off doodads" )
    call SetBlightRectBJ( false, udg_AP02_Player, gg_rct_Fog )
    call SetDoodadAnimationRectBJ( "death", 'YOtf', gg_rct_Fog )
    call SetDoodadAnimationRectBJ( "death", 'LObr', gg_rct_Fog )
    call TriggerSleepAction( 0.20 )
    call CommentString( "Turn off Special Effects" )
    call DestroyEffectBJ( udg_Effect01 )
    call DestroyEffectBJ( udg_Effect02 )
    call TriggerSleepAction( 0.20 )
    call CommentString( "Reveal the forest" )
    call ConditionalTriggerExecute( gg_trg_Forest_Fawn )
    call ConditionalTriggerExecute( gg_trg_Forest_Vision )
    call TriggerSleepAction( 0.20 )
    call ConditionalTriggerExecute( gg_trg_Forest_Weather_Change )
    call TriggerSleepAction( 0.20 )
    call CommentString( "Clean the trees" )
    call ConditionalTriggerExecute( gg_trg_Regrow_The_Forest )
    call TriggerSleepAction( 0.50 )
    call DestroyEffectBJ( udg_Effect03 )
    call QueuedTriggerAddBJ( gg_trg_Death_Revenant_Dies_Q, true )
endfunction

