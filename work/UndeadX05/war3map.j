function Trig_Init_02_Units_Actions takes nothing returns nothing
    // Heroes
    call ConditionalTriggerExecute( gg_trg_Load_Heroes )
    call SetPlayerHandicapXPBJ( udg_AP4_Player, 40.00 )
    // NPCs
    set udg_Detheroc = gg_unit_Udth_0124
    call SetHeroLevelBJ( udg_Detheroc, 8, false )
    call SelectHeroSkill( udg_Detheroc, 'AUcs' )
    call SelectHeroSkill( udg_Detheroc, 'AUcs' )
    call SelectHeroSkill( udg_Detheroc, 'AUsl' )
    call SelectHeroSkill( udg_Detheroc, 'AUsl' )
    call SelectHeroSkill( udg_Detheroc, 'AEsh' )
    call SelectHeroSkill( udg_Detheroc, 'AEsh' )
    call SelectHeroSkill( udg_Detheroc, 'AUdd' )
    call UnitAddItemByIdSwapped( 'I003', udg_Detheroc )
    call UnitAddItemByIdSwapped( 'I000', udg_Detheroc )
    call UnitAddItemByIdSwapped( 'I004', udg_Detheroc )
    call UnitAddItemByIdSwapped( 'phlt', udg_Detheroc )
    call UnitAddItemByIdSwapped( 'phlt', udg_Detheroc )
    call UnitAddItemByIdSwapped( 'phlt', udg_Detheroc )
    set udg_Garithos = gg_unit_Hlgr_0123
    call SetUnitColor( udg_Garithos, PLAYER_COLOR_LIGHT_GRAY )
    call UnitAddItemByIdSwapped( 'I001', udg_Garithos )
    call UnitAddItemByIdSwapped( 'I005', udg_Garithos )
    call UnitAddItemByIdSwapped( 'I002', udg_Garithos )
    call UnitAddItemByIdSwapped( 'phlt', udg_Garithos )
    call UnitAddItemByIdSwapped( 'phlt', udg_Garithos )
    call UnitAddItemByIdSwapped( 'phlt', udg_Garithos )
    call SuspendHeroXPBJ( false, udg_Garithos )
    call SuspendHeroXPBJ( false, udg_Detheroc )
    // Creeps
    // Initialize Timer
    call StartTimerBJ( udg_RandomLineTimer, false, 1.00 )
    // Turn off Sleeping for Player and Non-AI units
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP4_Player), function Trig_Init_02_Units_Func034A )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_AP4_Player )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P5_Garithos_NonAI), function Trig_Init_02_Units_Func036A )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P5_Garithos_NonAI )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P8_Detheroc_NonAI), function Trig_Init_02_Units_Func038A )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P8_Detheroc_NonAI )
    // Setting Up Buildings I Care About Groups
    call GroupClear( udg_BuildingsICareAboutHuman )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P2_Garithos, Condition(function Trig_Init_02_Units_Func042001002)), function Trig_Init_02_Units_Func042A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P3_Garithos_Satellite, Condition(function Trig_Init_02_Units_Func043001002)), function Trig_Init_02_Units_Func043A )
    call ForGroupBJ( udg_BuildingsICareAboutHuman, function Trig_Init_02_Units_Func044A )
    call GroupClear( udg_BuildingsICareAboutUndead )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P11_Detheroc_Satellite, Condition(function Trig_Init_02_Units_Func046001002)), function Trig_Init_02_Units_Func046A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P7_Detheroc, Condition(function Trig_Init_02_Units_Func047001002)), function Trig_Init_02_Units_Func047A )
    call ForGroupBJ( udg_BuildingsICareAboutUndead, function Trig_Init_02_Units_Func048A )
    // Initialize Marketplace
    call AddItemToStockBJ( 'gcel', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'pghe', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'rde2', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'fgsk', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'sreg', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'rag1', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'rwiz', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'sror', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'penr', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'hslv', gg_unit_nmrk_0119, 1, 1 )
    call AddItemToStockBJ( 'pnvu', gg_unit_nmrk_0119, 1, 1 )
    // Activate Web on Player Units
    call IssueImmediateOrderBJ( gg_unit_ucry_0235, "webon" )
    call IssueImmediateOrderBJ( gg_unit_ucry_0236, "webon" )
    // Deactivate Curse on Player Units
    call ForGroupBJ( GetUnitsOfPlayerAndTypeId(udg_AP4_Player, 'uban'), function Trig_Init_02_Units_Func065A )
    // Disable Destroyer Form
    call SetPlayerUnitAvailableBJ( 'ubsp', false, udg_AP4_Player )
    // Deactivate Alarm Generation
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P2_Garithos, Condition(function Trig_Init_02_Units_Func069001002)), function Trig_Init_02_Units_Func069A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P3_Garithos_Satellite, Condition(function Trig_Init_02_Units_Func070001002)), function Trig_Init_02_Units_Func070A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P7_Detheroc, Condition(function Trig_Init_02_Units_Func071001002)), function Trig_Init_02_Units_Func071A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P11_Detheroc_Satellite, Condition(function Trig_Init_02_Units_Func072001002)), function Trig_Init_02_Units_Func072A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P5_Garithos_NonAI, Condition(function Trig_Init_02_Units_Func073001002)), function Trig_Init_02_Units_Func073A )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P8_Detheroc_NonAI, Condition(function Trig_Init_02_Units_Func074001002)), function Trig_Init_02_Units_Func074A )
endfunction

