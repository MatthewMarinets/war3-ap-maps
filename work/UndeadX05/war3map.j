function Trig_Timer_Expires_Actions takes nothing returns nothing
    set udg_SleepyTime = false
    call TimerDialogDisplayForPlayerBJ( false, GetLastCreatedTimerDialogBJ(), udg_AP4_Player )
    call LeaderboardSetLabelBJ( udg_SleepLeaderboard, "TRIGSTR_282" )
    call StartTimerBJ( udg_HideLeaderboardTimer, false, 15.00 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P6_Garithos_SleepSatellite), function Trig_Timer_Expires_Func006A )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P10_Garithos_SleepMain), function Trig_Timer_Expires_Func007A )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P9_Detheroc_SleepSatellite), function Trig_Timer_Expires_Func008A )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P12_Detheroc_SleepMain), function Trig_Timer_Expires_Func009A )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P2_Garithos )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P3_Garithos_Satellite )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P5_Garithos_NonAI )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P6_Garithos_SleepSatellite )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P7_Detheroc )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P8_Detheroc_NonAI )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P9_Detheroc_SleepSatellite )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P10_Garithos_SleepMain )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P11_Detheroc_Satellite )
    call SetPlayerAbilityAvailableBJ( false, 'A000', udg_P12_Detheroc_SleepMain )
    // Take Away Sight
    call SetPlayerAllianceBJ( udg_P2_Garithos, ALLIANCE_SHARED_VISION, false, udg_AP4_Player )
    call SetPlayerAllianceBJ( udg_P3_Garithos_Satellite, ALLIANCE_SHARED_VISION, false, udg_AP4_Player )
    call SetPlayerAllianceBJ( udg_P5_Garithos_NonAI, ALLIANCE_SHARED_VISION, false, udg_AP4_Player )
    call SetPlayerAllianceBJ( udg_P7_Detheroc, ALLIANCE_SHARED_VISION, false, udg_AP4_Player )
    call SetPlayerAllianceBJ( udg_P8_Detheroc_NonAI, ALLIANCE_SHARED_VISION, false, udg_AP4_Player )
    call SetPlayerAllianceBJ( udg_P11_Detheroc_Satellite, ALLIANCE_SHARED_VISION, false, udg_AP4_Player )
    // START AIs
    call ConditionalTriggerExecute( gg_trg_Start_AI )
    call QueuedTriggerAddBJ( gg_trg_Time_is_Up_Q, true )
    call QueuedTriggerAddBJ( gg_trg_Quest_Strike_Complete_Q, true )
    call QueuedTriggerAddBJ( gg_trg_Hint_Town_Portal_Scroll_Q, true )
endfunction

