//===========================================================================
function Trig_Random_Footman_Attack_Lines_Conditions takes nothing returns boolean
    if ( not ( udg_InCinematic == false ) ) then
        return false
    endif
    if ( not ( GetUnitTypeId(GetAttacker()) == 'hfoo' ) ) then
        return false
    endif
    if ( not ( IsUnitInGroup(GetAttacker(), GetUnitsOfPlayerAll(udg_AP4_Player)) == false ) ) then
        return false
    endif
    if ( not ( TimerGetRemaining(udg_RandomLineTimer) == 0.00 ) ) then
        return false
    endif
    if ( not ( IsTriggerEnabled(GetTriggeringTrigger()) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Random_Footman_Attack_Lines_Func006Func001Func001C takes nothing returns boolean
    if ( not ( udg_RandomNumber <= 3.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Random_Footman_Attack_Lines_Func006Func001C takes nothing returns boolean
    if ( not ( udg_RandomNumber <= 2.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Random_Footman_Attack_Lines_Func006C takes nothing returns boolean
    if ( not ( udg_RandomNumber <= 1.00 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Random_Footman_Attack_Lines_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    set udg_RandomNumber = GetRandomReal(0, 4.00)
    set udg_Speaker = GetAttacker()
    call StartTimerBJ( udg_RandomLineTimer, false, 15.00 )
    if ( Trig_Random_Footman_Attack_Lines_Func006C() ) then
        call QueuedTriggerAddBJ( gg_trg_Footman_AttackA_Q, true )
    else
        if ( Trig_Random_Footman_Attack_Lines_Func006Func001C() ) then
            call QueuedTriggerAddBJ( gg_trg_Footman_AttackB_Q, true )
        else
            if ( Trig_Random_Footman_Attack_Lines_Func006Func001Func001C() ) then
                call QueuedTriggerAddBJ( gg_trg_Footman_AttackC_Q, true )
            else
                call QueuedTriggerAddBJ( gg_trg_Footman_AttackD_Q, true )
            endif
        endif
    endif
    call TriggerSleepAction( 60.00 )
    call EnableTrigger( GetTriggeringTrigger() )
endfunction

