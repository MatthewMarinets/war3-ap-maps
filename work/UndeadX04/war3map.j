function Trig_Dragon_Cleanup_Actions takes nothing returns nothing
    call KillDestructable( gg_dest_ITcr_0501 )
    call KillDestructable( gg_dest_ITcr_0505 )
    call KillDestructable( gg_dest_ITcr_0509 )
    call KillDestructable( gg_dest_ITcr_0507 )
    call KillDestructable( gg_dest_ITcr_0508 )
    call SetPlayerAbilityAvailableBJ( true, 'AUau', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( true, 'AUcb', udg_AP1_Player )
    call RemoveUnit( udg_CinematicAnubarak )
    call RemoveUnit( udg_CinematicArthas )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Dragon_Cleanup_Func011002 )
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Dragon_Cleanup_Func012002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Give Units to Player
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Dragon_Cleanup_Func019001() ) then
        call SuspendHeroXPBJ( true, udg_Arthas )
    else
        call DoNothing(  )
    endif
    if ( Trig_Dragon_Cleanup_Func020001() ) then
        call SuspendHeroXPBJ( true, udg_Anubarak )
    else
        call DoNothing(  )
    endif
    // Return Heroes
    call SetUnitPositionLoc( udg_Arthas, udg_ArthasReturnSpot )
    call SetUnitPositionLoc( udg_Anubarak, udg_AnubarakReturnSpot )
    // Restore Selection
    call SelectGroupBJ( udg_SelectedUnits )
    // Reset Sky/Fog
    call ResetTerrainFogBJ(  )
    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP1_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP1_Player, udg_CameraReturnPoint, 0 )
    if ( Trig_Dragon_Cleanup_Func033C() ) then
        call CreateNUnitsAtLoc( 1, 'ubdd', udg_AP1_Player, udg_SapphironDyingSpot, 200.00 )
        set udg_SapphironSaved = true
        set udg_Sapphiron_Undead = GetLastCreatedUnit()
        call SetPlayerTechResearchedSwap( 'Rufb', 1, udg_AP1_Player )
        call RemoveUnit( udg_Sapphiron )
    else
        call DoNothing(  )
    endif
endfunction

