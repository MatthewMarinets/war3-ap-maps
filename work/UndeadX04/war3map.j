    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P10_Kael, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P3_Beetle, udg_P5_Kael, bj_ALLIANCE_UNALLIED )
    call SetPlayerAllianceStateBJ( udg_P3_Beetle, udg_P6_Naga, bj_ALLIANCE_UNALLIED )
