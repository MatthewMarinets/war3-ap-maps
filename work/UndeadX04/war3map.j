function Trig_Victory_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Quest update
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Prep )
    call ConditionalTriggerExecute( gg_trg_Victory_Setup )
    call SetUnitFacingToFaceLocTimed( udg_CinematicAnubarak, GetRectCenter(gg_rct_VictoryEveryoneGoTo), 0 )
    call SetUnitFacingToFaceLocTimed( udg_CinematicArthas, GetRectCenter(gg_rct_VictoryEveryoneGoTo), 0 )
    call SetUnitFacingToFaceLocTimed( udg_EndCryptF01, GetRectCenter(gg_rct_VictoryEveryoneGoTo), 0 )
    call SetUnitFacingToFaceLocTimed( udg_EndCryptF02, GetRectCenter(gg_rct_VictoryEveryoneGoTo), 0 )
    call SetUnitFacingToFaceLocTimed( udg_GhoulEnd01, GetRectCenter(gg_rct_VictoryEveryoneGoTo), 0 )
    call SetUnitFacingToFaceLocTimed( udg_GhoulEnd02, GetRectCenter(gg_rct_VictoryEveryoneGoTo), 0 )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_01, udg_AP1_Player, 0 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Victory_Skipped )
    call SetUnitFacingToFaceUnitTimed( udg_CinematicArthas, udg_CinematicAnubarak, 0.30 )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_CinematicArthas, "TRIGSTR_282", gg_snd_L04Arthas60, "TRIGSTR_283", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_L04Arthas60, 0 )
    if ( Trig_Victory_Cinematic_Q_Func029001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_CinematicAnubarak, "TRIGSTR_284", gg_snd_L04Anubarak61, "TRIGSTR_285", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_L04Anubarak61, 0 )
    if ( Trig_Victory_Cinematic_Q_Func032001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_CinematicArthas, "TRIGSTR_286", gg_snd_L04Arthas62, "TRIGSTR_287", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_L04Arthas62, 0 )
    if ( Trig_Victory_Cinematic_Q_Func035001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_CinematicArthas, "move", GetRectCenter(gg_rct_VictoryEveryoneGoTo) )
    call PlaySoundOnUnitBJ( gg_snd_HorseLoop1, 70.00, udg_CinematicArthas )
    call IssuePointOrderLocBJ( udg_GhoulEnd01, "move", GetRectCenter(gg_rct_VictoryEveryoneGoTo) )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Victory_Cinematic_Q_Func040001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_GhoulEnd02, "move", GetRectCenter(gg_rct_VictoryEveryoneGoTo) )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Victory_Cinematic_Q_Func043001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_CinematicAnubarak, "move", GetRectCenter(gg_rct_VictoryEveryoneGoTo) )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Victory_Cinematic_Q_Func046001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( udg_EndCryptF01, "move", GetRectCenter(gg_rct_VictoryEveryoneGoTo) )
    call IssuePointOrderLocBJ( udg_EndCryptF02, "move", GetRectCenter(gg_rct_VictoryEveryoneGoTo) )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Victory_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 2.00 )
    call ResetTerrainFogBJ(  )
    call RemoveUnit( udg_CinematicAnubarak )
    call RemoveUnit( udg_CinematicArthas )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Run )
endfunction

