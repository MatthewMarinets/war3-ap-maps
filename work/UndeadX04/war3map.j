function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky/Fog
    call SetSkyModel( "Environment\\Sky\\LordaeronSummerSky\\LordaeronSummerSky.mdl" )
    call SetTerrainFogExBJ( 0, 200.00, 5000.00, 0, 100, 100, 100 )
    // Clear Selection
    call ClearSelection(  )
    // Revive Hero
    set udg_RevivalHero = udg_Arthas
    set udg_RevivalSpot = GetRectCenter(gg_rct_Victory_Arthas_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_ArthasReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceUnitTimed( udg_Arthas, udg_Anubarak, 0 )
    call CreateNUnitsAtLoc( 1, 'Uear', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Arthas_Start), bj_UNIT_FACING )
    set udg_CinematicArthas = GetLastCreatedUnit()
    // Revive Hero
    set udg_RevivalHero = udg_Anubarak
    set udg_RevivalSpot = GetRectCenter(gg_rct_Victory_Anubarak_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_AnubarakReturnSpot = udg_RevivalReturnSpot
    call CreateNUnitsAtLoc( 1, 'Uanb', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Anubarak_Start), bj_UNIT_FACING )
    set udg_CinematicAnubarak = GetLastCreatedUnit()
    call SelectHeroSkill( udg_CinematicAnubarak, 'AUts' )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_CinematicArthas )
    call SuspendHeroXPBJ( false, udg_CinematicAnubarak )
    call SetPlayerAbilityAvailableBJ( false, 'AUau', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( false, 'AUcb', udg_AP1_Player )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_CinematicArthas )
    call PauseUnitBJ( false, udg_CinematicAnubarak )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_AP1_Player, Condition(function Trig_Victory_Setup_Func034002002))
    call GroupRemoveUnitSimple( udg_CinematicArthas, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_CinematicAnubarak, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func037002 )
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func038002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP1_Player), function Trig_Victory_Setup_Func040002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P1_Kael), function Trig_Victory_Setup_Func041002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P2_Kael), function Trig_Victory_Setup_Func042002 )
    // Create New Units
    call CreateNUnitsAtLoc( 1, 'ugho', udg_AP1_Player, GetRectCenter(gg_rct_Ghoul01), bj_UNIT_FACING )
    set udg_GhoulEnd01 = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'ugho', udg_AP1_Player, GetRectCenter(gg_rct_Ghoul02), bj_UNIT_FACING )
    set udg_GhoulEnd02 = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'ucry', udg_AP1_Player, GetRectCenter(gg_rct_CryptFiend01), bj_UNIT_FACING )
    set udg_EndCryptF01 = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'ucry', udg_AP1_Player, GetRectCenter(gg_rct_CryptFiend02), bj_UNIT_FACING )
    set udg_EndCryptF02 = GetLastCreatedUnit()
    if ( Trig_Victory_Setup_Func052C() ) then
        call SetUnitPositionLoc( udg_Sapphiron, GetRectCenter(gg_rct_Extra_Unit) )
    else
        call CreateNUnitsAtLoc( 1, 'umtw', udg_AP1_Player, GetRectCenter(gg_rct_Extra_Unit), bj_UNIT_FACING )
    endif
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\DarkVictory.mp3" )
endfunction

