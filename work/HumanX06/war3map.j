//===========================================================================
function Trig_BridgePlate_Left_Conditions takes nothing returns boolean
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    if ( not ( GetOwningPlayer(GetEnteringUnit()) == udg_AP1_Player ) ) then
        return false
    endif
    return true
endfunction

function Trig_BridgePlate_Left_Func019C takes nothing returns boolean
    if ( not ( udg_BridgeOpenRight == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_BridgePlate_Left_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    // Kill Plate
    call KillDestructable( udg_BridgePlateLeft )
    call PlaySoundAtPointBJ( gg_snd_Switch, 100, GetRectCenter(gg_rct_BridgePlateLeft), 0 )
    // Turn off Magical Rune Doodads
    call SetDoodadAnimationRectBJ( "death", 'XOmr', gg_rct_BridgeRunesLeft )
    call TriggerSleepAction( 0.50 )
    // Effect
    call ChangeElevatorHeight( udg_ElevatorLeftA, 3 )
    call ChangeElevatorHeight( udg_ElevatorLeftB, 3 )
    call ChangeElevatorHeight( udg_ElevatorLeftC, 3 )
    call PlaySoundBJ( udg_ElevatorSound )
    call TriggerSleepAction( 1.30 )
    call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_WEST, udg_ElevatorLeftA )
    call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_WEST, udg_ElevatorLeftB )
    call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_WEST, udg_ElevatorLeftC )
    // Adjust Center Divider
    set udg_BridgeOpenLeft = true
    if ( Trig_BridgePlate_Left_Func019C() ) then
        call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_EAST, udg_ElevatorLeftA )
        call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_EAST, udg_ElevatorLeftB )
        call ChangeElevatorWalls( true, bj_ELEVATOR_WALL_TYPE_EAST, udg_ElevatorLeftC )
    else
        call ChangeElevatorWalls( false, bj_ELEVATOR_WALL_TYPE_EAST, udg_ElevatorLeftA )
        call ChangeElevatorWalls( false, bj_ELEVATOR_WALL_TYPE_EAST, udg_ElevatorLeftB )
        call ChangeElevatorWalls( false, bj_ELEVATOR_WALL_TYPE_EAST, udg_ElevatorLeftC )
    endif
endfunction

