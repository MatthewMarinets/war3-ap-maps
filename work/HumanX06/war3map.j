    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_OpenGate_Setup_Func019002 )
    // Disable Abilities
    call SetPlayerAbilityAvailableBJ( false, 'Ahea', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( false, 'ACvp', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( false, 'AIba', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( false, 'ACba', udg_AP1_Player )
