    call SetSkyModel( "Environment\\Sky\\Outland_Sky\\Outland_Sky.mdl" )
    // Save Selection
    set udg_SelectedUnits = GetUnitsSelectedAll(udg_AP1_Player)
    call ClearSelection(  )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Akama )
    call SuspendHeroXPBJ( false, udg_Illidan )
    call SuspendHeroXPBJ( false, udg_Vashj )
    call SuspendHeroXPBJ( false, udg_Kael )
    // Pause Units
    call PauseAllUnitsBJ( true )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_AP1_Player, Condition(function Trig_OpenGate_Setup_Func016002002))
