function Trig_Victory_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_Magtheridon_Completed )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Prep )
    call ConditionalTriggerExecute( gg_trg_Victory_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_01, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_01A, udg_AP1_Player, ( GetSoundDurationBJ(gg_snd_A07Magtheridon22) + 3.00 ) )
    call IssuePointOrderLocBJ( udg_CinematicIllidan, "move", GetRectCenter(gg_rct_Victory_Illidan_01) )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Victory_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TriggerSleepAction( 1.00 )
    if ( Trig_Victory_Cinematic_Q_Func025001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_CinematicMagtheridon, "TRIGSTR_169", gg_snd_A07Magtheridon22, "TRIGSTR_170", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_A07Magtheridon22, 0.50 )
    if ( Trig_Victory_Cinematic_Q_Func028001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_02, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_02A, udg_AP1_Player, ( GetSoundDurationBJ(gg_snd_A07Illidan23) + 2.00 ) )
    call TriggerSleepAction( 0.50 )
    if ( Trig_Victory_Cinematic_Q_Func032001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_CinematicIllidan, "TRIGSTR_171", gg_snd_A07Illidan23, "TRIGSTR_172", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_A07Illidan23, 0 )
    if ( Trig_Victory_Cinematic_Q_Func035001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 0.50 )
    if ( Trig_Victory_Cinematic_Q_Func038001() ) then
        return
    else
        call DoNothing(  )
    endif
    call AdjustCameraBoundsForPlayerBJ( bj_CAMERABOUNDS_ADJUST_ADD, udg_AP1_Player, 0, 0, 512.00, 0 )
    // Spawn Bystanders
    call ConditionalTriggerExecute( gg_trg_Victory_Create_Bystanders )
    // Move Illidan
    call SetUnitPositionLoc( udg_CinematicIllidan, GetRectCenter(gg_rct_Victory_Illidan_02) )
    call IssuePointOrderLocBJ( udg_CinematicIllidan, "move", GetRectCenter(gg_rct_Victory_Illidan_03) )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_03, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Victory_03A, udg_AP1_Player, ( GetSoundDurationBJ(gg_snd_A07Illidan24) + 4.00 ) )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 0.50 )
    if ( Trig_Victory_Cinematic_Q_Func049001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_CinematicIllidan, "TRIGSTR_173", gg_snd_A07Illidan24, "TRIGSTR_174", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_A07Illidan24, 0 )
    if ( Trig_Victory_Cinematic_Q_Func052001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Victory_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 2.00 )
    call RemoveUnit( udg_CinematicAkama )
    call RemoveUnit( udg_CinematicIllidan )
    call RemoveUnit( udg_CinematicKael )
    call RemoveUnit( udg_CinematicMagtheridon )
    call RemoveUnit( udg_CinematicVashj )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Run )
endfunction

