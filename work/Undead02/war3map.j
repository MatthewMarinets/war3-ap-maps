//===========================================================================
// Trigger: Initialize Initializations
//===========================================================================
function Trig_Initialize_Initializations_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_U )
    call ConditionalTriggerExecute( gg_trg_Handicap )
    call ConditionalTriggerExecute( gg_trg_Remove_Units_For_Easy )
    call ConditionalTriggerExecute( gg_trg_Remove_Units_For_Normal )
    call ConditionalTriggerExecute( gg_trg_Give_Uther_Mana_For_Hard )
    call ConditionalTriggerExecute( gg_trg_LoadGameCache )
    call ConditionalTriggerExecute( gg_trg_Friendly_Friends )
    call ConditionalTriggerExecute( gg_trg_Intro_Cinematic )
    call EnableTrigger( gg_trg_Level_3_Stop_XP )
    call ConditionalTriggerExecute( gg_trg_Knights_Patrol )
    call ConditionalTriggerExecute( gg_trg_Never_Sleep )
    call ConditionalTriggerExecute( gg_trg_DayTime )
    call ConditionalTriggerExecute( gg_trg_Money_Edmunson )
    call ConditionalTriggerExecute( gg_trg_Money_Kang )
    call ConditionalTriggerExecute( gg_trg_Start_AI )
    call SetUnitInvulnerable( gg_unit_Utic_0006, true )
    call SetUnitInvulnerable( gg_unit_nvil_0144, true )
    call SetUnitInvulnerable( gg_unit_nvl2_0146, true )
    call SetUnitInvulnerable( gg_unit_nvlw_0148, true )
    call SetUnitInvulnerable( gg_unit_nvil_0145, true )
    call SetUnitInvulnerable( gg_unit_nvlw_0147, true )
    call SetPlayerHandicapXPBJ( udg_ThePlayer, 25.00 )
    set udg_Uther = gg_unit_Huth_0091
    call CommentString( "Hide Tichondrius Soul Fires" )
    call SetDoodadAnimationRectBJ( "death", 'YOf3', gg_rct_FireGust1 )
    call SetDoodadAnimationRectBJ( "death", 'YOf3', gg_rct_FireGust2 )
    call SetDoodadAnimationRectBJ( "death", 'YOf3', gg_rct_FireGust3 )
    call SetDoodadAnimationRectBJ( "death", 'YOf3', gg_rct_FireGust4 )
    call SetDoodadAnimationRectBJ( "death", 'YOf3', gg_rct_FireGust5 )
    call CommentString( "Hide Players In Score Screen" )
    call SetPlayerOnScoreScreenBJ( false, udg_Tichondrius )
    call SetPlayerOnScoreScreenBJ( false, Player(6) )
    call SetPlayerOnScoreScreenBJ( false, udg_KELTHUZAD )
    call SetMapMusicIndexedBJ( "music", 1 )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
endfunction

//===========================================================================
function InitTrig_Initialize_Initializations takes nothing returns nothing
    set gg_trg_Initialize_Initializations = CreateTrigger(  )
    call TriggerAddAction( gg_trg_Initialize_Initializations, function Trig_Initialize_Initializations_Actions )
endfunction

//===========================================================================
// Trigger: Handicap
//===========================================================================
function Trig_Handicap_Conditions takes nothing returns boolean
    if ( not ( GetGameDifficulty() == MAP_DIFFICULTY_EASY ) ) then
        return false
    endif
    return true
endfunction

function Trig_Handicap_Actions takes nothing returns nothing
    call SetPlayerHandicapBJ( Player(1), bj_HANDICAP_EASY )
    call SetPlayerHandicapBJ( Player(2), bj_HANDICAP_EASY )
    call SetPlayerHandicapBJ( Player(5), bj_HANDICAP_EASY )
    call SetPlayerHandicapBJ( Player(9), bj_HANDICAP_EASY )
    call SetPlayerHandicapBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_HANDICAP_EASY )
endfunction

//===========================================================================
function InitTrig_Handicap takes nothing returns nothing
    set gg_trg_Handicap = CreateTrigger(  )
    call TriggerAddCondition( gg_trg_Handicap, Condition( function Trig_Handicap_Conditions ) )
    call TriggerAddAction( gg_trg_Handicap, function Trig_Handicap_Actions )
endfunction

//===========================================================================
// Trigger: Remove Units For Easy
//===========================================================================
function Trig_Remove_Units_For_Easy_Conditions takes nothing returns boolean
    if ( not ( GetGameDifficulty() == MAP_DIFFICULTY_EASY ) ) then
        return false
    endif
    return true
endfunction

function Trig_Remove_Units_For_Easy_Actions takes nothing returns nothing
