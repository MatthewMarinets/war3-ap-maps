function Trig_Intro_Skip_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call UseTimeOfDayBJ( true )
    set udg_IntroSkip = true
    call ResetUnitAnimation( udg_Illidan )
    call AdjustPlayerStateBJ( 500, udg_Player, PLAYER_STATE_RESOURCE_GOLD )
    call AdjustPlayerStateBJ( 200, udg_Player, PLAYER_STATE_RESOURCE_LUMBER )
    call RemoveUnit( udg_Arthas )
    call FogEnableOn(  )
    call FogMaskEnableOn(  )
    call SetUnitInvulnerable( udg_Illidan, false )
    call SetSkyModel( null )
    call SetUnitPositionLoc( udg_Illidan, GetRectCenter(gg_rct_Illidan_Final_Intro_Move) )
    call IssueImmediateOrderBJ( udg_Illidan, "unimmolation" )
    call ResetToGameCameraForPlayer( udg_Player, 0.00 )
    call PanCameraToTimedLocForPlayer( udg_Player, GetRectCenter(gg_rct_Illidan_Final_Intro_Move), 0 )
    call CommentString( "Start the AI, and give it a little time to get going" )
    call ConditionalTriggerExecute( gg_trg_Start_AI )
    call TriggerSleepAction( 1.20 )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call CinematicModeBJ( false, GetPlayersAll() )
    call SelectUnitSingle( udg_Illidan )
    call SetUnitLifePercentBJ( udg_Illidan, 90.00 )
    call SetUnitManaPercentBJ( udg_Illidan, 80.00 )
    call CameraClearNoiseForPlayer( udg_Player )
    call CommentString( "Start harvesters" )
    set udg_EntangledGoldMine = GroupPickRandomUnit(GetUnitsOfPlayerAndTypeId(udg_Player, 'egol'))
    call ForGroup( GetUnitsInRectMatching(gg_rct_Harvest_Gold, Condition(function Trig_Intro_Skip_Func029001002)), function Trig_Intro_Skip_Func029002 )
    call ForGroup( GetUnitsInRectAll(gg_rct_Harvest_Lumber), function Trig_Intro_Skip_Func030002 )
    call ForGroup( GetUnitsInRectAll(gg_rct_Harvest_Lumber2), function Trig_Intro_Skip_Func031002 )
    call CommentString( "Hint Timers" )
    call TriggerSleepAction( 1.00 )
    call QuestMessageBJ( GetPlayersAll(), bj_QUESTMESSAGE_DISCOVERED, "TRIGSTR_114" )
    call PingMinimapLocForForce( GetPlayersAll(), GetUnitLoc(gg_unit_nshr_0063), 7.00 )
    call SetCameraQuickPositionLocForPlayer( udg_Player, GetUnitLoc(gg_unit_nshr_0063) )
    call StartTimerBJ( udg_HintDruidTimer, false, 30.00 )
    call StartTimerBJ( udg_HintImmolationTimer, false, 60.00 )
endfunction

