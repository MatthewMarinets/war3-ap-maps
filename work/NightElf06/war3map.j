//===========================================================================
// Trigger: Map Initialization
//===========================================================================
function Trig_Map_Initialization_Func026002 takes nothing returns nothing
    call SetUnitManaPercentBJ( GetEnumUnit(), 100 )
endfunction

function Trig_Map_Initialization_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_N )
    call ConditionalTriggerExecute( gg_trg_Difficulty_Hard )
    call ConditionalTriggerExecute( gg_trg_Difficulty_Normal )
    call ConditionalTriggerExecute( gg_trg_Difficulty_Easy )
    call TriggerExecute( gg_trg_Set_Heroes )
    call TriggerExecute( gg_trg_Alliance_Settings )
    call TriggerExecute( gg_trg_Starting_Cash )
    call TriggerExecute( gg_trg_Starting_Cash_Demon )
    call TriggerExecute( gg_trg_Starting_Cash_Ancients )
    call TriggerExecute( gg_trg_Experience_Slowed )
    call SetPlayerAbilityAvailableBJ( false, 'AEIl', udg_Player )
    call IssueTargetOrderBJ( gg_unit_etoa_0189, "entangleinstant", gg_unit_ngol_0041 )
    call GroupClear( udg_HiddenPlayerUnits )
    call SetPlayerOnScoreScreenBJ( false, Player(0) )
    call SetPlayerOnScoreScreenBJ( false, Player(7) )
    call SetPlayerOnScoreScreenBJ( false, Player(3) )
    call SetPlayerColorBJ( Player(7), PLAYER_COLOR_RED, true )
    call SetPlayerColorBJ( Player(11), PLAYER_COLOR_GREEN, true )
    call SetPlayerAllianceStateBJ( udg_Player, Player(7), bj_ALLIANCE_ALLIED )
    call SetPlayerColorBJ( Player(6), PLAYER_COLOR_RED, true )
    call SetPlayerAllianceStateBJ( Player(3), Player(1), bj_ALLIANCE_ALLIED_VISION )
    call CommentString( "GIVE SKULL VISION TO PLAYER" )
    call UnitShareVisionBJ( true, gg_unit_nshr_0063, Player(1) )
    call SetUnitInvulnerable( gg_unit_nshr_0063, true )
    call ConditionalTriggerExecute( gg_trg_Intro )
    call ForGroup( GetUnitsOfPlayerAndTypeId(udg_Player, 'emow'), function Trig_Map_Initialization_Func026002 )
    call CommentString( "QUESTS" )
    call CreateQuestBJ( bj_QUESTTYPE_REQ_DISCOVERED, "TRIGSTR_122", "TRIGSTR_123", "ReplaceableTextures\\CommandButtons\\BTNGuldanSkull.tga" )
    set udg_QuestSkullofGuldan = GetLastCreatedQuestBJ()
    call CreateQuestItemBJ( udg_QuestSkullofGuldan, "TRIGSTR_124" )
    set udg_QuestSkullReqDemongate = GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ( udg_QuestSkullofGuldan, "TRIGSTR_128" )
    set udg_QuestSkullReqSkull = GetLastCreatedQuestItemBJ()
    call CreateQuestBJ( bj_QUESTTYPE_REQ_UNDISCOVERED, "TRIGSTR_125", "TRIGSTR_126", "ReplaceableTextures\\CommandButtons\\BTNTichondrius.tga" )
    set udg_QuestKillTicho = GetLastCreatedQuestBJ()
    call CreateQuestItemBJ( udg_QuestKillTicho, "TRIGSTR_127" )
    set udg_QuestKillReqKill = GetLastCreatedQuestItemBJ()
    call CommentString( "After Skull" )
    set udg_Infernal1 = gg_unit_ninf_0011
    set udg_Infernal2 = gg_unit_ninf_0012
endfunction

//===========================================================================
function InitTrig_Map_Initialization takes nothing returns nothing
    set gg_trg_Map_Initialization = CreateTrigger(  )
    call TriggerAddAction( gg_trg_Map_Initialization, function Trig_Map_Initialization_Actions )
endfunction

