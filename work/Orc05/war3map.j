function Trig_Initialize_Variables_Actions takes nothing returns nothing
    call CommentString( "HEROES" )
    set udg_Cenarius = gg_unit_Ecen_0000
    call CommentString( "UNIT GROUPS" )
    call ForGroup( GetUnitsInRectOfPlayer(gg_rct_UnitGroupFinalSatyr, Player(PLAYER_NEUTRAL_AGGRESSIVE)), function Trig_Initialize_Variables_Func004002 )
    call ForGroup( GetUnitsInRectOfPlayer(gg_rct_SatyrWayGateGuards, Player(PLAYER_NEUTRAL_AGGRESSIVE)), function Trig_Initialize_Variables_Func005002 )
    call ForGroup( GetUnitsInRectMatching(gg_rct_UnitGroupInitialBaseTop, Condition(function Trig_Initialize_Variables_Func006001002)), function Trig_Initialize_Variables_Func006002 )
    call ForGroup( GetUnitsInRectMatching(gg_rct_UnitGroupInitialBaseMiddle, Condition(function Trig_Initialize_Variables_Func007001002)), function Trig_Initialize_Variables_Func007002 )
    call ForGroup( GetUnitsInRectMatching(gg_rct_UnitGroupInitialBaseBottom, Condition(function Trig_Initialize_Variables_Func008001002)), function Trig_Initialize_Variables_Func008002 )
    call CommentString( "NO CHAOS UNITS AT START" )
    call SetPlayerUnitAvailableBJ( 'ncpn', false, udg_PlayerGrom )
    call SetPlayerUnitAvailableBJ( 'nchg', false, udg_PlayerGrom )
    call SetPlayerUnitAvailableBJ( 'nchr', false, udg_PlayerGrom )
    call SetPlayerUnitAvailableBJ( 'nckb', false, udg_PlayerGrom )
    call SetPlayerUnitAvailableBJ( 'nchw', false, udg_PlayerGrom )
    call CommentString( "SET FOUNTAIN OF BLOOD ANIMATION" )
    call SetUnitAnimation( gg_unit_nbfl_0035, "stand" )
    call SetUnitInvulnerable( gg_unit_nbfl_0035, true )
    call CommentString( "MAKE CREEPS INSOMNIACS" )
    call ForGroup( GetUnitsOfPlayerAll(Player(PLAYER_NEUTRAL_AGGRESSIVE)), function Trig_Initialize_Variables_Func019002 )
    call CommentString( "STARTING TIME OF DAY" )
    call SetTimeOfDay( 7.00 )
    call CommentString( "RANDOMIZE CENARIUS SMACK" )
    set udg_RandomCenariusSmack = GetRandomReal(0, 3.00)
    call CommentString( "HIDE PLAYERS IN SCORE SCREEN" )
    call SetPlayerOnScoreScreenBJ( false, Player(4) )
    call SetPlayerOnScoreScreenBJ( false, Player(11) )
    call CommentString( "CHANGE PLAYER COLORS" )
    call SetPlayerColorBJ( Player(4), PLAYER_COLOR_PURPLE, true )
    call SetPlayerColorBJ( Player(11), PLAYER_COLOR_CYAN, true )
endfunction

