function Trig_Load_Grom_Func005001 takes nothing returns boolean
    return ( udg_Grom != null )
endfunction

function Trig_Load_Grom_Actions takes nothing returns nothing
    call SetPlayerHandicapXPBJ( Player(3), 30.00 )
    call InitGameCacheBJ( "Campaigns.w3v" )
    call RestoreUnitLocFacingAngleBJ( "Grom", "Orc05", GetLastCreatedGameCacheBJ(), udg_PlayerGrom, GetRectCenter(gg_rct_InitialGromLocation), 180.00 )
    set udg_Grom = GetLastRestoredUnitBJ()
    if ( Trig_Load_Grom_Func005001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CreateNUnitsAtLoc( 1, 'Ogrh', udg_PlayerGrom, GetRectCenter(gg_rct_InitialGromLocation), 180.00 )
    set udg_Grom = GetLastCreatedUnit()
    call SetHeroLevel( udg_Grom, 6, false )
    call SelectHeroSkill( udg_Grom, 'AOcr' )
    call SelectHeroSkill( udg_Grom, 'AOcr' )
    call SelectHeroSkill( udg_Grom, 'AOmi' )
    call SelectHeroSkill( udg_Grom, 'AOmi' )
    call SelectHeroSkill( udg_Grom, 'AOwk' )
    call SelectHeroSkill( udg_Grom, 'AOww' )
    call EnableTrigger( gg_trg_Limit_Grom_XP )
endfunction

