//===========================================================================
// Trigger: Occasional Lightning
//
// The Storm starts this up.
//===========================================================================
function Trig_Occasional_Lightning_Func001001 takes nothing returns boolean
    return ( udg_InCinematic == false )
endfunction

function Trig_Occasional_Lightning_Func009001 takes nothing returns boolean
    return ( udg_RandomSound == 1 )
endfunction

function Trig_Occasional_Lightning_Func010001 takes nothing returns boolean
    return ( udg_RandomSound == 2 )
endfunction

function Trig_Occasional_Lightning_Actions takes nothing returns nothing
    loop
        exitwhen ( Trig_Occasional_Lightning_Func001001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 1))
    endloop
    call StartTimerBJ( udg_LightningTimer, false, GetRandomReal(65.00, 120.00) )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUTIN, 0.12, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 100.00, 100.00, 100.00, 5.00 )
    call TriggerSleepAction( 0.03 )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUTIN, 0.12, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 100.00, 100.00, 100.00, 5.00 )
    call TriggerSleepAction( 0.20 )
    set udg_RandomSound = GetRandomInt(1, 2)
    if ( Trig_Occasional_Lightning_Func009001() ) then
        call PlaySoundBJ( gg_snd_LightningBolt1 )
    else
        call DoNothing(  )
    endif
    if ( Trig_Occasional_Lightning_Func010001() ) then
        call PlaySoundBJ( gg_snd_RollingThunder1 )
    else
        call DoNothing(  )
    endif
endfunction

