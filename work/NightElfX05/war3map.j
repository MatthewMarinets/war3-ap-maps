function Trig_Init_02_Units_Actions takes nothing returns nothing
    // Heroes
    call ConditionalTriggerExecute( gg_trg_Load_Heroes )
    call SetPlayerHandicapXPBJ( udg_AP2_Player, 25.00 )
    // NPCs
    call SetUnitInvulnerable( gg_unit_etrs_0141, true )
    call SetUnitInvulnerable( gg_unit_etrs_0142, true )
    call SetUnitInvulnerable( gg_unit_etrs_0143, true )
    call SetUnitInvulnerable( gg_unit_etrs_0140, true )
    call SetUnitInvulnerable( gg_unit_etrs_0020, true )
    call SetUnitInvulnerable( gg_unit_etrs_0028, true )
    call ForGroupBJ( GetUnitsOfPlayerAndTypeId(udg_AP2_Player, 'emow'), function Trig_Init_02_Units_Func011002 )
    // Naga Buildings
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P4_Naga_Purple, 'nnad'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P4_Naga_Purple, 'nnsg'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P4_Naga_Purple, 'nntt'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P4_Naga_Purple, 'nnsa'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P9_Naga_Gray, 'nnad'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P9_Naga_Gray, 'nnsg'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P9_Naga_Gray, 'nntt'), udg_NagaBuildings )
    call GroupAddGroup( GetUnitsOfPlayerAndTypeId(udg_P9_Naga_Gray, 'nnsa'), udg_NagaBuildings )
    // Naga Units
    call ForGroupBJ( GetUnitsInRectAll(gg_rct_Recyled_Units), function Trig_Init_02_Units_Func022A )
    // Fountian of Health
    call RemoveGuardPosition( gg_unit_nnsw_0207 )
    call RemoveGuardPosition( gg_unit_nsnp_0032 )
    // Creeps
    call EnableCreepSleepBJ( false )
    set udg_NagaGroupA = GetUnitsInRectOfPlayer(gg_rct_Naga_Group_A, udg_P6_Naga_Orange)
