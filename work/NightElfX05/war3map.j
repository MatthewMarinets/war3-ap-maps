    call SetPlayerAllianceStateBJ( udg_P4_Naga_Purple, udg_AP2_Player, bj_ALLIANCE_NEUTRAL )
    // Disable Ability
    call SetPlayerAbilityAvailableBJ( false, 'AEah', udg_AP2_Player )
    call SetPlayerAbilityAvailableBJ( false, 'AEar', udg_AP2_Player )
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', udg_AP2_Player )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Maiev )
    call PauseUnitBJ( false, udg_Furion )
    call PauseUnitBJ( false, udg_Tyrande )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerAll(udg_AP2_Player)
    call GroupRemoveUnitSimple( udg_Maiev, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Furion, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Tyrande, udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P4_Naga_Purple), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P9_Naga_Gray), udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func044002 )
    // Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func046002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP2_Player), function Trig_Victory_Setup_Func048002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P4_Naga_Purple), function Trig_Victory_Setup_Func049002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P3_Naga_Teal), function Trig_Victory_Setup_Func050002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P9_Naga_Gray), function Trig_Victory_Setup_Func051002 )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\HeroicVictory.mp3" )
endfunction

