function Trig_Boat_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    set udg_FleetQuestDiscovered = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Boat_Cinematic_Q_Func010C() ) then
        return
    else
    endif
    call ConditionalTriggerExecute( gg_trg_Boat_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Min_Cin_Boat_01, udg_AP2_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Mid_Cin_Boat_02, udg_AP2_Player, ( GetSoundDurationBJ(gg_snd_S05Furion11) + ( GetSoundDurationBJ(gg_snd_S05Tyrande12) + ( GetSoundDurationBJ(gg_snd_S05Furion13) + 4.00 ) ) ) )
    call SetUnitFacingToFaceUnitTimed( udg_Furion, gg_unit_nhmc_0147, 0 )
    call SetUnitFacingToFaceUnitTimed( udg_Tyrande, gg_unit_nhmc_0147, 0 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Boat_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // Rescue Town
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_Furion, "TRIGSTR_162", gg_snd_S05Furion11, "TRIGSTR_163", bj_TIMETYPE_ADD, 0, true )
    call WaitForSoundBJ( gg_snd_S05Furion11, 0 )
    if ( Trig_Boat_Cinematic_Q_Func027001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetUnitLookAt( udg_Tyrande, "bone_head", udg_Furion, 0, 0, 90 )
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_Tyrande, "TRIGSTR_164", gg_snd_S05Tyrande12, "TRIGSTR_165", bj_TIMETYPE_ADD, 0, true )
    call WaitForSoundBJ( gg_snd_S05Tyrande12, 0 )
    if ( Trig_Boat_Cinematic_Q_Func031001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_Furion, "TRIGSTR_170", gg_snd_S05Furion13, "TRIGSTR_171", bj_TIMETYPE_ADD, 0, true )
    call ResetUnitLookAt( udg_Tyrande )
    call WaitForSoundBJ( gg_snd_S05Furion13, 0 )
    if ( Trig_Boat_Cinematic_Q_Func035001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Boat_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Boat_Cleanup )
    call CinematicModeBJ( false, udg_APG1_Player )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Quest update
    call ConditionalTriggerExecute( gg_trg_Quest_Night_Elf_Fleet_Discover )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    set udg_InCinematic = false
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

