// Trigger: The Storm
//
// When you get to the middle of the temple, it will flash lightning and begin raining across the entire map.
//===========================================================================
function Trig_The_Storm_Func002001 takes nothing returns boolean
    return ( udg_InCinematic == false )
endfunction

function Trig_The_Storm_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    loop
        exitwhen ( Trig_The_Storm_Func002001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 1))
    endloop
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUTIN, 0.12, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 100.00, 100.00, 100.00, 5.00 )
    call TriggerSleepAction( 0.03 )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUTIN, 0.12, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 100.00, 100.00, 100.00, 5.00 )
    call PlaySoundBJ( gg_snd_RollingThunder1 )
    call TriggerSleepAction( 2.50 )
    call StartTimerBJ( udg_LightningTimer, false, 75.00 )
endfunction

