//===========================================================================
function Trig_Defense_Setup_Conditions takes nothing returns boolean
    if ( not ( udg_CinematicSkip == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Defense_Setup_Func006A takes nothing returns nothing
    call GroupAddUnitSimple( GetEnumUnit(), udg_Peons )
    call SetUnitMoveSpeed( GetEnumUnit(), 250.00 )
endfunction

function Trig_Defense_Setup_Func010A takes nothing returns nothing
    call GroupAddUnitSimple( GetEnumUnit(), udg_SkeletonStartGroup )
    call SetUnitMoveSpeed( GetEnumUnit(), 230.00 )
endfunction

function Trig_Defense_Setup_Func011A takes nothing returns nothing
    call GroupAddUnitSimple( GetEnumUnit(), udg_SkeletonPortalGroup )
    call SetUnitMoveSpeed( GetEnumUnit(), 230.00 )
endfunction

function Trig_Defense_Setup_Actions takes nothing returns nothing
    // SETUP ENVIRONMENT
    call SetTerrainFogExBJ( 0, 0.00, 4000.00, 0, 70.00, 80.00, 90.00 )
    // CHANGE COLORS
    // STORE PEONS
    call ForGroupBJ( GetUnitsInRectOfPlayer(gg_rct_Peon_Start, udg_Viewer), function Trig_Defense_Setup_Func006A )
    // IGNORE HOSTILITY (Prevents pathing problems)
    call SetPlayerAllianceStateBJ( udg_Hostile, udg_Viewer, bj_ALLIANCE_ALLIED )
    // STORE SKELETONS
    call ForGroupBJ( GetUnitsInRectOfPlayer(gg_rct_Skeleton_Charge_Start, udg_Hostile), function Trig_Defense_Setup_Func010A )
    call ForGroupBJ( GetUnitsInRectOfPlayer(gg_rct_Skeleton_Charge_Portal, udg_Hostile), function Trig_Defense_Setup_Func011A )
    // STORE HEROES
    set udg_Grom = gg_unit_Ogrh_0002
    set udg_Thrall = gg_unit_Othr_0047
    set udg_Rokhan = gg_unit_Orkn_0051
    set udg_Cairne = gg_unit_Ocb2_0052
    call PauseUnitBJ( true, udg_Grom )
    call PauseUnitBJ( true, udg_Thrall )
    call PauseUnitBJ( true, udg_Cairne )
    call PauseUnitBJ( true, udg_Rokhan )
    call ShowUnitHide( udg_Grom )
    call ShowUnitHide( udg_Thrall )
    call ShowUnitHide( udg_Rokhan )
    call ShowUnitHide( udg_Cairne )
    // START CHASE
    call ConditionalTriggerExecute( gg_trg_Peon_Run_Cameras )
    call ConditionalTriggerExecute( gg_trg_Peon_Run_1 )
    call ConditionalTriggerExecute( gg_trg_Skeleton_Charge_1 )
    call ConditionalTriggerExecute( gg_trg_Spawn_Start )
endfunction

