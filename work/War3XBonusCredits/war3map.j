        call SetUnitMoveSpeed( GetLastCreatedUnit(), 220.00 )
        call SetUnitLifePercentBJ( GetTriggerUnit(), 25.00 )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    if ( Trig_Spawn_Create_Func004001() ) then
        call DoNothing(  )
    else
        return
    endif
    call GroupPointOrderLocBJ( udg_SpawnGroup, "attack", GetRectCenter(udg_SpawnDest) )
