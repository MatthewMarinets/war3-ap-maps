    set gg_trg_Defense_Setup = CreateTrigger(  )
    call TriggerAddCondition( gg_trg_Defense_Setup, Condition( function Trig_Defense_Setup_Conditions ) )
    call TriggerAddAction( gg_trg_Defense_Setup, function Trig_Defense_Setup_Actions )
endfunction

