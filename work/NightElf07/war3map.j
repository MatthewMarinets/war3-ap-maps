function Trig_Intro_Cinematic_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    set udg_InCinematic = true
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call UseTimeOfDayBJ( false )
    call CameraSetupApplyForPlayer( true, gg_cam_IntroNEBase01, udg_AAAP_NightElves, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_IntroNEBase02, udg_AAAP_NightElves, 8.00 )
    call CinematicModeBJ( true, udg_AAPG_NightElves )
    call CommentString( "Establish Scene" )
    call SetUnitPositionLoc( udg_Furion, GetRectCenter(gg_rct_IntroFurion00) )
    call SetUnitPositionLoc( udg_Tyrande, GetRectCenter(gg_rct_IntroTyrande00) )
    call SetUnitFacingToFaceUnitTimed( udg_Tyrande, udg_Furion, 0 )
    call SetUnitFacingToFaceUnitTimed( udg_Furion, udg_Tyrande, 0 )
    call CommentString( "CINEMATIC STARTS" )
    call CommentString( "Night Elf Base Scene" )
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 2.00 )
    call CommentString( "CAN CANCEL FROM THIS POINT FORWARD" )
    call EnableTrigger( gg_trg_Intro_Cancel )
    call TriggerSleepAction( 6.00 )
    if ( Trig_Intro_Cinematic_Func020001() ) then
        return
    else
        call DoNothing(  )
    endif
