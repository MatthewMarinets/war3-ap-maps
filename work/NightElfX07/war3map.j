    call RemoveUnit( udg_AncientProtector02 )
    call RemoveUnit( udg_AncientProtector04 )
    call RemoveUnit( udg_AncientofLore01 )
    call RemoveUnit( udg_AncientofWar01 )
    call RemoveUnit( udg_AncientofWind01 )
    call RemoveUnit( udg_AncientofWonders01 )
    // Replace Buildings
    call CreateNUnitsAtLoc( 1, 'etrp', udg_AP2_Player, GetRectCenter(gg_rct_Ancient_Protector_02_req), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'etrp', udg_AP2_Player, GetRectCenter(gg_rct_Ancient_Protector_04_req), bj_UNIT_FACING )
