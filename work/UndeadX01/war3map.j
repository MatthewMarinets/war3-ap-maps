function Trig_Halahk_Defends_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    set udg_TempPoint = GetUnitLoc(GroupPickRandomUnit(GetUnitsInRangeOfLocMatching(1536.00, GetUnitLoc(GetAttackedUnitBJ()), Condition(function Trig_Halahk_Defends_Func002002001001003))))
