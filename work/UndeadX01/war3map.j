function Trig_Intro_Cleanup_Actions takes nothing returns nothing
    // Unhide Units
    call ForGroup( udg_HiddenUnits, function Trig_Intro_Cleanup_Func002002 )
    call ForGroup( GetUnitsOfPlayerAll(udg_P12_CinematicUnits), function Trig_Intro_Cleanup_Func003002 )
    call ForGroup( udg_CinematicVillagers, function Trig_Intro_Cleanup_Func004002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Intro_Cleanup_Func010001() ) then
        call SuspendHeroXPBJ( true, udg_Arthas )
    else
        call DoNothing(  )
    endif
    if ( Trig_Intro_Cleanup_Func011001() ) then
        call SuspendHeroXPBJ( true, udg_Kelthuzad )
    else
        call DoNothing(  )
    endif
    if ( Trig_Intro_Cleanup_Func012001() ) then
        call SuspendHeroXPBJ( true, udg_Sylvanas )
    else
        call DoNothing(  )
    endif
    // Restore Selection
    call SelectUnitSingle( udg_Arthas )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP01_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP01_Player, GetRectCenter(gg_rct_Gameplay_Start_Arthas), 0 )
    // Wipe Visibility
    call CreateFogModifierRectBJ( true, udg_AP01_Player, FOG_OF_WAR_MASKED, GetPlayableMapRect() )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
    // Brown is Brown
    call SetPlayerColorBJ( Player(11), PLAYER_COLOR_BROWN, false )
endfunction

