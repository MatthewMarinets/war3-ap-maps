//===========================================================================
// Trigger: Initialize Order
//===========================================================================
function Trig_Initialize_Order_Func028002 takes nothing returns nothing
    call SetDestructableMaxLifeBJ( GetEnumDestructable(), 70.00 )
endfunction

function Trig_Initialize_Order_Func029002 takes nothing returns nothing
    call SetDestructableMaxLifeBJ( GetEnumDestructable(), 90.00 )
endfunction

function Trig_Initialize_Order_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_O )
    call ConditionalTriggerExecute( gg_trg_Not_Hard_Difficulty )
    call ConditionalTriggerExecute( gg_trg_Handicap )
    call TriggerExecute( gg_trg_Game_Cache )
    call TriggerExecute( gg_trg_Set_Infernal_Fall_Array )
    call TriggerExecute( gg_trg_Create_Warlock_Region_Array )
    call TriggerExecute( gg_trg_Starting_Cash_WS )
    call TriggerExecute( gg_trg_Starting_Cash_J )
    call CreateQuestBJ( bj_QUESTTYPE_REQ_DISCOVERED, "TRIGSTR_206", "TRIGSTR_207", "ReplaceableTextures\\CommandButtons\\BTNHellscream.tga" )
    set udg_SaveGromQuest = GetLastCreatedQuestBJ()
    call CreateQuestItemBJ( GetLastCreatedQuestBJ(), "TRIGSTR_208" )
    set udg_ReqCaptureGrom = GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ( GetLastCreatedQuestBJ(), "TRIGSTR_209" )
    set udg_ReqBringGromCOP = GetLastCreatedQuestItemBJ()
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call PauseUnitBJ( true, udg_Grom )
    call CommentString( "Color Swapping Score Screen Hiding" )
    call SetPlayerOnScoreScreenBJ( false, Player(10) )
    call SetPlayerOnScoreScreenBJ( false, Player(1) )
    call SetPlayerOnScoreScreenBJ( false, Player(7) )
    call SetPlayerOnScoreScreenBJ( false, Player(5) )
    call SetPlayerColorBJ( Player(1), PLAYER_COLOR_PURPLE, true )
    call SetPlayerColorBJ( Player(5), PLAYER_COLOR_GREEN, true )
    call SetPlayerColorBJ( Player(7), PLAYER_COLOR_LIGHT_BLUE, true )
    call SetPlayerColorBJ( Player(10), PLAYER_COLOR_PURPLE, true )
    call TriggerExecute( gg_trg_Friends_Forever )
    call CommentString( "All Trees Hit Points" )
    call EnumDestructablesInRectAll( GetPlayableMapRect(), function Trig_Initialize_Order_Func028002 )
    call EnumDestructablesInRectAll( gg_rct_Jaina_Trees, function Trig_Initialize_Order_Func029002 )
    call CommentString( "Ability Tinkering" )
    call SetPlayerUnitAvailableBJ( 'nchw', false, udg_Player )
    call SetPlayerUnitAvailableBJ( 'nchg', false, udg_Player )
    call SetPlayerUnitAvailableBJ( 'nchr', false, udg_Player )
    call SetPlayerUnitAvailableBJ( 'nckb', false, udg_Player )
    call SetPlayerUnitAvailableBJ( 'ncpn', false, udg_Player )
    set udg_PlayerSwapperInt = 0
    call TriggerExecute( gg_trg_Intro_Cin )
endfunction

//===========================================================================
function InitTrig_Initialize_Order takes nothing returns nothing
    set gg_trg_Initialize_Order = CreateTrigger(  )
    call TriggerAddAction( gg_trg_Initialize_Order, function Trig_Initialize_Order_Actions )
endfunction

