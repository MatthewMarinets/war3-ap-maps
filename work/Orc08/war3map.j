//===========================================================================
// Trigger: Game Cache
//===========================================================================
function Trig_Game_Cache_Func004001 takes nothing returns boolean
    return ( udg_Thrall == null )
endfunction

function Trig_Game_Cache_Func007001 takes nothing returns boolean
    return ( udg_Cairne == null )
endfunction

function Trig_Game_Cache_Actions takes nothing returns nothing
    call InitGameCacheBJ( "Campaigns.w3v" )
    call RestoreUnitLocFacingAngleBJ( "Thrall", "Orc08", GetLastCreatedGameCacheBJ(), udg_Player, GetRectCenter(gg_rct_THRALL), 270.00 )
    set udg_Thrall = GetLastRestoredUnitBJ()
    if ( Trig_Game_Cache_Func004001() ) then
        call ConditionalTriggerExecute( gg_trg_Game_Cache_Default_Thrall )
    else
        set bj_forLoopAIndex = 1
        set bj_forLoopAIndexEnd = 6
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call ConditionalTriggerExecute( gg_trg_Burn_Thrall_TP_Scrolls )
            set bj_forLoopAIndex = bj_forLoopAIndex + 1
        endloop
    endif
    call RestoreUnitLocFacingAngleBJ( "Cairne", "Orc08", GetLastCreatedGameCacheBJ(), udg_Player, GetRectCenter(gg_rct_CAIRNE), 270.00 )
    set udg_Cairne = GetLastRestoredUnitBJ()
    if ( Trig_Game_Cache_Func007001() ) then
        call ConditionalTriggerExecute( gg_trg_Game_Cache_Default_Cairne )
    else
        set bj_forLoopAIndex = 1
        set bj_forLoopAIndexEnd = 6
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call ConditionalTriggerExecute( gg_trg_Burn_Cairne_TP_Scrolls )
            set bj_forLoopAIndex = bj_forLoopAIndex + 1
        endloop
    endif
    set udg_Jaina = gg_unit_Hjai_0021
    set udg_Grom = gg_unit_Opgh_0020
endfunction

