//
// Waiting on triggers that will allow me to place an item in a specific slot on a hero.
//===========================================================================
function Trig_Thrall_Dead_New_Gem_Conditions takes nothing returns boolean
    if ( not ( GetRevivingUnit() == udg_Thrall ) ) then
        return false
    endif
    if ( not ( IsQuestCompleted(udg_SaveGromQuest) == false ) ) then
        return false
    endif
    if ( not ( UnitHasItemOfTypeBJ(udg_Thrall, 'soul') == true ) ) then
        return false
    endif
    if ( not ( udg_GAMEOVER == false ) ) then
        return false
    endif
    return true
endfunction

