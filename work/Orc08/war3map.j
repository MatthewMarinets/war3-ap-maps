    call RemoveItem( udg_SoulItemVar )
    call UnitAddItemByIdSwapped( 'gsou', udg_Thrall )
    call SetItemDroppableBJ( GetItemOfTypeFromUnitBJ(udg_Thrall, 'gsou'), false )
    if ( Trig_Thrall_Dead_New_Gem_Func007001() ) then
        call QuestItemSetCompletedBJ( udg_ReqCaptureGrom, false )
    else
        call DoNothing(  )
    endif
    call QuestMessageBJ( GetPlayersAll(), bj_QUESTMESSAGE_UPDATED, "TRIGSTR_036" )
endfunction

