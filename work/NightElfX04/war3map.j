function Trig_Arrive_Cleanup_Actions takes nothing returns nothing
    // Move Units
    // Unhide Units
    call ForGroup( udg_HiddenUnits, function Trig_Arrive_Cleanup_Func003002 )
    // Hide Units
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Arrive_Cleanup_Func005A )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Arrive_Cleanup_Func011001() ) then
        call SuspendHeroXPBJ( true, udg_Maiev )
    else
        call DoNothing(  )
    endif
    // Restore Selection
    call SelectGroupBJ( udg_SelectedUnits )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP2_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP2_Player, udg_CameraReturnPoint, 0 )
    // Remove Units
    if ( Trig_Arrive_Cleanup_Func021C() ) then
        call RemoveUnit( udg_AncientSpawner )
        call CreateNUnitsAtLocFacingLocBJ( 1, 'Udre', Player(9), GetRectCenter(gg_rct_StarRegSummon), GetUnitLoc(udg_ArrivalWitness) )
        set udg_AncientSpawner = GetLastCreatedUnit()
        call ShowUnitHide( udg_AncientSpawner )
        call SetHeroLevelBJ( udg_AncientSpawner, 10, false )
        call SelectHeroSkill( udg_AncientSpawner, 'AUin' )
        call IssuePointOrderLocBJ( udg_AncientSpawner, "dreadlordinferno", GetRectCenter(gg_rct_StarRegSummon) )
        call TriggerSleepAction( 0.80 )
        call TerrainDeformationCraterBJ( 0.40, true, GetRectCenter(gg_rct_StarRegSummon), 256.00, 128.00 )
        call SetDoodadAnimationRectBJ( "birth", 'YOfb', gg_rct_BlueFireDoods )
    else
    endif
    // Final Actions
    call IssuePointOrderLocBJ( udg_Ancient, "attack", GetRectCenter(gg_rct_StarRegEnter) )
    // Enable Abilities
    call SetPlayerAbilityAvailableBJ( true, 'Ashm', udg_AP2_Player )
