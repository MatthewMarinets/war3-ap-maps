    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Intro_Cleanup_Func012001() ) then
        call SuspendHeroXPBJ( true, udg_Maiev )
    else
        call DoNothing(  )
    endif
    // Restore Selection
    call SelectUnitSingle( udg_Maiev )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Doodads
    call SetDoodadAnimationRectBJ( "death", 'ZZcd', gg_rct_Intro_RocksFall )
