function Trig_Intro_Cleanup_Actions takes nothing returns nothing
    // Stop Looping Sounds
    call StopSoundBJ( gg_snd_EarthquakeLoop1, true )
    // Move Units
    call SetUnitPositionLocFacingBJ( udg_Maiev, GetRectCenter(gg_rct_Maiev_Begins_Here), 255.00 )
