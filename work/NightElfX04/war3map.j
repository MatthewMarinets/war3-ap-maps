function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\Sky\\SkyLight.mdl" )
    // Clear Selection
    call ClearSelection(  )
    // Revive Hero
    set udg_RevivalHero = udg_Maiev
    set udg_RevivalSpot = GetRectCenter(gg_rct_FinalCin_Maiev01)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_WarderReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceLocTimed( udg_Maiev, GetRectCenter(gg_rct_EndCin_Boat02), 0.00 )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Maiev )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Maiev )
    // Hide Units
    set udg_HiddenUnits = GetUnitsInRectMatching(GetPlayableMapRect(), Condition(function Trig_Victory_Setup_Func019002002))
    call GroupRemoveUnitSimple( udg_Maiev, udg_HiddenUnits )
    call ForGroup( udg_HiddenUnits, function Trig_Victory_Setup_Func021002 )
    call ShowUnitShow( udg_Maiev )
    // Hide Units
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func024A )
    // Remove Buffs
    call ForGroup( GetUnitsOfPlayerAll(udg_AP2_Player), function Trig_Victory_Setup_Func026002 )
    call ForGroup( GetUnitsOfPlayerAll(udg_P4_Naga), function Trig_Victory_Setup_Func027002 )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\HeroicVictory.mp3" )
    // Disable Abilities
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', udg_AP2_Player )
endfunction

