function Trig_Load_Grom_Actions takes nothing returns nothing
    call InitGameCacheBJ( "Campaigns.w3v" )
    call CommentString( "Load the hero data" )
    call RestoreUnitLocFacingAngleBJ( "Grom", "Orc04", GetLastCreatedGameCacheBJ(), udg_AAAP_Orcs, GetRectCenter(gg_rct_GromStart), 135.00 )
    set udg_Grom = GetLastRestoredUnitBJ()
    if ( Trig_Load_Grom_Func005001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CommentString( "If the hero data wasn't found, create a default hero" )
    call CreateNUnitsAtLoc( 1, 'Ogrh', udg_AAAP_Orcs, GetRectCenter(gg_rct_GromStart), 135.00 )
    set udg_Grom = GetLastCreatedUnit()
    call SetHeroLevel( udg_Grom, 4, false )
    call SelectHeroSkill( udg_Grom, 'AOmi' )
    call SelectHeroSkill( udg_Grom, 'AOwk' )
    call SelectHeroSkill( udg_Grom, 'AOcr' )
    call SelectHeroSkill( udg_Grom, 'AOcr' )
endfunction

