    endif
    call StoreBooleanBJ( udg_RokhanInParty, "RokhanInParty", "OrcX02", GetLastCreatedGameCacheBJ() )
    if ( Trig_Next_Level_Prep_Func008C() ) then
        call StoreUnitBJ( udg_Rokhan, "Rokhan", "OrcX02", GetLastCreatedGameCacheBJ() )
    else
