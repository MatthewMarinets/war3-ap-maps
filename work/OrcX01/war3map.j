    call StoreBooleanBJ( udg_ChenInParty, "ChenInParty", "OrcX02", GetLastCreatedGameCacheBJ() )
    if ( Trig_Next_Level_Prep_Func010C() ) then
        call StoreUnitBJ( udg_Chen, "Chen", "OrcX02", GetLastCreatedGameCacheBJ() )
    else
    endif
    // Save Stash Contents In A Hero
    call CreateNUnitsAtLoc( 1, 'Obla', Player(PLAYER_NEUTRAL_PASSIVE), GetUnitLoc(udg_Stash), 0.00 )
    set udg_StashHero = GetLastCreatedUnit()
    call ShowUnitHide( udg_StashHero )
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 6
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call UnitAddItemSwapped( UnitItemInSlotBJ(udg_Stash, GetForLoopIndexA()), udg_StashHero )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    call StoreUnitBJ( udg_StashHero, "Stash", "OrcX02", GetLastCreatedGameCacheBJ() )
    call RemoveUnit( udg_StashHero )
    // Save Cache
    call SaveGameCacheBJ( GetLastCreatedGameCacheBJ() )
    // Enable Missions, Campaigns, Cinematics
endfunction

