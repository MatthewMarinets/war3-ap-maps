    call PauseUnitBJ( false, udg_IntroQuillboar02 )
    call PauseUnitBJ( false, udg_IntroQuillboar03 )
    call PauseUnitBJ( false, udg_IntroQuillboar04 )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_AP1_Player, Condition(function Trig_Intro_Setup_Func017002002))
    call GroupAddUnitSimple( udg_MeetThrallPeon01, udg_HiddenUnits )
    call GroupAddUnitSimple( udg_MeetThrallPeon02, udg_HiddenUnits )
    call GroupAddUnitSimple( udg_MeetThrallPeon03, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Rexxar, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Misha, udg_HiddenUnits )
