    else
    endif
    call VolumeGroupResetBJ(  )
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
    // Guards Step Aside
    call IssuePointOrderLocBJ( udg_EntranceGruntA, "move", GetRectCenter(gg_rct_Entrance_MoveTargA) )
