function Trig_Pack_Despawn_Conditions takes nothing returns boolean
    if ( not ( GetRandomReal(0, 100.00) < 8.00 ) ) then
        return false
    endif
    if ( not ( CountUnitsInGroup(udg_PackGroupArray[udg_SpawnPackID]) > 0 ) ) then
