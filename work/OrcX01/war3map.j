function Trig_Next_Level_Prep_Actions takes nothing returns nothing
    // Remove Quest Items
    // Create Cache
    call InitGameCacheBJ( "Campaigns.w3v" )
    // Save Heroes
    call StoreBooleanBJ( udg_RexxarInParty, "RexxarInParty", "OrcX02", GetLastCreatedGameCacheBJ() )
