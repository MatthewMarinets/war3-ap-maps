    if ( not ( udg_DebugSpawning == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pack_Despawn_Func002C takes nothing returns boolean
    if ( not ( GetUnitTypeId(udg_TempDespawnUnit) != 'nvlk' ) ) then
        return false
    endif
    return true
endfunction

function Trig_Pack_Despawn_Actions takes nothing returns nothing
    set udg_TempDespawnUnit = GroupPickRandomUnit(udg_PackGroupArray[udg_SpawnPackID])
    if ( Trig_Pack_Despawn_Func002C() ) then
        // Don't Despawn Unit if Visible
        set udg_TempPoint = GetUnitLoc(udg_TempDespawnUnit)
        if ( Trig_Pack_Despawn_Func002Func004C() ) then
            call RemoveLocation( udg_TempPoint )
            return
        else
        endif
        // Despawn Unit
        call GroupRemoveUnitSimple( udg_TempDespawnUnit, udg_PackGroupArray[udg_SpawnPackID] )
        call GroupRemoveUnitSimple( udg_TempDespawnUnit, udg_SpawnPackDropTable )
        call RemoveUnit( udg_TempDespawnUnit )
        if ( Trig_Pack_Despawn_Func002Func009C() ) then
            call PingMinimapLocForForceEx( GetPlayersAll(), udg_TempPoint, 1, bj_MINIMAPPINGSTYLE_SIMPLE, 100.00, 0.00, 0.00 )
        else
        endif
        call RemoveLocation( udg_TempPoint )
    else
    endif
endfunction

