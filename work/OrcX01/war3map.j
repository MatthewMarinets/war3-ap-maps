    endif
    return true
endfunction

function Trig_Entrance_Scene_Q_Actions takes nothing returns nothing
    call SetSpeechVolumeGroupsBJ(  )
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_EntranceGruntB, "TRIGSTR_186", gg_snd_D00GruntGuard18, "TRIGSTR_187", bj_TIMETYPE_ADD, 0, true )
