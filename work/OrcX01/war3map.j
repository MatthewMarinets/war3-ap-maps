//===========================================================================
function Trig_Intro_Cleanup_Func020002 takes nothing returns nothing
    call ShowUnitShow( GetEnumUnit() )
endfunction

function Trig_Intro_Cleanup_Func027001 takes nothing returns boolean
    return ( udg_ExpDisabledForRexxar == false )
endfunction

function Trig_Intro_Cleanup_Actions takes nothing returns nothing
    call UnitAddItemByIdSwapped( 'mort', udg_Rexxar )
    call SetItemDroppableBJ( GetLastCreatedItem(), false )
    call SetItemInvulnerableBJ( GetLastCreatedItem(), true )
    set udg_MogrinsReport = GetLastCreatedItem()
    // Restore Abilities
    call ConditionalTriggerExecute( gg_trg_Cinematics_Enable_Talky_Effects )
    call ConditionalTriggerExecute( gg_trg_Cinematics_Enable_Abilities )
    // Unlock Bodyparts
    call ResetUnitLookAt( udg_Rexxar )
    // Remove Units
    call RemoveUnit( udg_IntroMogrin )
    call RemoveUnit( udg_IntroQuillboar01 )
    call RemoveUnit( udg_IntroQuillboar02 )
    call RemoveUnit( udg_IntroQuillboar03 )
    call RemoveUnit( udg_IntroQuillboar04 )
    // Move Units
    call SetUnitPositionLocFacingBJ( udg_Rexxar, GetRectCenter(gg_rct_Gameplay_Rexxar_Start), 255.00 )
    call SetUnitPositionLocFacingBJ( udg_Misha, GetRectCenter(gg_rct_Gameplay_Misha_Start), 255.00 )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Intro_Cleanup_Func020002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    call ConditionalTriggerExecute( gg_trg_Cinematics_AutoPause )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Intro_Cleanup_Func027001() ) then
        call SuspendHeroXPBJ( true, udg_Rexxar )
    else
        call DoNothing(  )
    endif
    // Special - Don't Restore Selection Until After Misha Is Summoned
    // Reset Sky
    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP1_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP1_Player, CameraSetupGetDestPositionLoc(gg_cam_Intro_Skipped), 0 )
    // Initial Harvesting
    // Wipe Visibility
    call CreateFogModifierRectBJ( true, udg_AP1_Player, FOG_OF_WAR_MASKED, GetPlayableMapRect() )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
    // Summon Misha
    call RemoveUnit( udg_Misha )
    call IssueImmediateOrderBJ( udg_Rexxar, "spiritwolf" )
endfunction

