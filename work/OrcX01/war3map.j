endfunction

function Trig_Treachery_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, GetPlayersAll() )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Treachery_Cinematic_Q_Func009001() ) then
        return
    else
        call DoNothing(  )
    endif
    call ConditionalTriggerExecute( gg_trg_Treachery_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Treachery_01A, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Treachery_01B, udg_AP1_Player, 30.00 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    set udg_TreacherySkipped = false
    call EnableTrigger( gg_trg_Treachery_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_CinematicRexxar, "TRIGSTR_025", gg_snd_D10Rexxar15, "TRIGSTR_026", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_D10Rexxar15, 0 )
    if ( Trig_Treachery_Cinematic_Q_Func024001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_EmissaryCorpse, "TRIGSTR_027", gg_snd_D10Emissary16, "TRIGSTR_028", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_D10Emissary16, 0 )
    if ( Trig_Treachery_Cinematic_Q_Func027001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Treachery_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Treachery_Cleanup )
    call CinematicModeBJ( false, GetPlayersAll() )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    set udg_InCinematic = false
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

