    // Set Sky
    call SetSkyModel( "Environment\\Sky\\LordaeronFallSky\\LordaeronFallSky.mdl" )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Rexxar )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Rexxar )
    call PauseUnitBJ( false, udg_Misha )
