    set udg_FurionReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingToFaceUnitTimed( udg_Furion, udg_Tyrande, 0 )
    // Tyrande Setup
    call SetUnitPositionLocFacingLocBJ( udg_Tyrande, GetRectCenter(gg_rct_FinalCinTyrande1), GetUnitLoc(udg_Illidan) )
    // Disable XP Gain
    // Disable Auras
    call SetPlayerAbilityAvailableBJ( false, 'AEah', Player(3) )
    call SetPlayerAbilityAvailableBJ( false, 'AEar', Player(2) )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Tyrande )
    call PauseUnitBJ( false, udg_Furion )
    call PauseUnitBJ( false, udg_Illidan )
    // Hide Units
    call GroupClear( udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_AP2_BluePlayer, Condition(function Trig_Victory_Setup_Func034001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(Player(2), Condition(function Trig_Victory_Setup_Func035001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P04_PurplePlayer, Condition(function Trig_Victory_Setup_Func036001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P05_YellowUndead, Condition(function Trig_Victory_Setup_Func037001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P06_OrangeUndead, Condition(function Trig_Victory_Setup_Func038001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P07_GreenUndead, Condition(function Trig_Victory_Setup_Func039001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P09_GreyUndead, Condition(function Trig_Victory_Setup_Func040001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P11_DarkGreenUndead, Condition(function Trig_Victory_Setup_Func041001002)), udg_HiddenUnits )
    call GroupAddGroup( GetUnitsOfPlayerMatching(udg_P12_BrownUndead, Condition(function Trig_Victory_Setup_Func042001002)), udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Tyrande, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Furion, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Illidan, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func046002 )
    // Hide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func048002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP2_BluePlayer), function Trig_Victory_Setup_Func050002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(Player(3)), function Trig_Victory_Setup_Func051002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P07_GreenUndead), function Trig_Victory_Setup_Func052002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P09_GreyUndead), function Trig_Victory_Setup_Func053002 )
    // Disable Shadowmeld and Auras
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', Player(2) )
    call SetPlayerAbilityAvailableBJ( false, 'AEah', udg_AP2_BluePlayer )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\HeroicVictory.mp3" )
endfunction

