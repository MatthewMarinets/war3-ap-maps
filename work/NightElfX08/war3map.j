function Trig_Dam_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG2_BluePlayer )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Dam_Cinematic_Q_Func009001() ) then
        return
    else
        call DoNothing(  )
    endif
    call ConditionalTriggerExecute( gg_trg_Dam_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Dam01, udg_AP2_BluePlayer, 0 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Dam_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitTypeWithNameBJ( GetPlayersAll(), udg_AP2_BluePlayer, 'nmyr', "TRIGSTR_237", GetRectCenter(gg_rct_Mine_Gold), gg_snd_S08Naga35, "TRIGSTR_238", bj_TIMETYPE_ADD, 0, false )
    call TriggerSleepAction( ( GetSoundDurationBJ(gg_snd_S08Naga35) / 2.00 ) )
    if ( Trig_Dam_Cinematic_Q_Func022001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_Dam02, udg_AP2_BluePlayer, 0 )
    call WaitForSoundBJ( gg_snd_S08Naga35, 0 )
    if ( Trig_Dam_Cinematic_Q_Func025001() ) then
        return
    else
        call DoNothing(  )
    endif
    if ( Trig_Dam_Cinematic_Q_Func026C() ) then
        call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Illidan, "TRIGSTR_146", gg_snd_S08Illidan36, "TRIGSTR_083", bj_TIMETYPE_ADD, 0, false )
        call WaitForSoundBJ( gg_snd_S08Illidan36, 0 )
        if ( Trig_Dam_Cinematic_Q_Func026Func003001() ) then
            return
        else
            call DoNothing(  )
        endif
    else
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Dam_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Dam_Cleanup )
    call CinematicModeBJ( false, udg_APG2_BluePlayer )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Quest update
    call PingMinimapLocForForce( GetPlayersAll(), GetRectCenter(gg_rct_PingTroll01), 7.00 )
    call PingMinimapLocForForce( GetPlayersAll(), GetRectCenter(gg_rct_PingTroll02), 7.00 )
    call PingMinimapLocForForce( GetPlayersAll(), GetRectCenter(gg_rct_PingTroll03), 7.00 )
    call SetCameraQuickPositionLocForPlayer( udg_AP2_BluePlayer, GetRectCenter(gg_rct_PingTroll01) )
    call SetCameraQuickPositionLocForPlayer( udg_AP2_BluePlayer, GetRectCenter(gg_rct_PingTroll02) )
    call SetCameraQuickPositionLocForPlayer( udg_AP2_BluePlayer, GetRectCenter(gg_rct_PingTroll03) )
    call ConditionalTriggerExecute( gg_trg_Quest_Dam_Discover )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    set udg_InCinematic = false
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
endfunction

