    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Furion, "TRIGSTR_190", gg_snd_S08Furion02, "TRIGSTR_191", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_S08Furion02, 5.50 )
    if ( Trig_Intro_Cinematic_Q_Func024001() ) then
        return
    else
        call DoNothing(  )
    endif
    call IssuePointOrderLocBJ( gg_unit_nhyc_0266, "move", GetRectCenter(gg_rct_IntroTurtleMove) )
