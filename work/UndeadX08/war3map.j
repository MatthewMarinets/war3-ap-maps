function Trig_Next_Level_Prep_Func007C takes nothing returns boolean
    if ( not ( GetGameDifficulty() == MAP_DIFFICULTY_HARD ) ) then
        return false
    endif
    if ( not ( udg_VictoryCheated == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Next_Level_Prep_Actions takes nothing returns nothing
    // Remove Quest Items
    // Save Heroes
    call InitGameCacheBJ( "Campaigns.w3v" )
    call StoreUnitBJ( udg_Arthas, "Arthas", "FinalX", GetLastCreatedGameCacheBJ() )
    call StoreUnitBJ( udg_Anub, "Anub", "FinalX", GetLastCreatedGameCacheBJ() )
    // Save credits flags
    if ( Trig_Next_Level_Prep_Func007C() ) then
        call StoreBooleanBJ( true, "VictoryHard", "CreditsEx", GetLastCreatedGameCacheBJ() )
    else
        call StoreBooleanBJ( true, "Victory", "CreditsEx", GetLastCreatedGameCacheBJ() )
    endif
    call SaveGameCacheBJ( GetLastCreatedGameCacheBJ() )
    // Enable Missions, Campaigns, Cinematics
    call SetMissionAvailableBJ( true, bj_MISSION_INDEX_XU13 )
    call SetCinematicAvailableBJ( true, bj_CINEMATICINDEX_XED )
    call SetNextLevelBJ( "Maps\\FrozenThrone\\Campaign\\War3XBonusCredits.w3x" )
endfunction

