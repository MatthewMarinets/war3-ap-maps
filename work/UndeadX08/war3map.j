    call TriggerSleepAction( 0.20 )
    call IssuePointOrderLocBJ( udg_Beetle02, "move", GetRectCenter(gg_rct_Intro_Beetle_02_Dest) )
    call TriggerSleepAction( 0.60 )
    call SetTerrainFogExBJ( 0, 1000.00, 6000.00, 0, 100.00, 100.00, 100.00 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call PlaySoundOnUnitBJ( gg_snd_HorseLoop2, 60.00, udg_Arthas )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Intro_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
