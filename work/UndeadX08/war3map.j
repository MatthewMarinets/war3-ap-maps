    return true
endfunction

function Trig_Illidan_Leaves_Home_Func023C takes nothing returns boolean
    if ( not ( GetGameDifficulty() == MAP_DIFFICULTY_HARD ) ) then
        return false
    endif
    return true
endfunction

function Trig_Illidan_Leaves_Home_Func025A takes nothing returns nothing
    call SetUnitPositionLocFacingBJ( GetEnumUnit(), udg_TempPointIllidan, 225.00 )
    call PauseUnitBJ( false, GetEnumUnit() )
endfunction

function Trig_Illidan_Leaves_Home_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    set udg_TempPointIllidan = GetUnitLoc(udg_Illidan)
    call PolledWait( 1.00 )
    // Defenders
    call CreateNUnitsAtLoc( 1, 'nmyr', udg_P07_COP_01, udg_TempPointIllidan, 225.00 )
    call RecycleGuardPosition( GetLastCreatedUnit() )
    call TriggerSleepAction( 0.30 )
    call CreateNUnitsAtLoc( 1, 'nnsw', udg_P07_COP_01, udg_TempPointIllidan, 225.00 )
    call RecycleGuardPosition( GetLastCreatedUnit() )
    call TriggerSleepAction( 0.30 )
    call CreateNUnitsAtLoc( 1, 'nnsw', udg_P07_COP_01, udg_TempPointIllidan, 225.00 )
