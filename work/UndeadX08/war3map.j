//===========================================================================
// Trigger: Defeat Units Dead
//===========================================================================
function Trig_Defeat_Units_Dead_Conditions takes nothing returns boolean
    if ( not ( GetPlayerStructureCount(udg_AP1_Player, true) == 0 ) ) then
        return false
    endif
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Defeat_Units_Dead_Func001002002001 takes nothing returns boolean
    return ( IsUnitAliveBJ(GetFilterUnit()) == true )
endfunction

function Trig_Defeat_Units_Dead_Func001002002002 takes nothing returns boolean
    return ( GetUnitTypeId(GetFilterUnit()) == 'ushd' )
endfunction

function Trig_Defeat_Units_Dead_Func001002002 takes nothing returns boolean
    return GetBooleanAnd( Trig_Defeat_Units_Dead_Func001002002001(), Trig_Defeat_Units_Dead_Func001002002002() )
endfunction

function Trig_Defeat_Units_Dead_Func002Func004C takes nothing returns boolean
    if ( ( GetPlayerUnitCount(udg_AP1_Player, false) == 0 ) ) then
        return true
    endif
    if ( ( GetPlayerUnitCount(udg_AP1_Player, false) == CountUnitsInGroup(udg_ShadeGroup) ) ) then
        return true
    endif
    return false
endfunction

function Trig_Defeat_Units_Dead_Func002C takes nothing returns boolean
    if ( not Trig_Defeat_Units_Dead_Func002Func004C() ) then
        return false
    endif
    return true
endfunction

function Trig_Defeat_Units_Dead_Actions takes nothing returns nothing
    set udg_ShadeGroup = GetUnitsOfPlayerMatching(udg_AP1_Player, Condition(function Trig_Defeat_Units_Dead_Func001002002))
    if ( Trig_Defeat_Units_Dead_Func002C() ) then
        call DestroyGroup( udg_ShadeGroup )
    else
        call DestroyGroup( udg_ShadeGroup )
        return
    endif
    set udg_GameOver = true
    call TriggerSleepAction( 2 )
    call ConditionalTriggerExecute( gg_trg_Mission_Failed_All_units_lost )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    call CustomDefeatBJ( udg_AP1_Player, "TRIGSTR_030" )
endfunction

