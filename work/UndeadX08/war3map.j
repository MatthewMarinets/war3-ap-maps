function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\FoggedSky\\FoggedSky.mdl" )
    // Clear Selection
    call ClearSelection(  )
    // Revive Hero *** Not Neccessary ***
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_WarderReturnSpot = udg_RevivalReturnSpot
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_FurionReturnSpot = udg_RevivalReturnSpot
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Arthas )
    call SuspendHeroXPBJ( false, udg_Anub )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Arthas )
    call PauseUnitBJ( false, udg_Illidan02 )
    call PauseUnitBJ( false, udg_Illidan03 )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerAll(udg_AP1_Player)
    call GroupRemoveUnitSimple( udg_Arthas, udg_HiddenUnits )
    call GroupAddUnitSimple( udg_Illidan, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func024002 )
    // Disable Auras
    call SetPlayerAbilityAvailableBJ( false, 'AUau', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( false, 'ACvp', Player(PLAYER_NEUTRAL_AGGRESSIVE) )
    // Remove Buffs
    set bj_wantDestroyGroup = true
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP1_Player), function Trig_Victory_Setup_Func030002 )
    set bj_wantDestroyGroup = true
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P02_IllidanMainBase), function Trig_Victory_Setup_Func032002 )
    // Disable Leaderboard
    call LeaderboardDisplayBJ( false, udg_LeaderboardObelisks )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\LichKingTheme.mp3" )
endfunction

