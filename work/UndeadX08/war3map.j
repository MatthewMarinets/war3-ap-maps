function Trig_Start_AI_Func069002 takes nothing returns nothing
    call RecycleGuardPosition( GetEnumUnit() )
endfunction

function Trig_Start_AI_Actions takes nothing returns nothing
    // Start All AI Scripts
    // Blue main
    call StartMeleeAI( udg_P02_IllidanMainBase, "u08x02.ai" )
    // Teal main
    call StartMeleeAI( udg_P03_LadyVashj, "u08x03.ai" )
    call CommandAI( udg_P03_LadyVashj, 0, 0 )
    // Purple main
    call StartMeleeAI( udg_P04_Kael, "u08x04.ai" )
    call CommandAI( udg_P04_Kael, 0, 0 )
    // Orange main Defend AI only
    call StartMeleeAI( udg_P06_IllidanDefendOnlyBase, "u08x06.ai" )
    call CommandAI( udg_P06_IllidanDefendOnlyBase, 0, 0 )
    // Yellow main Defend AI only
    call StartMeleeAI( udg_P05_IllidanDefense, "u08x05.ai" )
    call CommandAI( udg_P06_IllidanDefendOnlyBase, 0, 0 )
    // Green
    call StartMeleeAI( udg_P07_COP_01, "u08x07.ai" )
    // Pink
    call StartMeleeAI( udg_P08_COP_02, "u08x08.ai" )
    // Gray
    call StartMeleeAI( udg_P09_COP_03, "u08x09.ai" )
    // Light Blue
    call StartMeleeAI( udg_P10_COP_04, "u08x10.ai" )
    // ***********************
    // Start Illidan Territories Scripts
    call TriggerSleepAction( 0.25 )
    // COP 01 Green
    call CommandAI( udg_P02_IllidanMainBase, 1, R2I(GetRectCenterX(gg_rct_AI_COP01_AttackTarget)) )
    call CommandAI( udg_P02_IllidanMainBase, 2, R2I(GetRectCenterY(gg_rct_AI_COP01_AttackTarget)) )
    call TriggerSleepAction( 0.25 )
    // COP 02 Pink
    call CommandAI( udg_P02_IllidanMainBase, 3, R2I(GetRectCenterX(gg_rct_AI_COP02_AttackTarget)) )
    call CommandAI( udg_P02_IllidanMainBase, 4, R2I(GetRectCenterY(gg_rct_AI_COP02_AttackTarget)) )
    call TriggerSleepAction( 0.25 )
    // COP 03 Gray
    call CommandAI( udg_P02_IllidanMainBase, 5, R2I(GetRectCenterX(gg_rct_AI_COP03_AttackTarget)) )
    call CommandAI( udg_P02_IllidanMainBase, 6, R2I(GetRectCenterY(gg_rct_AI_COP03_AttackTarget)) )
    call TriggerSleepAction( 0.25 )
    // COP 04 Light Blue
    call CommandAI( udg_P02_IllidanMainBase, 7, R2I(GetRectCenterX(gg_rct_AI_COP04_AttackTarget)) )
    call CommandAI( udg_P02_IllidanMainBase, 8, R2I(GetRectCenterY(gg_rct_AI_COP04_AttackTarget)) )
    call TriggerSleepAction( 0.25 )
    // Start Individual Bases
    // COP 01 Green
    call CommandAI( udg_P07_COP_01, 1, R2I(GetRectCenterX(gg_rct_AI_COP01_DefenseArea)) )
    call CommandAI( udg_P07_COP_01, 2, R2I(GetRectCenterY(gg_rct_AI_COP01_DefenseArea)) )
    call TriggerSleepAction( 0.25 )
    // COP 02 Pink
    call CommandAI( udg_P08_COP_02, 1, R2I(GetRectCenterX(gg_rct_AI_COP02_DefenseArea)) )
    call CommandAI( udg_P08_COP_02, 2, R2I(GetRectCenterY(gg_rct_AI_COP02_DefenseArea)) )
    call TriggerSleepAction( 0.25 )
    // COP 03 Gray
    call CommandAI( udg_P09_COP_03, 1, R2I(GetRectCenterX(gg_rct_AI_COP03_DefenseArea)) )
    call CommandAI( udg_P09_COP_03, 2, R2I(GetRectCenterY(gg_rct_AI_COP03_DefenseArea)) )
    call TriggerSleepAction( 0.25 )
    // COP 04 Light Blue
    call CommandAI( udg_P10_COP_04, 1, R2I(GetRectCenterX(gg_rct_AI_COP04_DefenseArea)) )
    call CommandAI( udg_P10_COP_04, 2, R2I(GetRectCenterY(gg_rct_AI_COP04_DefenseArea)) )
    call TriggerSleepAction( 0.25 )
    // COP 04 Light Blue Lost
    call CommandAI( udg_P02_IllidanMainBase, 10, 4 )
    // COP 03 Gray Lost
    call CommandAI( udg_P02_IllidanMainBase, 10, 3 )
    // COP 02 Pink Lost
    call CommandAI( udg_P02_IllidanMainBase, 10, 2 )
    // COP 01 Green Lost
    call CommandAI( udg_P02_IllidanMainBase, 10, 1 )
    // ***********************
    call ForGroupBJ( GetUnitsInRectAll(gg_rct_Recycle), function Trig_Start_AI_Func069002 )
endfunction

