//===========================================================================
// Trigger: Load Thrall
//
// Attempt to load the hero.
// Upon failure, create a default version of the hero.
// Special case:  Thrall cannot attain level 6 on his own prior to this mission, as we cap his level to 5 on Orc03.  To correct this, if he is successfully loaded from the cache, we give him enough xp to boost him from level 5 to level 6.
//===========================================================================
function Trig_Load_Thrall_Func004001 takes nothing returns boolean
    return ( udg_Thrall != null )
endfunction

function Trig_Load_Thrall_Func005001 takes nothing returns boolean
    return ( udg_Thrall != null )
endfunction

function Trig_Load_Thrall_Actions takes nothing returns nothing
    call CommentString( "Load the hero data" )
    call RestoreUnitLocFacingAngleBJ( "Thrall", "Orc06", GetLastCreatedGameCacheBJ(), Player(0), GetRectCenter(gg_rct_Thrall_Start_Spot), 202.67 )
    set udg_Thrall = GetLastRestoredUnitBJ()
    if ( Trig_Load_Thrall_Func004001() ) then
        call AddHeroXPSwapped( 600, udg_Thrall, false )
    else
        call DoNothing(  )
    endif
    if ( Trig_Load_Thrall_Func005001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CommentString( "If the hero data wasn't found, create a default hero" )
    call CreateNUnitsAtLoc( 1, 'Othr', Player(0), GetRectCenter(gg_rct_Thrall_Start_Spot), 202.67 )
    set udg_Thrall = GetLastCreatedUnit()
    call SetHeroLevel( udg_Thrall, 6, false )
    call SelectHeroSkill( udg_Thrall, 'AOcl' )
    call SelectHeroSkill( udg_Thrall, 'AOfs' )
    call SelectHeroSkill( udg_Thrall, 'AOsf' )
    call SelectHeroSkill( udg_Thrall, 'AOcl' )
    call SelectHeroSkill( udg_Thrall, 'AOsf' )
    call SelectHeroSkill( udg_Thrall, 'AOeq' )
endfunction

