function Trig_Check_For_Possessed_Units_Actions takes nothing returns nothing
    if ( Trig_Check_For_Possessed_Units_Func003C() ) then
        // Gnolls
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_GnollGroup01 )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_GnollGroup01Temp )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_GnollGroup02 )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_GnollGroup02Temp )
        // Trolls
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_TrollGroup01 )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_TrollGroup01Temp )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_TrollGroup02 )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_TrollGroup02Temp )
        // Murloc
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_MurlocGroup01 )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_MurlocGroup01Temp )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_MurlocGroup02 )
        call GroupRemoveUnitSimple( GetChangingUnit(), udg_MurlocGroup02Temp )
        if ( Trig_Check_For_Possessed_Units_Func003Func016C() ) then
            // Ogre
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup01 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup01Temp )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup02 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup02Temp )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup03 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup03Temp )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup04 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup05 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_OgreGroup05Temp )
        else
        endif
        if ( Trig_Check_For_Possessed_Units_Func003Func017C() ) then
            // Bandits
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup01 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup02 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup03 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup04 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup05 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup06 )
            call GroupRemoveUnitSimple( GetChangingUnit(), udg_BanditGroup07 )
        else
        endif
    else
    endif
    if ( Trig_Check_For_Possessed_Units_Func004001() ) then
        set udg_Ogre01 = null
    else
        call DoNothing(  )
    endif
    if ( Trig_Check_For_Possessed_Units_Func005001() ) then
        set udg_Ogre02 = null
    else
        call DoNothing(  )
    endif
    if ( Trig_Check_For_Possessed_Units_Func006001() ) then
        set udg_Ogre03 = null
    else
        call DoNothing(  )
    endif
endfunction

