//===========================================================================
// Trigger: Initialization
//===========================================================================
function Trig_Initialization_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_XU )
    call SetAllyColorFilterState( 0 )
    call ConditionalTriggerExecute( gg_trg_Init_01_Players )
    call ConditionalTriggerExecute( gg_trg_Init_02_Units )
    call ConditionalTriggerExecute( gg_trg_Init_03_Music )
    call ConditionalTriggerExecute( gg_trg_Init_04_Environment )
    call ConditionalTriggerExecute( gg_trg_Init_05_Quests )
    call ConditionalTriggerExecute( gg_trg_Init_06a_Hard )
    call ConditionalTriggerExecute( gg_trg_Init_06b_Normal )
    call ConditionalTriggerExecute( gg_trg_Init_06c_Easy )
    call ConditionalTriggerExecute( gg_trg_Init_07_Regions_to_Points )
    // Start Intro Cinematic
    call QueuedTriggerAddBJ( gg_trg_Intro_Cinematic_Q, true )
endfunction

//===========================================================================
function InitTrig_Initialization takes nothing returns nothing
    set gg_trg_Initialization = CreateTrigger(  )
    call TriggerAddAction( gg_trg_Initialization, function Trig_Initialization_Actions )
endfunction

//===========================================================================
// Trigger: Init 01 Players
//===========================================================================
function Trig_Init_01_Players_Func036002 takes nothing returns nothing
    call SetUnitColor( GetEnumUnit(), ConvertPlayerColor(12) )
endfunction

function Trig_Init_01_Players_Func037002 takes nothing returns nothing
    call SetUnitColor( GetEnumUnit(), ConvertPlayerColor(12) )
endfunction

function Trig_Init_01_Players_Actions takes nothing returns nothing
    // Alliance Settings
    call SetPlayerAllianceStateBJ( udg_P1_Mugthol, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P4_Undead, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P5_Blackthorn, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P6_Varimathras, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P1_Mugthol, udg_P10_Neutral_LightBlue, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P4_Undead, udg_P10_Neutral_LightBlue, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P5_Blackthorn, udg_P10_Neutral_LightBlue, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P6_Varimathras, udg_P10_Neutral_LightBlue, bj_ALLIANCE_NEUTRAL )
    // -
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P1_Mugthol, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P4_Undead, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P5_Blackthorn, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P6_Varimathras, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_P10_Neutral_LightBlue, bj_ALLIANCE_NEUTRAL )
    // -
    call SetPlayerAllianceStateBJ( udg_P10_Neutral_LightBlue, Player(PLAYER_NEUTRAL_AGGRESSIVE), bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P10_Neutral_LightBlue, udg_P1_Mugthol, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P10_Neutral_LightBlue, udg_P4_Undead, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P10_Neutral_LightBlue, udg_P5_Blackthorn, bj_ALLIANCE_NEUTRAL )
    call SetPlayerAllianceStateBJ( udg_P10_Neutral_LightBlue, udg_P6_Varimathras, bj_ALLIANCE_NEUTRAL )
    // Creeps
    call EnableCreepSleepBJ( false )
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', Player(PLAYER_NEUTRAL_AGGRESSIVE) )
    // Initial Resources
    call SetPlayerStateBJ( udg_AP2_Player, PLAYER_STATE_RESOURCE_GOLD, 500 )
    call SetPlayerStateBJ( udg_AP2_Player, PLAYER_STATE_RESOURCE_LUMBER, 150 )
    call SetPlayerStateBJ( udg_P6_Varimathras, PLAYER_STATE_RESOURCE_GOLD, 5000 )
    call SetPlayerStateBJ( udg_P6_Varimathras, PLAYER_STATE_RESOURCE_LUMBER, 5000 )
    call SetPlayerStateBJ( udg_P4_Undead, PLAYER_STATE_RESOURCE_GOLD, 5000 )
    call SetPlayerStateBJ( udg_P4_Undead, PLAYER_STATE_RESOURCE_LUMBER, 5000 )
    // Player Colors
    call SetPlayerColorBJ( udg_P6_Varimathras, PLAYER_COLOR_RED, true )
    call SetPlayerColorBJ( udg_P1_Mugthol, PLAYER_COLOR_CYAN, true )
    call SetPlayerColorBJ( udg_P5_Blackthorn, PLAYER_COLOR_YELLOW, true )
    call ForGroup( GetUnitsInRectOfPlayer(GetPlayableMapRect(), udg_P1_Mugthol), function Trig_Init_01_Players_Func036002 )
    call ForGroup( GetUnitsInRectOfPlayer(GetPlayableMapRect(), udg_P5_Blackthorn), function Trig_Init_01_Players_Func037002 )
    // Hide Excess Players
    call SetPlayerOnScoreScreenBJ( false, udg_P1_Mugthol )
    call SetPlayerOnScoreScreenBJ( false, udg_P4_Undead )
    call SetPlayerOnScoreScreenBJ( false, udg_P5_Blackthorn )
    call SetPlayerOnScoreScreenBJ( false, udg_P10_Neutral_LightBlue )
endfunction

