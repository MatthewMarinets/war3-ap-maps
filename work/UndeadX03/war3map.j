//===========================================================================
// Trigger: Init 02 Units
//===========================================================================
function Trig_Init_02_Units_Func018C takes nothing returns boolean
    if ( not ( GetGameDifficulty() == MAP_DIFFICULTY_HARD ) ) then
        return false
    endif
    return true
endfunction

function Trig_Init_02_Units_Func026002 takes nothing returns nothing
    call RemoveGuardPosition( GetEnumUnit() )
endfunction

function Trig_Init_02_Units_Actions takes nothing returns nothing
    // Heroes
    call ConditionalTriggerExecute( gg_trg_Load_Heroes )
    call SetPlayerHandicapXPBJ( udg_AP2_Player, 20.00 )
    // NPCs
    set udg_UnitOgreWarlord = gg_unit_n001_0196
    set udg_UnitBanditLord = gg_unit_n003_0203
    set udg_Varimathras = gg_unit_Uvar_0186
    call SuspendHeroXPBJ( false, udg_Varimathras )
    // Neutral Building
    call SetItemTypeSlots( gg_unit_nmrk_0002, 0 )
    set udg_UnitMercenaryCamp01 = gg_unit_nmrk_0002
    call SetItemTypeSlots( gg_unit_nmrk_0078, 0 )
    set udg_UnitMercenaryCamp02 = gg_unit_nmrk_0078
    // -
    call AddUnitToStockBJ( 'nftr', udg_UnitMercenaryCamp01, 3, 3 )
    call AddUnitToStockBJ( 'nftr', udg_UnitMercenaryCamp02, 3, 3 )
    // Varimathras Forces
    if ( Trig_Init_02_Units_Func018C() ) then
        call CreateNUnitsAtLoc( 1, 'uslh', udg_P6_Varimathras, GetRectCenter(gg_rct_Hard_Slaughter_House), bj_UNIT_FACING )
    else
    endif
