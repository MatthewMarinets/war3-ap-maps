function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\LordaeronWinterSkyBrightGreen\\LordaeronWinterSkyBrightGreen.mdl" )
    // Clear Selection
    call ClearSelection(  )
    // Alliance Settings
    call SetPlayerAllianceStateBJ( udg_AP2_Player, udg_P6_Varimathras, bj_ALLIANCE_ALLIED_VISION )
    call SetPlayerAllianceStateBJ( udg_P6_Varimathras, udg_AP2_Player, bj_ALLIANCE_ALLIED_VISION )
    call SetPlayerAbilityAvailableBJ( false, 'AUav', udg_P6_Varimathras )
    // Revive Hero
    set udg_RevivalHero = udg_Sylvanas
    set udg_RevivalSpot = GetRectCenter(gg_rct_SylvanasBlockB)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_SylvanasReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingTimed( udg_Sylvanas, 0.00, 0 )
    // Revive Hero
    set udg_RevivalHero = udg_Varimathras
    set udg_RevivalSpot = GetRectCenter(gg_rct_SylvanasBlockA)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_VarimathrasReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingTimed( udg_Varimathras, 270.00, 0 )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Sylvanas )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func026002 )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_Sylvanas )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerAll(udg_AP2_Player)
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_P6_Varimathras), udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Sylvanas, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Varimathras, udg_HiddenUnits )
    call ForGroup( udg_HiddenUnits, function Trig_Victory_Setup_Func035002 )
    // Remove Buffs
    call ForGroup( GetUnitsOfPlayerAll(udg_AP2_Player), function Trig_Victory_Setup_Func037002 )
    call ForGroup( GetUnitsOfPlayerAll(udg_P6_Varimathras), function Trig_Victory_Setup_Func038002 )
    call ForGroup( GetUnitsOfPlayerAll(udg_P4_Undead), function Trig_Victory_Setup_Func039002 )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\DarkVictory.mp3" )
endfunction

