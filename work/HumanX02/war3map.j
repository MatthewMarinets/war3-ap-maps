    call CreateNUnitsAtLoc( 1, 'uzg1', udg_WhichPlayer, GetRectCenter(gg_rct_TRTower03), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'uzg1', udg_WhichPlayer, GetRectCenter(gg_rct_TRTower04), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'usep', udg_WhichPlayer, GetRectCenter(gg_rct_TRCrypt), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'ugrv', udg_WhichPlayer, GetRectCenter(gg_rct_TRGraveyard), bj_UNIT_FACING )
    call BlightGoldMineForPlayer( GroupPickRandomUnit(GetUnitsInRectMatching(gg_rct_TRHauntedMine, Condition(function Trig_Create_Undead_Towns_Top_Func038001001002))), udg_WhichPlayer )
