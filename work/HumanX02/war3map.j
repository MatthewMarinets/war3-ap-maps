    call ForGroupBJ( udg_HiddenUnits, function Trig_Naga_Come_Back_Cleanup_Func010002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Naga_Come_Back_Cleanup_Func016001() ) then
        call SuspendHeroXPBJ( true, udg_Kael )
    else
        call DoNothing(  )
    endif
    // Return Heroes
    call SetUnitPositionLoc( udg_Kael, udg_KaelReturnSpot )
    call SetUnitPositionLoc( udg_SpellBreaker, GetRectCenter(gg_rct_SpellBreaker) )
