    call CreateNUnitsAtLoc( 1, 'uzg1', udg_WhichPlayer, GetRectCenter(gg_rct_TLTower02), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'uzg1', udg_WhichPlayer, GetRectCenter(gg_rct_TLTower03), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'uzig', udg_WhichPlayer, GetRectCenter(gg_rct_TLTower04), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'uzig', udg_WhichPlayer, GetRectCenter(gg_rct_TLTower06), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'uaod', udg_WhichPlayer, GetRectCenter(gg_rct_TLAltar), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'usep', udg_WhichPlayer, GetRectCenter(gg_rct_TLCrypt), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'ugrv', udg_WhichPlayer, GetRectCenter(gg_rct_TLGraveyard), bj_UNIT_FACING )
    call CreateNUnitsAtLoc( 1, 'utod', udg_WhichPlayer, GetRectCenter(gg_rct_TLTemple), bj_UNIT_FACING )
    if ( Trig_Create_Undead_Towns_Top_Func022C() ) then
        call CreateNUnitsAtLoc( 1, 'usep', udg_WhichPlayer, GetRectCenter(gg_rct_TLHARDCrypt), bj_UNIT_FACING )
        call CreateNUnitsAtLoc( 1, 'uslh', udg_WhichPlayer, GetRectCenter(gg_rct_TLHARDSlaughterHouse), bj_UNIT_FACING )
    else
    endif
    call BlightGoldMineForPlayer( GroupPickRandomUnit(GetUnitsInRectMatching(gg_rct_TLHauntedMine, Condition(function Trig_Create_Undead_Towns_Top_Func023001001002))), udg_WhichPlayer )
    // #############################################
