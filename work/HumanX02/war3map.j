function Trig_Bottom_Waves_Actions takes nothing returns nothing
    // Start Attacking
    set udg_BottomAttacking = true
    call ConditionalTriggerExecute( gg_trg_Spawn_Bottom_Waves )
    // Clear Town 01
    set udg_ClearingRegionBottom = gg_rct_Bottom_Attack_Spot_01
    set udg_DoneCleaningBottom = false
    call TriggerExecute( gg_trg_Bottom_Keep_Attacking )
    loop
        exitwhen ( Trig_Bottom_Waves_Func008001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 1.00))
    endloop
    // Clear Town 02
    set udg_ClearingRegionBottom = gg_rct_Bottom_Attack_Spot_02
    set udg_DoneCleaningBottom = false
    call TriggerExecute( gg_trg_Bottom_Keep_Attacking )
    loop
        exitwhen ( Trig_Bottom_Waves_Func013001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 1.00))
    endloop
    // Stop Attacking
    set udg_BottomAttacking = false
    set udg_BottomDone = true
    call ConditionalTriggerExecute( gg_trg_Naga_Cinematic )
    // Bottom Town 01
    call ForGroupBJ( udg_BottomUnitGroupTown01, function Trig_Bottom_Waves_Func019A )
    call GroupRemoveGroup( GetUnitsInRectOfPlayer(gg_rct_BottomTown01_Guards, udg_P5_Undead), udg_BottomUnitGroupTown01 )
    // Bottom Town 02
    call ForGroupBJ( udg_BottomUnitGroupTown02, function Trig_Bottom_Waves_Func022A )
    call GroupRemoveGroup( GetUnitsInRectOfPlayer(gg_rct_BottomTown02_Guards, udg_P5_Undead), udg_BottomUnitGroupTown02 )
    // Bottom Leave
    if ( Trig_Bottom_Waves_Func025C() ) then
        call GroupPointOrderLocBJ( udg_BottomUnitGroupLeave, "attack", udg_zPointBottomLeaveSpot )
    else
        call ForGroupBJ( udg_BottomUnitGroupLeave, function Trig_Bottom_Waves_Func025Func001A )
    endif
    call TriggerSleepAction( 20.00 )
    if ( Trig_Bottom_Waves_Func027C() ) then
        call GroupPointOrderLocBJ( udg_BottomUnitGroupLeave, "move", udg_zPointBottomLeaveSpot )
    else
        call ForGroupBJ( udg_BottomUnitGroupLeave, function Trig_Bottom_Waves_Func027Func001A )
    endif
endfunction

