function Trig_Top_Waves_Actions takes nothing returns nothing
    // Start Attacking
    set udg_TopAttacking = true
    call ConditionalTriggerExecute( gg_trg_Spawn_Top_Waves )
    // Clear Town 01
    set udg_ClearingRegionTop = gg_rct_Top_Attack_Spot_01
    set udg_DoneCleaningTop = false
    call TriggerExecute( gg_trg_Top_Keep_Attacking )
    loop
        exitwhen ( Trig_Top_Waves_Func008001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 1.00))
    endloop
    // Clear Town 02
    set udg_ClearingRegionTop = gg_rct_Top_Attack_Spot_02
    set udg_DoneCleaningTop = false
    call TriggerExecute( gg_trg_Top_Keep_Attacking )
    loop
        exitwhen ( Trig_Top_Waves_Func013001() )
        call TriggerSleepAction(RMaxBJ(bj_WAIT_FOR_COND_MIN_INTERVAL, 1.00))
    endloop
    // Stop Attacking
    set udg_TopAttacking = false
    set udg_TopDone = true
    call ConditionalTriggerExecute( gg_trg_Naga_Cinematic )
    // Top Town 01
    call ForGroupBJ( udg_TopUnitGroupTown01, function Trig_Top_Waves_Func019A )
    call GroupRemoveGroup( GetUnitsInRectOfPlayer(gg_rct_TopTown01_Guards, udg_P5_Undead), udg_TopUnitGroupTown01 )
    // Top Town 02
    call ForGroupBJ( udg_TopUnitGroupTown02, function Trig_Top_Waves_Func022A )
    call GroupRemoveGroup( GetUnitsInRectOfPlayer(gg_rct_TopTown02_Guards, udg_P6_Undead), udg_TopUnitGroupTown02 )
    // Top Leave
    if ( Trig_Top_Waves_Func025C() ) then
        call GroupPointOrderLocBJ( udg_TopUnitGroupLeave, "attack", udg_zPointTopLeaveSpot )
    else
        call ForGroupBJ( udg_TopUnitGroupLeave, function Trig_Top_Waves_Func025Func001A )
    endif
    call TriggerSleepAction( 20.00 )
    if ( Trig_Top_Waves_Func027C() ) then
        call GroupPointOrderLocBJ( udg_TopUnitGroupLeave, "move", udg_zPointTopLeaveSpot )
    else
        call ForGroupBJ( udg_TopUnitGroupLeave, function Trig_Top_Waves_Func027Func001A )
    endif
endfunction

