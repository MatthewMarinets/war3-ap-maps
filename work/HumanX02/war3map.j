    // Computer Town P6   TOP RIGHT
    set udg_WhichPlayer = udg_P6_Undead
    call SetBlightRadiusLocBJ( true, udg_WhichPlayer, GetRectCenter(gg_rct_TRBlightBombA), 1300.00 )
    call CreateNUnitsAtLoc( 1, 'uzig', udg_WhichPlayer, GetRectCenter(gg_rct_TRTower01), bj_UNIT_FACING )
