    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP1_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP1_Player, CameraSetupGetDestPositionLoc(gg_cam_Naga_Skipped), 0 )
    // Give Naga Units to Player
    call SetUnitPositionLoc( udg_LadyVashj, GetRectCenter(gg_rct_LadyVashjAfter) )
    call SetUnitOwner( udg_LadyVashj, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit01, GetRectCenter(gg_rct_NagaUnit01After) )
    call SetUnitOwner( udg_NagaUnit01, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit02, GetRectCenter(gg_rct_NagaUnit02After) )
    call SetUnitOwner( udg_NagaUnit02, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit03, GetRectCenter(gg_rct_NagaUnit03After) )
    call SetUnitOwner( udg_NagaUnit03, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit04, GetRectCenter(gg_rct_NagaUnit04After) )
    call SetUnitOwner( udg_NagaUnit04, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit05, GetRectCenter(gg_rct_NagaUnit05After) )
    call SetUnitOwner( udg_NagaUnit05, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit06, GetRectCenter(gg_rct_NagaUnit06After) )
    call SetUnitOwner( udg_NagaUnit06, Player(0), true )
    call SetUnitPositionLoc( udg_NagaUnit07, GetRectCenter(gg_rct_NagaUnit07After) )
    call SetUnitOwner( udg_NagaUnit07, Player(0), true )
    // Give the Player Money
    if ( Trig_Naga_Come_Back_Cleanup_Func046C() ) then
        call AdjustPlayerStateBJ( 500, udg_AP1_Player, PLAYER_STATE_RESOURCE_GOLD )
    else
