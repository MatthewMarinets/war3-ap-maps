    // Computer Town P5   TOP LEFT
    set udg_WhichPlayer = udg_P5_Undead
    call SetBlightRadiusLocBJ( true, udg_WhichPlayer, GetRectCenter(gg_rct_TLBlightBombA), 1500.00 )
    call SetBlightRadiusLocBJ( true, udg_WhichPlayer, GetRectCenter(gg_rct_TLBlightBombB), 1200.00 )
