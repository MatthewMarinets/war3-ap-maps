function Trig_Ending_Cinematic_Q_Actions takes nothing returns nothing
    // Fade Out
    set udg_InCinematic = true
    call ConditionalTriggerExecute( gg_trg_Quest_Crush_and_Destroy_Completed )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call SetCameraBoundsToRectForPlayerBJ( Player(0), GetEntireMapRect() )
    call CinematicModeBJ( true, GetPlayersAll() )
    call TriggerSleepAction( 0.25 )
    call ConditionalTriggerExecute( gg_trg_Next_Level_Prep )
    call ConditionalTriggerExecute( gg_trg_Ending_Setup )
    call CameraSetupApplyForPlayer( true, gg_cam_ENDIntro01, udg_AP1_Player, 0 )
    call SetCinematicCameraForPlayer( Player(0), "cinimatic.mdl" )
    call TriggerSleepAction( 0.25 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 2.20, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 2.20 )
    call EnableTrigger( gg_trg_Ending_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call CameraSetupApplyForPlayer( true, gg_cam_ENDIntro02, udg_AP1_Player, ( GetSoundDurationBJ(gg_snd_A02Kael24) + 3.00 ) )
    call ShowUnitHide( udg_KaelUnit09 )
    call ShowUnitHide( udg_GarUnit13 )
    call ShowUnitHide( udg_GarUnit16 )
    call ShowUnitHide( udg_GarUnit06 )
    call ShowUnitHide( udg_GarUnit05 )
    call ShowUnitHide( udg_GarUnit04 )
    call ShowUnitHide( udg_GarUnit03 )
    call ShowUnitHide( udg_GarUnit12 )
