    call CinematicModeBJ( true, udg_APG1_Player )
    call TriggerSleepAction( 0.25 )
    if ( Trig_Furbolg_Cinematic_Q_Func010C() ) then
        return
    else
    endif
    call ConditionalTriggerExecute( gg_trg_Furbolg_Setup )
