    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call VolumeGroupSetVolumeBJ( SOUND_VOLUMEGROUP_COMBAT, 70.00 )
    call VolumeGroupSetVolumeBJ( SOUND_VOLUMEGROUP_UNITSOUNDS, 70.00 )
    call EnableTrigger( gg_trg_Furbolg_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Speaker, "TRIGSTR_265", gg_snd_S01WatcherTwo18, "TRIGSTR_266", bj_TIMETYPE_ADD, 0.00, false )
    call PlaySoundAtPointBJ( gg_snd_FurbolgWhat3, 60.00, GetRectCenter(gg_rct_Furbolg_Mad_Area01), 0 )
