endfunction

function Trig_Init_02_Units_Func011002 takes nothing returns nothing
    call GroupAddUnitSimple( GetEnumUnit(), udg_NagaVictoryGroup )
endfunction

function Trig_Init_02_Units_Func013002 takes nothing returns nothing
    call ShowUnitHide( GetEnumUnit() )
endfunction

function Trig_Init_02_Units_Func014001002001 takes nothing returns boolean
    return ( GetOwningPlayer(GetFilterUnit()) == Player(PLAYER_NEUTRAL_PASSIVE) )
endfunction

function Trig_Init_02_Units_Func014001002002 takes nothing returns boolean
    return ( GetUnitTypeId(GetFilterUnit()) == 'nder' )
endfunction

function Trig_Init_02_Units_Func014001002 takes nothing returns boolean
    return GetBooleanAnd( Trig_Init_02_Units_Func014001002001(), Trig_Init_02_Units_Func014001002002() )
endfunction

function Trig_Init_02_Units_Func014002 takes nothing returns nothing
    call GroupAddUnitSimple( GetEnumUnit(), udg_StagGroup )
endfunction

function Trig_Init_02_Units_Func015002 takes nothing returns nothing
    call PauseUnitBJ( true, GetEnumUnit() )
endfunction

function Trig_Init_02_Units_Actions takes nothing returns nothing
    // Heroes
    call ConditionalTriggerExecute( gg_trg_Load_Heroes )
    // NPCs
    // Creeps
    call IssueImmediateOrderBJ( gg_unit_nder_0001, "holdposition" )
    call EnableCreepSleepBJ( false )
    call SetPlayerFlagBJ( PLAYER_STATE_GIVES_BOUNTY, false, Player(PLAYER_NEUTRAL_AGGRESSIVE) )
    // Hide Units
    call ForGroupBJ( GetUnitsInRectAll(gg_rct_Wildkin_Archer_Area_Group), function Trig_Init_02_Units_Func009002 )
    call ForGroupBJ( GetUnitsInRectOfPlayer(gg_rct_Hide_Ships, udg_AP3_FishingVillage), function Trig_Init_02_Units_Func010002 )
    call ForGroupBJ( GetUnitsInRectOfPlayer(gg_rct_NagaVictoryGroupArea, udg_AP4_Naga), function Trig_Init_02_Units_Func011002 )
    set udg_WildkinAreaGroup = GetUnitsInRectAll(gg_rct_Wildkin_Archer_Area_Group)
