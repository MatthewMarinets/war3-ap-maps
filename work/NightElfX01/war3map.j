function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\LordaeronSummerSky\\LordaeronSummerSky.mdl" )
    // Clear Selection
    call ClearSelection(  )
    // Setup Units
    call CreateNUnitsAtLoc( 1, 'earc', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Archer), 45.00 )
    set udg_VictoryArcher = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'ensh', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Huntress), 85.00 )
    set udg_VictoryHuntress = GetLastCreatedUnit()
    call SetUnitPositionLoc( udg_Maiev, GetRectCenter(gg_rct_Victory_Maiev) )
    call SetUnitFacingTimed( udg_Maiev, 230.00, 0 )
    call CreateNUnitsAtLoc( 1, 'earc', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Archer01_Spawn), 45.00 )
    set udg_VictoryArcher01 = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'earc', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Archer02_Spawn), 45.00 )
    set udg_VictoryArcher02 = GetLastCreatedUnit()
    call CreateNUnitsAtLoc( 1, 'earc', udg_AP1_Player, GetRectCenter(gg_rct_Victory_Archer03_Spawn), 45.00 )
    set udg_VictoryArcher03 = GetLastCreatedUnit()
    // Disable XP Gain
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call PauseUnitBJ( false, udg_VictoryArcher )
    call PauseUnitBJ( false, udg_VictoryArcher01 )
    call PauseUnitBJ( false, udg_VictoryArcher02 )
    call PauseUnitBJ( false, udg_VictoryArcher03 )
    call PauseUnitBJ( false, udg_VictoryHuntress )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerAll(udg_AP1_Player)
    call GroupAddGroup( GetUnitsOfPlayerAll(udg_AP4_Naga), udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_VictoryArcher, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_VictoryHuntress, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Maiev, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_VictoryArcher01, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_VictoryArcher02, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_VictoryArcher03, udg_HiddenUnits )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func037002 )
    call LeaderboardDisplayBJ( false, udg_LeaderboardShips )
    // Hide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Victory_Setup_Func040002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP1_Player), function Trig_Victory_Setup_Func042002 )
    // Disable Abilities
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', udg_AP1_Player )
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', udg_AP2_Trackers )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\PursuitTheme.mp3" )
endfunction

