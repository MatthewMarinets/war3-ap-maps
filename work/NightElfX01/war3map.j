    if ( Trig_Furbolg_Cinematic_Q_Func034001() ) then
        return
    else
        call DoNothing(  )
    endif
    call PlaySoundAtPointBJ( gg_snd_FurbolgPissed3, 60.00, GetRectCenter(gg_rct_Furbolg_Mad_Area01), 0 )
    call WaitForSoundBJ( gg_snd_S01WatcherTwo18, 0.00 )
    if ( Trig_Furbolg_Cinematic_Q_Func037001() ) then
        return
    else
        call DoNothing(  )
    endif
    call PlaySoundAtPointBJ( gg_snd_FurbolgPissed1, 60.00, GetRectCenter(gg_rct_Furbolg_Mad_Area01), 0 )
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Maiev, "TRIGSTR_364", gg_snd_S01Maiev19, "TRIGSTR_268", bj_TIMETYPE_ADD, 0.00, false )
    call WaitForSoundBJ( gg_snd_S01Maiev19, 4.00 )
    if ( Trig_Furbolg_Cinematic_Q_Func041001() ) then
        return
    else
        call DoNothing(  )
    endif
    call PlaySoundAtPointBJ( gg_snd_FurbolgYes1, 60.00, GetRectCenter(gg_rct_Furbolg_Mad_Area01), 0 )
    call WaitForSoundBJ( gg_snd_S01Maiev19, 2.00 )
    if ( Trig_Furbolg_Cinematic_Q_Func044001() ) then
        return
    else
        call DoNothing(  )
    endif
    call PlaySoundAtPointBJ( gg_snd_FurbolgYesAttack1, 60.00, GetRectCenter(gg_rct_Furbolg_Mad_Area01), 0 )
    call WaitForSoundBJ( gg_snd_S01Maiev19, 0.00 )
    if ( Trig_Furbolg_Cinematic_Q_Func047001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetSpeechVolumeGroupsBJ(  )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Furbolg_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
