//===========================================================================
function Trig_Map_Initialization_Actions takes nothing returns nothing
    call SetCampaignMenuRaceBJ( bj_CAMPAIGN_INDEX_O )
    call CommentString( "Difficulty" )
    call ConditionalTriggerExecute( gg_trg_Difficulty_Easy )
    call ConditionalTriggerExecute( gg_trg_Difficulty_Normal )
    call ConditionalTriggerExecute( gg_trg_Difficulty_Hard )
    call CommentString( "***********************" )
    call ConditionalTriggerExecute( gg_trg_Run_Hungry_Set_Arrays_Swap_Loop )
    call ConditionalTriggerExecute( gg_trg_Load_Heroes )
    call ConditionalTriggerExecute( gg_trg_Opening01 )
    call ConditionalTriggerExecute( gg_trg_SetupQuests )
    call ConditionalTriggerExecute( gg_trg_SetupEnvironment )
    call ConditionalTriggerExecute( gg_trg_Setup_Creatures )
    call ConditionalTriggerExecute( gg_trg_Setup_Players )
    call ConditionalTriggerExecute( gg_trg_SetupAszune )
    call ConditionalTriggerExecute( gg_trg_SetupSpectralBridge )
    call ConditionalTriggerExecute( gg_trg_SetupDoors )
