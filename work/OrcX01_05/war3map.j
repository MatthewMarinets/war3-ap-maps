function Trig_MeetDrekthar_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, GetPlayersAll() )
    call TriggerSleepAction( 0.25 )
    if ( Trig_MeetDrekthar_Cinematic_Q_Func009001() ) then
        return
    else
        call DoNothing(  )
    endif
    call ConditionalTriggerExecute( gg_trg_MeetDrekthar_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_DrekThar01, udg_AP1_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_DrekThar02, udg_AP1_Player, 15.00 )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_MeetDrekthar_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call SetUnitLookAt( udg_Drekthar, "bone_head", udg_Rexxar, 0, 0, 90 )
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_Drekthar, "TRIGSTR_899", gg_snd_D06ADrekThar03, "TRIGSTR_900", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_D06ADrekThar03, 0 )
    if ( Trig_MeetDrekthar_Cinematic_Q_Func024001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_Drek_Thar03, udg_AP1_Player, 0 )
    call TransmissionFromUnitWithNameBJ( udg_APG1_Player, udg_Drekthar, "TRIGSTR_901", gg_snd_D06ADrekThar03A, "TRIGSTR_902", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_D06ADrekThar03A, 0 )
    if ( Trig_MeetDrekthar_Cinematic_Q_Func028001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_MeetDrekthar_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_MeetDrekthar_Cleanup )
    call CinematicModeBJ( false, GetPlayersAll() )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call SetUnitInvulnerable( udg_Drekthar, false )
    call RescueUnitBJ( udg_Drekthar, udg_AP1_Player, true )
    call ConditionalTriggerExecute( gg_trg_Quest_ThunderLizards_Discovered )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    call ConditionalTriggerExecute( gg_trg_Quest_LizardEggs_Discovered )
    call TriggerSleepAction( bj_QUEUE_DELAY_QUEST )
    set udg_InCinematic = false
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
    call QueuedTriggerAddBJ( gg_trg_Drekthar_Acquired_Q, true )
endfunction

