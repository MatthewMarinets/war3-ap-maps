function Trig_MeetDrekthar_Setup_Actions takes nothing returns nothing
    // Save Camera Position
    set udg_CameraReturnPoint = GetCameraTargetPositionLoc()
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\LordaeronSummerSky\\LordaeronSummerSky.mdl" )
    // Save Selection
    set udg_SelectedUnits = GetUnitsSelectedAll(udg_AP1_Player)
    call ClearSelection(  )
    // Revive Hero
    set udg_RevivalHero = udg_Rexxar
    set udg_RevivalSpot = GetRectCenter(gg_rct_MeetDrekThar_Rexxar_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_RexxarReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingTimed( udg_Rexxar, 5.00, 0 )
    // Revive Hero
    set udg_RevivalHero = udg_Rokhan
    set udg_RevivalSpot = GetRectCenter(gg_rct_MeetDrekThar_Rokhan_Start)
    call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
    set udg_RokhanReturnSpot = udg_RevivalReturnSpot
    call SetUnitFacingTimed( udg_Rokhan, 15.00, 0 )
    // Revive Hero
    if ( Trig_MeetDrekthar_Setup_Func021C() ) then
        set udg_RevivalHero = udg_Chen
        set udg_RevivalSpot = GetRectCenter(gg_rct_MeetDrekThar_Chen_Start)
        call ConditionalTriggerExecute( gg_trg_Cinematic_Revival )
        set udg_ChenReturnSpot = udg_RevivalReturnSpot
        call SetUnitFacingTimed( udg_Chen, 345.00, 0 )
        call SuspendHeroXPBJ( false, udg_Chen )
    else
    endif
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Rexxar )
    call SuspendHeroXPBJ( false, udg_Rokhan )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Pause Units
    call PauseAllUnitsBJ( true )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_AP1_Player, Condition(function Trig_MeetDrekthar_Setup_Func030002002))
    call GroupAddGroup( GetUnitsOfPlayerMatching(Player(PLAYER_NEUTRAL_AGGRESSIVE), Condition(function Trig_MeetDrekthar_Setup_Func031001002)), udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Rexxar, udg_HiddenUnits )
    call GroupRemoveUnitSimple( udg_Rokhan, udg_HiddenUnits )
    if ( Trig_MeetDrekthar_Setup_Func034C() ) then
        call GroupRemoveUnitSimple( udg_Chen, udg_HiddenUnits )
    else
    endif
    call ForGroupBJ( udg_HiddenUnits, function Trig_MeetDrekthar_Setup_Func035002 )
    // Hide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_MeetDrekthar_Setup_Func037002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP1_Player), function Trig_MeetDrekthar_Setup_Func039002 )
    // Remove Drekthar Talkie
    call DestroyEffectBJ( udg_DrektharTalkie )
    // Disable Abilities
    call ConditionalTriggerExecute( gg_trg_Cinematics_Disable_Abilities )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\OrcTheme.mp3" )
endfunction

