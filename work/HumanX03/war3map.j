function Trig_Intro_Cleanup_Actions takes nothing returns nothing
    // Disable Auras
    call SetPlayerAbilityAvailableBJ( true, 'AIba', udg_AP2_Player )
    // Move Units
    call SetUnitPositionLocFacingBJ( udg_Kael, GetRectCenter(gg_rct_StartMBloodmage), 255.00 )
    call SetUnitPositionLocFacingBJ( udg_Vashj, GetRectCenter(gg_rct_StartMNagaSorc), 285.00 )
