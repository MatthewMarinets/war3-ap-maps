//===========================================================================
function Trig_Circle01_Actions takes nothing returns nothing
    // OuterCircle
    set udg_PointRingAroundWithOffset = PolarProjectionBJ(udg_PointRingAroundTheRegion, udg_CircleOutRadius, ( udg_CircleGuysAngle1 + 0.00 ))
    call IssuePointOrderLocBJ( udg_SecretGuy1, "move", udg_PointRingAroundWithOffset )
    call RemoveLocation( udg_PointRingAroundWithOffset )
    set udg_PointRingAroundWithOffset = PolarProjectionBJ(udg_PointRingAroundTheRegion, udg_CircleOutRadius, ( udg_CircleGuysAngle1 + 72.00 ))
    call IssuePointOrderLocBJ( udg_SecretGuy2, "move", udg_PointRingAroundWithOffset )
    call RemoveLocation( udg_PointRingAroundWithOffset )
    set udg_PointRingAroundWithOffset = PolarProjectionBJ(udg_PointRingAroundTheRegion, udg_CircleOutRadius, ( udg_CircleGuysAngle1 + 144.00 ))
    call IssuePointOrderLocBJ( udg_SecretGuy3, "move", udg_PointRingAroundWithOffset )
    call RemoveLocation( udg_PointRingAroundWithOffset )
    set udg_PointRingAroundWithOffset = PolarProjectionBJ(udg_PointRingAroundTheRegion, udg_CircleOutRadius, ( udg_CircleGuysAngle1 + 216.00 ))
    call IssuePointOrderLocBJ( udg_SecretGuy4, "move", udg_PointRingAroundWithOffset )
    call RemoveLocation( udg_PointRingAroundWithOffset )
    set udg_PointRingAroundWithOffset = PolarProjectionBJ(udg_PointRingAroundTheRegion, udg_CircleOutRadius, ( udg_CircleGuysAngle1 + 288.00 ))
    call IssuePointOrderLocBJ( udg_SecretGuy5, "move", udg_PointRingAroundWithOffset )
    call RemoveLocation( udg_PointRingAroundWithOffset )
    // InnerCircle
    set udg_CircleGuysAngle1 = ( udg_CircleGuysAngle1 + 20.00 )
endfunction

