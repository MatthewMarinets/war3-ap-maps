    call SetStackedSoundBJ( false, gg_snd_EnchantedCellLoop, gg_rct_PrisonBlock00_KaelsDoor )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Intro_Cleanup_Func017002 )
    // Unhide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Intro_Cleanup_Func019A )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    call ConditionalTriggerExecute( gg_trg_PausedUnitsForLaterUse )
    // Pause Time Of Day
    call UseTimeOfDayBJ( false )
    // Re-enable XP Gain
    if ( Trig_Intro_Cleanup_Func026001() ) then
        call SuspendHeroXPBJ( true, udg_Kael )
    else
        call DoNothing(  )
    endif
    if ( Trig_Intro_Cleanup_Func027001() ) then
        call SuspendHeroXPBJ( true, udg_Vashj )
    else
        call DoNothing(  )
    endif
    // Restore Selection
    call SelectUnitSingle( udg_Kael )
    // Reset Vashj Mana/Cooldown
    call SetUnitManaPercentBJ( udg_Vashj, 100 )
    call UnitResetCooldown( udg_Vashj )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Fog and Weather and TOD
    call SetTimeOfDay( 24.00 )
    call EnableWeatherEffect( udg_LightFog, true )
    call SetTerrainFogExBJ( 0, 800.00, 7000.00, 0, 36.00, 79.00, 35.00 )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP2_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP2_Player, GetRectCenter(gg_rct_StartMBloodmage), 0 )
    call SetCameraBoundsToRectForPlayerBJ( udg_AP2_Player, GetCameraBoundsMapRect() )
    // Wipe Visibility
    call CreateFogModifierRectBJ( true, udg_AP2_Player, FOG_OF_WAR_MASKED, GetPlayableMapRect() )
    call DestroyFogModifier( GetLastCreatedFogModifier() )
endfunction

