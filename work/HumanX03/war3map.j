    // Kill Units
    if ( Trig_Intro_Cleanup_Func008001() ) then
        call RemoveUnit( gg_unit_hfoo_0038 )
    else
        call DoNothing(  )
    endif
    if ( Trig_Intro_Cleanup_Func009001() ) then
        call RemoveUnit( gg_unit_hfoo_0039 )
    else
        call DoNothing(  )
    endif
    // Remove Units
    call RemoveUnit( GroupPickRandomUnit(GetUnitsInRectAll(gg_rct_PortalMove)) )
    // Open Doors
    call EnumDestructablesInRectAll( gg_rct_PrisonBlock00_KaelsDoor, function Trig_Intro_Cleanup_Func013002 )
