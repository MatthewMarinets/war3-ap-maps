        call ConditionalTriggerExecute( gg_trg_Furbolg_Attacked_After_Quest_Complete )
    else
        call DoNothing(  )
    endif
    if ( Trig_Furbolg_Attacked_by_the_Player_Func045001() ) then
        call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
    else
        call DoNothing(  )
    endif
    if ( Trig_Furbolg_Attacked_by_the_Player_Func046001() ) then
        return
    else
        call DoNothing(  )
    endif
    call QuestSetDiscoveredBJ( udg_Furbolg, true )
