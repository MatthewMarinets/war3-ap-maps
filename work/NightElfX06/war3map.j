    call DisableTrigger( GetTriggeringTrigger() )
    call SetUnitInvulnerable( udg_Kael, true )
    call PolledWait( 1.00 )
    call SetUnitInvulnerable( udg_Kael, false )
    call PolledWait( 29.00 )
    call EnableTrigger( GetTriggeringTrigger() )
endfunction

