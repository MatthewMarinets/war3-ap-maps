function Trig_Victory_Setup_Actions takes nothing returns nothing
    // Set Sky
    call SetSkyModel( "Environment\\Sky\\FelwoodSky\\FelwoodSky.mdl" )
    call SetTimeOfDay( 7.00 )
    // Disable day/night cycle
    call UseTimeOfDayBJ( false )
    // Clear Selection
    call ClearSelection(  )
    // Disable XP Gain
    call SuspendHeroXPBJ( false, udg_Maiev )
    call SuspendHeroXPBJ( false, udg_Furion )
    // Remove Random Spawns
    call ForGroupBJ( udg_RandomSpawnGroup, function Trig_Victory_Setup_Func012002 )
    // Pause Units
    call PauseAllUnitsBJ( true )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P1_BloodElves), function Trig_Victory_Setup_Func015002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP2_Player), function Trig_Victory_Setup_Func016002 )
    // Reset Tyrande for Starfall
    call UnitResetCooldown( udg_Tyrande )
    call SetUnitManaPercentBJ( udg_Tyrande, 100 )
    // Hide Units
    set udg_HiddenUnits = GetUnitsOfPlayerMatching(udg_P11_Undead, Condition(function Trig_Victory_Setup_Func021002002))
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P7_Undead, Condition(function Trig_Victory_Setup_Func022001002)), function Trig_Victory_Setup_Func022002 )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_P4_Undead, Condition(function Trig_Victory_Setup_Func023001002)), function Trig_Victory_Setup_Func023002 )
    call ForGroupBJ( udg_HiddenUnits, function Trig_Victory_Setup_Func024002 )
    // Remove Buffs
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_AP2_Player), function Trig_Victory_Setup_Func026002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P1_BloodElves), function Trig_Victory_Setup_Func027002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P7_Undead), function Trig_Victory_Setup_Func028002 )
    call ForGroupBJ( GetUnitsOfPlayerAll(udg_P4_Undead), function Trig_Victory_Setup_Func029002 )
    // Reset Caravan Guard Speeds
    set bj_forLoopAIndex = udg_CaravanGuardFirst
    set bj_forLoopAIndexEnd = udg_CaravanGuardLast
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call SetUnitMoveSpeed( udg_CaravanUnitArray[GetForLoopIndexA()], GetUnitDefaultMoveSpeed(udg_CaravanUnitArray[GetForLoopIndexA()]) )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    call ForGroupBJ( udg_CaravanGuardsGroup, function Trig_Victory_Setup_Func032002 )
    // Disable Shadowmeld
    call SetPlayerAbilityAvailableBJ( false, 'Ashm', udg_AP2_Player )
    call SetPlayerAbilityAvailableBJ( false, 'AEar', udg_AP2_Player )
    call SetPlayerAbilityAvailableBJ( false, 'ACac', udg_AP2_Player )
    // Halt Caravan Behaviour
    call ConditionalTriggerExecute( gg_trg_Caravan_Orders_Pause )
    call DisableTrigger( gg_trg_Guard_Attacked )
    // Turn off Priest Heal
    call SetPlayerAbilityAvailableBJ( false, 'Ahea', udg_P1_BloodElves )
    call SetPlayerAbilityAvailableBJ( false, 'Anhe', udg_P1_BloodElves )
    // Move Units
    set bj_forLoopAIndex = udg_CaravanGuardFirst
    set bj_forLoopAIndexEnd = udg_CaravanGuardLast
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call SetUnitPositionLocFacingBJ( udg_CaravanUnitArray[GetForLoopIndexA()], GetRectCenter(gg_rct_ExitGuardArea), 315.00 )
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
    call ForGroupBJ( udg_FinalGuardGroup, function Trig_Victory_Setup_Func045002 )
    call ForGroupBJ( GetUnitsOfPlayerMatching(udg_AP2_Player, Condition(function Trig_Victory_Setup_Func047001002)), function Trig_Victory_Setup_Func047A )
    if ( Trig_Victory_Setup_Func048C() ) then
        call SetUnitPositionLocFacingBJ( udg_CaravanWagon01, GetRectCenter(gg_rct_ExitWagonA01), 60.00 )
    else
    endif
    if ( Trig_Victory_Setup_Func049C() ) then
        call SetUnitPositionLocFacingBJ( udg_CaravanWagon02, GetRectCenter(gg_rct_ExitWagonA02), 45.00 )
    else
    endif
    if ( Trig_Victory_Setup_Func050C() ) then
        call SetUnitPositionLocFacingBJ( udg_CaravanWagon03, GetRectCenter(gg_rct_ExitWagonA03), 25.00 )
    else
    endif
    if ( Trig_Victory_Setup_Func051C() ) then
        call SetUnitPositionLocFacingBJ( udg_CaravanWagon04, GetRectCenter(gg_rct_ExitWagonA04), 10.00 )
    else
    endif
    call SetUnitPositionLocFacingBJ( udg_Kael, GetRectCenter(gg_rct_ExitKael01), 190.00 )
    call SetUnitPositionLocFacingBJ( udg_Maiev, GetRectCenter(gg_rct_ExitMaiev01), 5.00 )
    call SetUnitPositionLocFacingBJ( udg_Tyrande, GetRectCenter(gg_rct_ExitTyrande01), 200.00 )
    // Music Theme
    call PlayThematicMusicBJ( "Sound\\Music\\mp3Music\\HeroicVictory.mp3" )
endfunction

