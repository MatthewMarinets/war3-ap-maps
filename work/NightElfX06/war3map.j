    endif
    return true
endfunction

function Trig_Guard_Attacked_Single_Actions takes nothing returns nothing
    set udg_TempPointB = GetUnitLoc(udg_CaravanUnitArray[GetForLoopIndexA()])
    set udg_TempPointC = GetUnitLoc(GetAttackedUnitBJ())
    if ( Trig_Guard_Attacked_Single_Func003C() ) then
        call SetUnitMoveSpeed( udg_CaravanUnitArray[GetForLoopIndexA()], GetUnitDefaultMoveSpeed(udg_CaravanUnitArray[GetForLoopIndexA()]) )
        // Order all guards to attack-move to the general area of the attacked unit
        set udg_TempPointA = GetUnitLoc(GetAttacker())
        call MoveRectToLoc( gg_rct_GuardCallForHelpArea, udg_TempPointA )
        call RemoveLocation( udg_TempPointA )
        if ( Trig_Guard_Attacked_Single_Func003Func007C() ) then
            set udg_TempPointA = GetRandomLocInRect(gg_rct_GuardCallForHelpArea)
            call IssuePointOrderLocBJ( udg_CaravanUnitArray[GetForLoopIndexA()], "attack", udg_TempPointA )
            call RemoveLocation( udg_TempPointA )
        else
            if ( Trig_Guard_Attacked_Single_Func003Func007Func002C() ) then
                set udg_TempPointA = GetRandomLocInRect(gg_rct_GuardCallForHelpArea)
                call IssuePointOrderLocBJ( udg_CaravanUnitArray[GetForLoopIndexA()], "attack", udg_TempPointA )
                call RemoveLocation( udg_TempPointA )
            else
            endif
        endif
    else
    endif
    call RemoveLocation( udg_TempPointB )
    call RemoveLocation( udg_TempPointC )
endfunction

