//===========================================================================
function Trig_Kael_Attacked_Conditions takes nothing returns boolean
    if ( not ( udg_GameOver == false ) ) then
        return false
    endif
    if ( not ( IsTriggerEnabled(GetTriggeringTrigger()) == true ) ) then
        return false
    endif
    if ( not ( GetUnitStateSwap(UNIT_STATE_LIFE, udg_Kael) < 100.00 ) ) then
        return false
    endif
    return true
endfunction

