function Trig_Mid2_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG2_Player )
    call TriggerSleepAction( 0.25 )
    call SetUnitMoveSpeed( udg_Maiev, GetUnitDefaultMoveSpeed(udg_Maiev) )
    call SetUnitMoveSpeed( udg_Tyrande, GetUnitDefaultMoveSpeed(udg_Tyrande) )
    if ( Trig_Mid2_Cinematic_Q_Func011C() ) then
        return
    else
    endif
    call ConditionalTriggerExecute( gg_trg_Mid2_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_Mid2_01, udg_AP2_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_Mid2_02, udg_AP2_Player, ( ( GetSoundDurationBJ(gg_snd_S06Kael43) + GetSoundDurationBJ(gg_snd_S06Maiev44) ) + ( GetSoundDurationBJ(gg_snd_S06Tyrande45) + 2.00 ) ) )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Mid2_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Kael, "TRIGSTR_257", gg_snd_S06Kael43, "TRIGSTR_258", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_S06Kael43, 0 )
    if ( Trig_Mid2_Cinematic_Q_Func025001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Maiev, "TRIGSTR_259", gg_snd_S06Maiev44, "TRIGSTR_260", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_S06Maiev44, 0 )
    if ( Trig_Mid2_Cinematic_Q_Func028001() ) then
        return
    else
        call DoNothing(  )
    endif
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Tyrande, "TRIGSTR_261", gg_snd_S06Tyrande45, "TRIGSTR_262", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_S06Tyrande45, 0 )
    if ( Trig_Mid2_Cinematic_Q_Func031001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Mid2_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 0.50 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Mid2_Cleanup )
    call SetUnitMoveSpeed( udg_Maiev, GetUnitDefaultMoveSpeed(udg_Maiev) )
    call SetUnitMoveSpeed( udg_Tyrande, GetUnitDefaultMoveSpeed(udg_Tyrande) )
    call CinematicModeBJ( false, udg_APG2_Player )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 0.50 )
    call QueuedTriggerRemoveBJ( GetTriggeringTrigger() )
    set udg_InCinematic = false
endfunction

