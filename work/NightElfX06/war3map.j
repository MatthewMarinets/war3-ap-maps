function Trig_Mid_Cinematic_Q_Actions takes nothing returns nothing
    set udg_InCinematic = true
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Set Up Scene
    call CinematicModeBJ( true, udg_APG2_Player )
    call TriggerSleepAction( 0.25 )
    call SetUnitMoveSpeed( udg_Maiev, GetUnitDefaultMoveSpeed(udg_Maiev) )
    call SetUnitMoveSpeed( udg_Tyrande, GetUnitDefaultMoveSpeed(udg_Tyrande) )
    if ( Trig_Mid_Cinematic_Q_Func011C() ) then
        return
    else
    endif
    call ConditionalTriggerExecute( gg_trg_Mid_Setup )
    call TriggerSleepAction( 0.25 )
    call CameraSetupApplyForPlayer( true, gg_cam_ForkCenter_01, udg_AP2_Player, 0 )
    call CameraSetupApplyForPlayer( true, gg_cam_ForkCenter_02, udg_AP2_Player, ( GetSoundDurationBJ(gg_snd_S06Kael39) + 4.00 ) )
    // Fade In
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    call EnableTrigger( gg_trg_Mid_Skipped )
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Kael, "TRIGSTR_094", gg_snd_S06Kael39, "TRIGSTR_166", bj_TIMETYPE_ADD, 0.00, true )
    if ( Trig_Mid_Cinematic_Q_Func024001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_ForkRight, udg_AP2_Player, ( GetSoundDurationBJ(gg_snd_S06Kael39A) + 2.00 ) )
    call TriggerSleepAction( 1.00 )
    if ( Trig_Mid_Cinematic_Q_Func027001() ) then
        return
    else
        call DoNothing(  )
    endif
    call SetUnitFacingToFaceLocTimed( udg_Kael, GetRectCenter(gg_rct_Breadcrumb_068B), 0.50 )
    call SetUnitFacingToFaceLocTimed( udg_Maiev, GetRectCenter(gg_rct_Breadcrumb_068B), 0.50 )
    call SetUnitFacingToFaceLocTimed( udg_Tyrande, GetRectCenter(gg_rct_Breadcrumb_068B), 0.50 )
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Kael, "TRIGSTR_241", gg_snd_S06Kael39A, "TRIGSTR_242", bj_TIMETYPE_ADD, 0, false )
    if ( Trig_Mid_Cinematic_Q_Func032C() ) then
        call TriggerSleepAction( 2.00 )
        if ( Trig_Mid_Cinematic_Q_Func032Func002001() ) then
            return
        else
            call DoNothing(  )
        endif
        call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
        call TriggerSleepAction( 0.50 )
        if ( Trig_Mid_Cinematic_Q_Func032Func007001() ) then
            return
        else
            call DoNothing(  )
        endif
        call CameraSetupApplyForPlayer( true, gg_cam_ForkBastion_01, udg_AP2_Player, 0 )
        call CameraSetupApplyForPlayer( true, gg_cam_ForkBastion_02, udg_AP2_Player, 5.00 )
        call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
        call TriggerSleepAction( 0.50 )
        if ( Trig_Mid_Cinematic_Q_Func032Func013001() ) then
            return
        else
            call DoNothing(  )
        endif
        call TriggerSleepAction( 3.50 )
        if ( Trig_Mid_Cinematic_Q_Func032Func015001() ) then
            return
        else
            call DoNothing(  )
        endif
        call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
        call TriggerSleepAction( 0.50 )
        if ( Trig_Mid_Cinematic_Q_Func032Func018001() ) then
            return
        else
            call DoNothing(  )
        endif
    else
        call TriggerSleepAction( ( GetSoundDurationBJ(gg_snd_S06Kael39A) + 1.00 ) )
        if ( Trig_Mid_Cinematic_Q_Func032Func004001() ) then
            return
        else
            call DoNothing(  )
        endif
    endif
    if ( Trig_Mid_Cinematic_Q_Func033C() ) then
        call CameraSetupApplyForPlayer( true, gg_cam_ForkCenter_02, udg_AP2_Player, 0 )
        call CinematicFadeBJ( bj_CINEFADETYPE_FADEIN, 0.50, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
        call TriggerSleepAction( 0.50 )
        if ( Trig_Mid_Cinematic_Q_Func033Func004001() ) then
            return
        else
            call DoNothing(  )
        endif
    else
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_ForkLeft, udg_AP2_Player, ( GetSoundDurationBJ(gg_snd_S06Kael39B) + 1 ) )
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Kael, "TRIGSTR_243", gg_snd_S06Kael39B, "TRIGSTR_244", bj_TIMETYPE_ADD, 0, false )
    call SetUnitFacingToFaceLocTimed( udg_Kael, GetRectCenter(gg_rct_Breadcrumb_068A), 0.50 )
    call SetUnitFacingToFaceLocTimed( udg_Maiev, GetRectCenter(gg_rct_Breadcrumb_068A), 0.50 )
    call SetUnitFacingToFaceLocTimed( udg_Tyrande, GetRectCenter(gg_rct_Breadcrumb_068A), 0.50 )
    if ( Trig_Mid_Cinematic_Q_Func039001() ) then
        return
    else
        call DoNothing(  )
    endif
    call WaitForSoundBJ( gg_snd_S06Kael39B, 0 )
    if ( Trig_Mid_Cinematic_Q_Func041001() ) then
        return
    else
        call DoNothing(  )
    endif
    call CameraSetupApplyForPlayer( true, gg_cam_ForkDecision_01, udg_AP2_Player, 0.00 )
    call CameraSetupApplyForPlayer( true, gg_cam_ForkDecision_02, udg_AP2_Player, ( GetSoundDurationBJ(gg_snd_S06Maiev40) + 2.00 ) )
    call TransmissionFromUnitWithNameBJ( GetPlayersAll(), udg_Maiev, "TRIGSTR_029", gg_snd_S06Maiev40, "TRIGSTR_168", bj_TIMETYPE_ADD, 0, false )
    call WaitForSoundBJ( gg_snd_S06Maiev40, 0 )
    if ( Trig_Mid_Cinematic_Q_Func046001() ) then
        return
    else
        call DoNothing(  )
    endif
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    call DisableTrigger( gg_trg_Mid_Skipped )
    // Fade Out
    call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.tga", 0, 0, 0, 0 )
    call TriggerSleepAction( 1.00 )
    // Cleanup
    call ConditionalTriggerExecute( gg_trg_Mid_Cleanup )
    call SetUnitMoveSpeed( udg_Maiev, GetUnitDefaultMoveSpeed(udg_Maiev) )
    call SetUnitMoveSpeed( udg_Tyrande, GetUnitDefaultMoveSpeed(udg_Tyrande) )
