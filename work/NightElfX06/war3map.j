function Trig_Mid_Cleanup_Actions takes nothing returns nothing
    // Restore Abilities
    call SetPlayerAbilityAvailableBJ( true, 'Ashm', udg_AP2_Player )
    call SetPlayerAbilityAvailableBJ( true, 'AEar', udg_AP2_Player )
    call SetPlayerAbilityAvailableBJ( true, 'ACac', udg_AP2_Player )
    // Unhide Items
    call EnumItemsInRectBJ( GetPlayableMapRect(), function Trig_Mid_Cleanup_Func006002 )
    // Unhide Units
    call ForGroupBJ( udg_HiddenUnits, function Trig_Mid_Cleanup_Func008002 )
    // Unpause Units
    call PauseAllUnitsBJ( false )
    call ConditionalTriggerExecute( gg_trg_Caravan_Orders_Resume )
    // Pause Time Of Day
    call UseTimeOfDayBJ( true )
    // Re-enable XP Gain
    if ( Trig_Mid_Cleanup_Func015001() ) then
        call SuspendHeroXPBJ( true, udg_Maiev )
    else
        call DoNothing(  )
    endif
    if ( Trig_Mid_Cleanup_Func016001() ) then
        call SuspendHeroXPBJ( true, udg_Tyrande )
    else
        call DoNothing(  )
    endif
    // Return Heroes
    call SetUnitPositionLocFacingBJ( udg_Kael, udg_MidKaelPoint, udg_MidKaelOrientation )
    call SetUnitPositionLocFacingBJ( udg_Maiev, udg_MidMaievPoint, udg_MidMaievOrientation )
    call SetUnitPositionLocFacingBJ( udg_Tyrande, udg_MidTyrandePoint, udg_MidTyrandeOrientation )
    // Reset Body Facings
    call ResetUnitLookAt( udg_Kael )
    call ResetUnitLookAt( udg_Tyrande )
    call ResetUnitLookAt( udg_Maiev )
    // Restore Selection
    call SelectGroupBJ( udg_SelectedUnits )
    // Reset Sky
    call SetSkyModel( null )
    // Reset Camera
    call CameraResetSmoothingFactorBJ(  )
    call ResetToGameCameraForPlayer( udg_AP2_Player, 0 )
    call PanCameraToTimedLocForPlayer( udg_AP2_Player, udg_CameraReturnPoint, 0 )
endfunction

